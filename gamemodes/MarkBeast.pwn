public OnPlayerDeath(playerid, killerid, reason)
{
	IsPlayerDeath[playerid] = false;

	if(IsPlayerLogged[playerid])
	{

        DontGiveSuspect[playerid] = 0;
		GivePlayerMoney(playerid, 100);
		
		gmd[playerid] = 0;

		if(PlayerInfo[playerid][pView] < 1 && IsPlayerInBJ[playerid] != 1 && IsPlayerInHell[playerid] != 1)
		{
			foreach(i)
			{
				if(PlayerInfo[i][pAdmin] > 0){SendDeathMessageToPlayer(i, killerid, playerid, reason);}
			}
		}
		if(HavingBall[playerid]) Baller = INVALID_PLAYER_ID;
		HavingBall[playerid] = 0;
		if(SomeOneWhoZombieCanKill[playerid] > 0)
		{
			SomeOneWhoZombieCanKill[playerid] = 0;

			SendClientMessage(playerid, COLOR_LIGHTRED,"Адская тварь утащила вас в преисподнюю.");


			SetSpawnInfo(playerid, 255, 8, -1685.1571,1097.7434,1501.0859,353.5934, 0, 0, 0, 0, 0, 0);

			PlayerPlaySound(playerid, 1058, 0.0, 0.0, 0.0);
			_ResetGun(playerid);
			PlayerInfo[playerid][pView] = 1*60;
			PlayerInfo[playerid][pWanted] = 0;
			pTemp[playerid][Anti_Pvar][0] = 1;

		}
		if(!IsPlayerSpawned[playerid]) return false;
		IsPlayerSpawned[playerid] = false;

		if(PlayerInfo[playerid][pSex] == 2){SendPlayerMe(playerid,"умерла");}
		else{SendPlayerMe(playerid,"умер");}
		if(VehicleToBuy[playerid] != INVALID_VEHICLE_ID)
		{
			if(IsValidVehicle(VehicleToBuy[playerid])){DestroyVehicle(VehicleToBuy[playerid]);}
			VehicleToBuy[playerid] = INVALID_VEHICLE_ID;
			OlVirtualWorld[playerid] = -1;
		}
		BuyCar[playerid] = -1;
		if(IsPlayerInBJ[playerid] != 1)
		{
			new weapon = GetPlayerWeapon(playerid);
			new ammo = GetPlayerAmmo(playerid);
			if(ammo > 0 || weapon > 0)
			{
			    new slot = GetWeaponSlot(weapon);
				if(PlayerInfo[playerid][pGuns][GetWeaponSlot(weapon)] == weapon)
				{
					if(PlayerInfo[playerid][pAmmos][slot] >= ammo)
					{
						if(!IsPlayerInAnyVehicle(playerid))
						{//нычка
							new Float:x, Float:y, Float:z,Float:eagle;
							GetPlayerPos(playerid, x, y, z);
							GetPlayerFAngle(playerid,eagle);
							if(!NotGoodGun(weapon))
							{
								new idgun = GunObjects[weapon];
								new idobject=-1;
								for(new i;i<MAX_DGUNS;i++)
								{
									if(dguns[i]==-1)
									{
										idobject=i;
										break;
									}
								}
								if(idobject==-1)
								{
									new min1=CurrentTimer;
									for(new i;i<MAX_DGUNS;i++)
									{
										if(dguntime[i]<min1)
										{
											min1=dguntime[i];
											idobject=i;
										}
									}
									DestroyDynamicObject(dguns[idobject]);
									DestroyDynamic3DTextLabel(DgunText[idobject]);
								}
								dguntime[idobject]=CurrentTimer;
								dpatrons[idobject]=ammo;
								dgunn[idobject]=weapon;
								dguns[idobject] = CreateDynamicObject(idgun, x, y, z-0.97, 96, 110, eagle, GetPlayerVirtualWorld(playerid));

								new buffer[100];
								format(buffer, sizeof(buffer), "Чтобы подобрать %s введите "COLOR_PROJECT_EX"/pgun{FFFFFF} либо нажмите "COLOR_PROJECT_EX"ALT", GunNames[weapon]);
								DgunText[idobject] =CreateDynamic3DTextLabel(buffer, -1, x, y, z-0.971,15.0);
							}
						}
					}
				}
			}
		}
		_ResetGun(playerid); // тута 2

		AntiCheatsOFF(playerid,5);
		StopAudioStreamForPlayer(playerid);
		Smoke[playerid] = 0;
		pSportZal[playerid] = 0;
		SellGzone[playerid] = 0;
		SellPGzone[playerid] = 0;
		SetFullness(playerid, 40);// тута 2

		new string[450];
		new job = PlayerInfo[playerid][pJob]; // тута
		new injob = InJob[playerid];
		if((job== 17 || job== 18 || job== 19 || job== 20) && (injob==1 || injob==3))
		{
			SendClientMessage(playerid,COLOR_GREY, "Вы забыли забрать свою зарплату, не забудьте вернутся что бы забрать ее!");
		}
		////
		InJob[playerid]=0;

		/////
		Vnedren[playerid]=0;
		//	J_SetPlayerHealth(playerid,15);// не забудь убрать
		if(GotoInfo[playerid][gtGoID]!=INVALID_PLAYER_ID)CheckPlayerGoCuff(GotoInfo[playerid][gtGoID]);
		if(GotoInfo[playerid][gtID]!=INVALID_PLAYER_ID)//жіза
		{
			Cuffed[GotoInfo[playerid][gtID]] = false;
			CuffedTime[GotoInfo[playerid][gtID]] = 0;
			TogglePlayerControllableEx(GotoInfo[playerid][gtID],true);
			SetPlayerSpecialAction(GotoInfo[playerid][gtID], 0);
			ClearAnims(GotoInfo[playerid][gtID]);
			SendClientMessage(GotoInfo[playerid][gtID],COLOR_GREY, "вы были выпущены с конвоя.");
			CheckPlayerGoCuff(GotoInfo[playerid][gtID]);
			CheckPlayerGoCuff(playerid);
		}
		gunmute[playerid] = CurrentTimer+5;

		for(new i;i<sizeof(Skinfon);i++)
		{
			TextDrawHideForPlayer(playerid,Skinfon[i]);
		}

		TextDrawHideForPlayer(playerid,ButtonLeft);
		TextDrawHideForPlayer(playerid,ButtonRight);
		TextDrawHideForPlayer(playerid,ButtonCancel);
		TextDrawHideForPlayer(playerid,SelectSkin);

		PlayerTextDrawHide(playerid,TaxiDraw[playerid]);
		TextDrawHideForPlayer(playerid, BoxTaxi1);
		TextDrawHideForPlayer(playerid, BoxTaxi2);
		TextDrawHideForPlayer(playerid, Taximonitor);
		TextDrawHideForPlayer(playerid, TaxiServ);
		TextDrawHideForPlayer(playerid, TaxiDol);
		PlayerTextDrawHide(playerid,InfoText[playerid]);
		CheckBed(playerid);
		//тута
		if(pTemp[playerid][Anti_Pvar][42] < CurrentTimer)
		{
			if(!PlayerInfo[playerid][pJailTime] && !PlayerInfo[playerid][pView] && PlayerInfo[playerid][pLevel] > 1 && IsPlayerInBJ[playerid] != 1 && IsPlayerInHell[playerid] != 1)
			{
				if(IsPlayerToKvadrat(playerid,-891.3302,-2950.0190,2915.9917,560.3257)) PlayerInfo[playerid][pCheepses]=1;
				else if(IsPlayerToKvadrat(playerid,-2929.6589,-2889.1660,-1326.9077,1493.1770)) PlayerInfo[playerid][pCheepses]=3;
				else PlayerInfo[playerid][pCheepses]=2;

				PlayerInfo[playerid][pHeal] = 1;
				PlayerInfo[playerid][pHealTime] = 0;
				//		TogglePlayerControllableEx(playerid,true);
			}
		}
		if(Merrit[0]!=0 && (Merrit[1]==playerid || Merrit[2]==playerid || Merrit[3]==playerid))
		{
			Merrit[0] = 0;
			Merrit[1] = 0;
			Merrit[2] = 0;
			Merrit[3] = 0;
			format(string,sizeof(string),"Свадьба была сорвана!");
			ProxDetector(25,playerid,string,COLOR_ME);
		}
		if(killerid != INVALID_PLAYER_ID)
		{
			if(!PlayerInfo[killerid][pView])
			{
				if(!IsPlayerInAnyVehicle(killerid))
				{
					if(!IsPlayerInRangeOfPlayer(150.0, playerid, killerid) && GetPlayerWeapon(killerid) == 0)
					{
						ToCheat(1,playerid, 7);
						return true;
					}
				}

				if(IsPlayerInRangeOfPoint(playerid, 110.0, 2760.7742,-2447.1958,13.5360) && IsPlayerInRangeOfPoint(killerid, 110.0, 2760.7742,-2447.1958,13.5360) ||
						IsPlayerInRangeOfPoint(playerid, 190.0, 255.7347,1837.5056,17.6991) && IsPlayerInRangeOfPoint(killerid, 190.0, 255.7347,1837.5056,17.6991) ||
						IsPlayerInRangeOfPoint(playerid, 250.0, -1368.9514,499.1776,11.1953) && IsPlayerInRangeOfPoint(killerid, 250.0, -1368.9514,499.1776,11.1953) ) if(IsAtGang(playerid) && IsAArmy(killerid))
				{
					GiveMoney(killerid,GLS[18]);
					if(PlayerInfo[playerid][pMoney]>GLS[18])GiveMoney(playerid,-GLS[18]);
					//aml(50, playerid, PN(killerid), "Попался бандит", GLS[18]);
					SendClientMessage(killerid,-1,"Вы получили премию за пойманого бандита!");
				}
				if(IsACop(killerid) && PlayerInfo[playerid][pWanted]>0)
				{
					if(ProxDetectorS(30.0, playerid, killerid, 1))
					{
						PlayerPlaySound(playerid, 1058, 0.0, 0.0, 0.0);
						if(PlayerInfo[playerid][pMember]!=3) PlayerInfo[playerid][pJailTime] = PlayerInfo[playerid][pWanted] * 300;
						else PlayerInfo[playerid][pJailTime] = PlayerInfo[playerid][pWanted] * 800;
						PlayerInfo[playerid][pBailPrice] = PlayerInfo[playerid][pWanted] * 500;
						if(PlayerInfo[playerid][pDrugs] >0 || PlayerInfo[playerid][pMats] >0) SendClientMessage(playerid,-1,"У вас отобрали наркотики и патроны!");
						new skininjail = 42;
						if(PlayerInfo[playerid][pSex] == 2){skininjail = 69;}
						if(IsPlayerToKvadrat(playerid,-891.3302,-2950.0190,2915.9917,560.3257)) //ls
						{
							new jrand = random( 2 );
							jrand = jrand +4;
							SetSpawnInfoEx(playerid, skininjail, JailPos[jrand][0],JailPos[jrand][1],JailPos[jrand][2], 180.0);
							PlayerInfo[playerid][pSprunks]=1;//ls
							Fix_SetPlayerInterior(playerid,SpawnIntWorld[TEAM_LSPD][0]);
							Fix_SetPlayerVirtualWorld(playerid,SpawnIntWorld[TEAM_LSPD][1]);
							//	SetPlayerTime(playerid,12,0);

						}
						else if(!IsPlayerToKvadrat(playerid,-2929.6589,-2889.1660,-1326.9077,1493.1770))//sf
						{
							new jrand = random( 3 );
							SetSpawnInfoEx(playerid, skininjail, JailPos[jrand][0],JailPos[jrand][1],JailPos[jrand][2], 180.0);
							PlayerInfo[playerid][pSprunks]=3;//sf
							Fix_SetPlayerInterior(playerid,SpawnIntWorld[TEAM_SFPD][0]);
							Fix_SetPlayerVirtualWorld(playerid,SpawnIntWorld[TEAM_SFPD][1]);
							//	SetPlayerTime(playerid,12,0);
						}
						else
						{
							SetSpawnInfoEx(playerid, skininjail, JailPos[3][0],JailPos[3][1],JailPos[3][2], 180.0);//lv
							PlayerInfo[playerid][pSprunks]=2;
							Fix_SetPlayerInterior(playerid,SpawnIntWorld[TEAM_PDLV][0]);
							Fix_SetPlayerVirtualWorld(playerid,SpawnIntWorld[TEAM_PDLV][1]);
							//		SetPlayerTime(playerid,12,0);
						}

						_ResetGun(playerid);
						PlayerInfo[playerid][pWanted] = 0;
						SetPlayerWantedLevel(playerid,0);
						PlayerSpawn(playerid);
						PlayerInfo[playerid][pDrugs] = 0;
						PlayerInfo[playerid][pMats] = 0;
						format(string, sizeof(string), "Вас посадили в тюрьму на %d минут.", (PlayerInfo[playerid][pJailTime]/60));
						SendClientMessage(playerid, COLOR_ADMINRED, string);
						
						string = "";
						format(string, sizeof(string), "Вы посадили "COLOR_PROJECT_EX"%s{FFFFFF} в тюрьму", Name(playerid));
						SendClientMessage(killerid, -1, string);

						TogglePlayerControllableEx(playerid, true);
						SetPlayerSkinEx(playerid,skininjail);
					}
				}
				if((CaptureTime[0]>0 || CaptureTime[1]>0)&&((GetTeamID(playerid) == CaptureAtac && GetTeamID(killerid) == CaptureDef) || (GetTeamID(playerid) == CaptureDef && GetTeamID(killerid) == CaptureAtac)))
				{
					if(GetTeamID(playerid) == CaptureAtac && GetTeamID(killerid) == CaptureDef)CaptureDefKill++;
					if(GetTeamID(playerid) == CaptureDef && GetTeamID(killerid) == CaptureAtac) CaptureAtacKill++;
					foreach(i){if(GetTeamID(i)==CaptureDef || GetTeamID(i)==CaptureAtac)SendDeathMessageToPlayer(i, killerid, playerid, reason);}
				}
				if(BizWarState == 2&&((GetTeamID(playerid) == BizAtack && GetTeamID(killerid) == BizDeff) || (GetTeamID(playerid) == BizDeff && GetTeamID(killerid) == BizAtack)))
				{
					if(GetTeamID(playerid) == BizAtack && GetTeamID(killerid) == BizDeff) BizDeffkill++;
					if(GetTeamID(playerid) == BizDeff && GetTeamID(killerid) == BizAtack) BizAtakkill++;
					foreach(i){if(GetTeamID(i)==BizDeff || GetTeamID(i)==BizAtack)SendDeathMessageToPlayer(i, killerid, playerid, reason);}
				}
				if(!DontGiveSuspect[killerid])
				{
					if(!IsACop(killerid) && !IsAArmy(killerid))
					{
						if(!IsPlayerInRangeOfPoint(playerid,100,-518.5075,-92.3690,62.2983))
						{
							if(GetPlayerGangZone(killerid) <= 0)
							{
								if(PlayerInfo[killerid][pZKP]>1)PlayerInfo[killerid][pZKP] --;
								if(PlayerInfo[killerid][pWanted]<6) PlayerInfo[killerid][pWanted]+=1;

								SendClientMessage(killerid,-1,"Вы были объявлены в розыск! Причина: нападение/убийство");
								format(string,sizeof(string),"Ваш текущий уровень розыска: %d",PlayerInfo[killerid][pWanted]);
								SendClientMessage(killerid,COLOR_YELLOW,string);
								format(string,sizeof(string),"Внимание! %s[%d] был(а) объявлен(a) в розыск! Причина: убийство | Уровень розыска: %d",Name(killerid),killerid,PlayerInfo[killerid][pWanted]);
								SendCopsMessage(COLOR_LIGHTRED,string);

								if(pTemp[playerid][Anti_Pvar][3]>CurrentTimer)
								{
									ToCheat(1,playerid, 8);
									return true;
								}
								pTemp[playerid][Anti_Pvar][3] = CurrentTimer+5;
							}
						}
					}
				}
			}
		}

		SetNextSpawn(playerid);// важно не сбить
	}
	return true;
}
