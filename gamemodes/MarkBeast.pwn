void OHB_SpinRoll(banditid)
{
	if (banditid < MAX_BANDITS)
	{
		if (OHB_Info[banditid][bnCreated] == 1)
		{
			new cur_pos[3];
			for (new i = 0; i < 3; i++)
			{
				cur_pos[i] = OHB_Info[banditid][bnRollPos][i];
				cur_pos[i]++;
				if (cur_pos[i] >= 18) cur_pos[i] = 0;
			}
			OHB_Info[banditid][bnPreSpin]++;

			if (OHB_Info[banditid][bnPreSpin] < 10)
			{
				OHB_SetRollPos(banditid, cur_pos[0], cur_pos[1], cur_pos[2]);
				SetTimerEx("OHB_SpinRoll", 50, 0, "d", banditid);
			}
			else
			{
				OHB_Info[banditid][bnPreSpin] = 0;
				OHB_Info[banditid][bnPreFinish][0] = 0;
				OHB_Info[banditid][bnPreFinish][1] = 0;
				OHB_Info[banditid][bnPreFinish][2] = 0;
				OHB_FinishSpinRoll(banditid);
			}
		}
	}
}

void OHB_FinishSpinRoll(banditid)
{
	if (banditid < MAX_BANDITS)
	{
		if (OHB_Info[banditid][bnCreated] == 1)
		{
			new goon = 0;
			new cur_pos[3];
			for (new i = 0; i < 3; i++)
			{
				cur_pos[i] = OHB_Info[banditid][bnRollPos][i];
			}

			if (cur_pos[2] != OHB_Info[banditid][bnRollFinishPos][2] || OHB_Info[banditid][bnPreFinish][2] < 8)
			{
				cur_pos[0]++;
				cur_pos[1]++;
				cur_pos[2]++;

				for (new i = 0; i < 3; i++)
				{
					if (cur_pos[i] >= 18) cur_pos[i] = 0;
				}

				goon++;
				OHB_Info[banditid][bnPreFinish][2]++;

				if (OHB_Info[banditid][bnPreFinish][2] > 7 && cur_pos[2] == OHB_Info[banditid][bnRollFinishPos][2])
				{
					PlayerPlaySoundForAll(1139, OHB_Info[banditid][bnPos][0], OHB_Info[banditid][bnPos][1], OHB_Info[banditid][bnPos][2], 10.0);
				}
				OHB_SetRollPos(banditid, cur_pos[0], cur_pos[1], cur_pos[2]);
				SetTimerEx("OHB_FinishSpinRoll", 50, 0, "d", banditid);
			}
			else if (cur_pos[1] != OHB_Info[banditid][bnRollFinishPos][1] || OHB_Info[banditid][bnPreFinish][1] < 8)
			{
				cur_pos[0]++;
				cur_pos[1]++;

				for (new i = 0; i < 3; i++)
				{
					if (cur_pos[i] >= 18) cur_pos[i] = 0;
				}

				goon++;
				OHB_Info[banditid][bnPreFinish][1]++;

				if (OHB_Info[banditid][bnPreFinish][1] > 7 && cur_pos[1] == OHB_Info[banditid][bnRollFinishPos][1])
				{
					PlayerPlaySoundForAll(1139, OHB_Info[banditid][bnPos][0], OHB_Info[banditid][bnPos][1], OHB_Info[banditid][bnPos][2], 10.0);
				}
				OHB_SetRollPos(banditid, cur_pos[0], cur_pos[1], cur_pos[2]);
				SetTimerEx("OHB_FinishSpinRoll", 50, 0, "d", banditid);
			}
			else if (cur_pos[0] != OHB_Info[banditid][bnRollFinishPos][0] || OHB_Info[banditid][bnPreFinish][0] < 8)
			{
				cur_pos[0]++;

				for (new i = 0; i < 3; i++)
				{
					if (cur_pos[i] >= 18) cur_pos[i] = 0;
				}

				goon++;
				OHB_Info[banditid][bnPreFinish][0]++;
				OHB_SetRollPos(banditid, cur_pos[0], cur_pos[1], cur_pos[2]);

				if (OHB_Info[banditid][bnPreFinish][0] > 7 && cur_pos[0] == OHB_Info[banditid][bnRollFinishPos][0])
				{
					PlayerPlaySoundForAll(1139, OHB_Info[banditid][bnPos][0], OHB_Info[banditid][bnPos][1], OHB_Info[banditid][bnPos][2], 10.0);

					OHB_Info[banditid][bnBusy] = 0;
					new line[3][3];
					new slot[3][3];
					for (new i = 0; i < 3; i++)
					{
						line[0][i] = cur_pos[i] + 1;
						line[1][i] = cur_pos[i];
						line[2][i] = cur_pos[i] - 1;
						if (line[0][i] >= 18) line[0][i] = 0;
						if (line[2][i] <= -1) line[2][i] = 17;
					}
					for (new i = 0; i < 3; i++)
					{
						for (new j = 0; j < 3; j++)
						{
							slot[i][j] = BanditRoll[j][ line[i][j] ];
						}
					}

					// выигранные комбинация (для учета)
					new win_set[MAX_ROLL_VALUES];

					for (new i = 0; i < sizeof(win_set); i++)
					{
						win_set[i] = 0;
					}

					// Left Top - Right Down Diagonal
					if (slot[0][0] == slot[1][1] && slot[1][1]  == slot[2][2])
					{
						new value = slot[0][0];
						if (value < sizeof(win_set))
						{
							win_set[value]++;
						}
					}
					// Right Top - Left Down Diagonal
					if (slot[0][2] == slot[1][1] && slot[1][1] == slot[2][0])
					{
						new value = slot[0][2];
						if (value < sizeof(win_set))
						{
							win_set[value]++;
						}
					}
					// 1 Line
					if (slot[0][0] == slot[0][1] && slot[0][1] == slot[0][2])
					{
						new value = slot[0][0];
						if (value < sizeof(win_set))
						{
							win_set[value]++;
						}
					}
					// 2 Line
					if (slot[1][0] == slot[1][1] && slot[1][1] == slot[1][2])
					{
						new value = slot[1][0];
						if (value < sizeof(win_set))
						{
							win_set[value]++;
						}
					}
					// 3 Line
					if (slot[2][0] == slot[2][1] && slot[2][1] == slot[2][2])
					{
						new value = slot[2][0];
						if (value < sizeof(win_set))
						{
							win_set[value]++;
						}
					}

					new pid = OHB_Info[banditid][bnUserID];
					if (IsPlayerConnected(pid))
					{
						new won = 0;

						new reward[MAX_ROLL_VALUES];

						reward[ROLL_CHERRY] = 2;
						reward[ROLL_BELLS] = 2;
						reward[ROLL_WINE] = 2;
						reward[ROLL_69] = 4;
						reward[ROLL_1GOLD] = 15;
						reward[ROLL_2GOLD] = 80;

						for (new i = 0; i < MAX_ROLL_VALUES; i++)
						{
							if (win_set[i] > 0)
							{
								won += (reward[i] * win_set[i]);
							}
						}

						if (won > 0)
						{
							won = won * OHB_Info[banditid][bnRate];

							if(CasinoInfo[OHB_Info[banditid][bnCasinoID]][casBalance] < won)
							{
								SendClientMessage(pid, 0xFFFFFFFF, "У казино нет денег.");
								if(CasinoInfo[OHB_Info[banditid][bnCasinoID]][casBalance] >= OHB_Info[banditid][bnRate])
								{
									GiveMoney(pid, OHB_Info[banditid][bnRate], "MoneyBack");
									CasinoInfo[OHB_Info[banditid][bnCasinoID]][casBalance] -= OHB_Info[banditid][bnRate];
								}
							}
							else
							{

								GiveMoney(pid, won, "CasinoWin");

								CasinoInfo[OHB_Info[banditid][bnCasinoID]][casBalance] -= won;
								CasinoInfo[OHB_Info[banditid][bnCasinoID]][casWin] -= won;

								new string[128];
								format(string, sizeof(string), "Вы выиграли $%d в игровом автомате. Поздравляем!", won);
								SendClientMessage(pid, 0xFFFFFFFF, string);
							}
						}
						else
						{
							SendClientMessage(pid, 0xFFFFFFFF, "Вы ничего не выиграли.");
						}
					}
					OHB_Info[banditid][bnUserID] = INVALID_PLAYER_ID;
				}
				else
				{
					SetTimerEx("OHB_FinishSpinRoll", 50, 0, "d", banditid);
				}
			}
		}
	}
}

OHB_StartGame(playerid, banditid)
{
	if (banditid < MAX_BANDITS)
	{
		if (OHB_Info[banditid][bnCreated] == 1)
		{
			if (OHB_Info[banditid][bnBusy] == 0)
			{
				OHB_Info[banditid][bnUserID] = playerid;
				OHB_Info[banditid][bnBusy] = 1;
				OHB_Info[banditid][bnPreSpin] = 0;
				OHB_SetRollPos(banditid, 0, 0, 0);
				OHB_Info[banditid][bnRollFinishPos][0] = random(18);
				OHB_Info[banditid][bnRollFinishPos][1] = random(18);
				OHB_Info[banditid][bnRollFinishPos][2] = random(18);
				OHB_SpinRoll(banditid);
			}
		}
	}
}



