CMD:su(playerid, params[])
{
	if(!IsACop(playerid)) return SendClientMessage(playerid,COLOR_GREY,"Вы не полицейский!");
	if(PlayerInfo[playerid][pRank]<2 ) return SendClientMessage(playerid,COLOR_RED,"Подавать в розыск доступно с 2 ранга!");
	if(!TeamDuty[playerid]) return SendClientMessage(playerid,COLOR_RED,"Вы не на дежурстве!");
	new reason[100], id, wanted;
	if(sscanf(params, "uds[100]", id, wanted, reason)) return SendClientMessage(playerid,COLOR_PROJECT,"Используйте: {FFFFFF}/su [id] [розыск] [причина]");
	if(!IsPlayerConnected(id))return SendClientMessage(playerid,-1,"Игрок не в сети!");
	if(IsADomen(reason) || IsAMg(reason))
	{
		SendClientMessage(playerid,COLOR_RED,"В вашем сообщении обнаружено запрещенное содержание.");

		return false;
	}
	if(IsAIP(reason))return 1;
	if(PlayerInfo[id][pWanted]>=6)return SendClientMessage(playerid,COLOR_GREY,"У этого игрока максимальный уровень розыска!");
	//	if(PlayerInfo[id][pWanted]==wanted) return SendClientMessage(playerid,COLOR_GREY,"Этот игрок и так уже имеет такой уровень розыска.");
	if(!IsPlayerConnected(id)) return SendClientMessage(playerid,COLOR_GREY,"Неверный id игрока!");
	if(IsACop(id))return SendClientMessage(playerid,COLOR_GREY,"Нельзя объявить в розыск полицейского");

	if(PlayerInfo[id][pJailTime]) return SendClientMessage(playerid,COLOR_GREY,"Нельзя объявить в розыск заключенного");
	if(wanted < 1 || wanted > 6) return SendClientMessage(playerid,COLOR_GREY,"Нельзя дать меньше 1 и больше 6 звезд");
	SetPlayerCriminal(playerid,id,PlayerInfo[id][pWanted]+wanted,reason);
	return true;
}
