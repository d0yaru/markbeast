SetSpawn(playerid)
{
	TogglePlayerControllableEx(playerid, true);



	if(Fullness[playerid]>15 || PlayerInfo[playerid][pLevel] <= 1)// здоровье
	{
		if(PlayerInfo[playerid][pHealth]>20)
		{
			J_SetPlayerHealth(playerid,PlayerInfo[playerid][pHealth]);
		}
		else
		{
			J_SetPlayerHealth(playerid,40);
		}
	}
	else
	{
		J_SetPlayerHealth(playerid,20);
		SendClientMessage(playerid,COLOR_GOLD,"Состояние здоровья слишком низкое! Необходимо вылечиться у врача или использовать аптечку!");
		SendClientMessage(playerid,COLOR_GOLD,"Вы слишком голодны!");
	}
	if((IsPlayerInBJ[playerid] == 1 || IsPlayerInHell[playerid] == 1) && PlayerInfo[playerid][pView] < 1)
	{
		IsPlayerInBJ[playerid] = 0;
		IsPlayerInHell[playerid] = 0;


		Chances[playerid] = 0;
		Regeneration[playerid] = 0;
		IsPlayerDeath[playerid] = false;

		PlayerInfo[playerid][pHeal] = 0;
		PlayerInfo[playerid][pCheepses]=0;


		J_SetPlayerHealth(playerid,100);
		Fix_SetPlayerInterior(playerid, 1);
		Fix_SetPlayerVirtualWorld(playerid,223);
		TextDrawHideForPlayer(playerid,Textdraw4);
		HideHellDraw(playerid);
		_ResetGun(playerid);

		BizEntered[playerid] = 123; // бизид

		SetPlayerWeather(playerid,Weathers[Weather]);
		
		ApplyAnimation(playerid,"PED","SEAT_down",4.1,0,0,0,1,1,1);
		
		SetPlayerTimer(playerid,"SetAnim",500,0);
        

		return true;

	}
	BizEntered[playerid] 		= -1;
	if(PlayerInfo[playerid][pJailTime] > 0)
	{
		if(PlayerInfo[playerid][pSprunks]==1) // лс
		{

			Fix_SetPlayerInterior(playerid,SpawnIntWorld[TEAM_LSPD][0]);
			Fix_SetPlayerVirtualWorld(playerid,SpawnIntWorld[TEAM_LSPD][1]);
			//	SetPlayerTime(playerid,12,0);
		}
		else if(PlayerInfo[playerid][pSprunks]==2) //лв
		{

			Fix_SetPlayerInterior(playerid,SpawnIntWorld[TEAM_PDLV][0]);
			Fix_SetPlayerVirtualWorld(playerid,SpawnIntWorld[TEAM_PDLV][1]);
			//SetPlayerTime(playerid,12,0);
		}
		else if(PlayerInfo[playerid][pSprunks]==3) //сф
		{

			Fix_SetPlayerInterior(playerid,SpawnIntWorld[TEAM_SFPD][0]);
			Fix_SetPlayerVirtualWorld(playerid,SpawnIntWorld[TEAM_SFPD][1]);
			//	SetPlayerTime(playerid,12,0);
		}
		else
		{
			Fix_SetPlayerInterior(playerid,SpawnIntWorld[TEAM_LSPD][0]);
			Fix_SetPlayerVirtualWorld(playerid,SpawnIntWorld[TEAM_LSPD][1]);
			//	SetPlayerTime(playerid,12,0);

		}
		J_SetPlayerHealth(playerid,100);
		return 1;
	}
	else if(PlayerInfo[playerid][pView]>0)
	{
		IsPlayerInBJ[playerid] = 0;

		J_SetPlayerHealth(playerid,100);
		Fix_SetPlayerInterior(playerid, 6);
		Fix_SetPlayerVirtualWorld(playerid,1229);

		IsPlayerInHell[playerid] = 0;

		SetPlayerWeather(playerid,Weathers[Weather]);

		Chances[playerid] = 0;
		Regeneration[playerid] = 0;
		IsPlayerDeath[playerid] = false;
		return 1;
	}
	else if(PlayerInfo[playerid][pHeal] > 0)//Если чувак спавниться в больничке
	{
		switch(PlayerInfo[playerid][pCheepses])//spawn больнички
		{
		case 1:
			{
				Fix_SetPlayerVirtualWorld(playerid, 1);
				Fix_SetPlayerInterior(playerid, 2);
			}
		case 2:
			{
				Fix_SetPlayerVirtualWorld(playerid, 2);
				Fix_SetPlayerInterior(playerid, 3);
			}
		default:
			{
				Fix_SetPlayerVirtualWorld(playerid, 3);
				Fix_SetPlayerInterior(playerid, 3);
			}
		}
		J_SetPlayerHealth(playerid,25);
		if(startlomka[playerid]==1)PlayerInfo[playerid][pOldRepl]=0;
		PlayerInHealSkin[playerid] = 1;
		return 1;
	}
	else//спавн дома
	{
		if(PlayerInfo[playerid][pDCTime]==0)
		{
			Fix_SetPlayerInterior(playerid, 0);
			Fix_SetPlayerVirtualWorld(playerid, 0);

		}
		if(PlayerInfo[playerid][pDCTime]==2 && PlayerInfo[playerid][pHouseKey] != -1)//проверка выбора спавна и наличия ключей
		{
			if(GetString(HouseInfo[PlayerInfo[playerid][pHouseKey]][hOwner],Name(playerid)))//проверка соответствия имени и
			{
				pTemp[playerid][Anti_Pvar][40] = CurrentTimer+4;
				HouseEntered[playerid] = PlayerInfo[playerid][pHouseKey];//отмечает в каком доме находиться чувак и отправляет его туда
				Fix_SetPlayerInterior(playerid,HouseInfo[PlayerInfo[playerid][pHouseKey]][hInt]);
				Fix_SetPlayerVirtualWorld(playerid,PlayerInfo[playerid][pHouseKey]+50);
				return 1;
			}
			else
			{
				new s = GetHouseStyle(PlayerInfo[playerid][pHouseKey]);//тип дома
				for(new i;i < HouseRooms[s];i ++)// перебираеться количество комнат
				{
					if(GetString(HRoomOwner[PlayerInfo[playerid][pHouseKey]][i],Name(playerid)))//возвращает комнаты
					{
						pTemp[playerid][Anti_Pvar][40] = CurrentTimer+4;
						HouseEntered[playerid] = PlayerInfo[playerid][pHouseKey];
						Fix_SetPlayerInterior(playerid,HouseInfo[PlayerInfo[playerid][pHouseKey]][hInt]);
						Fix_SetPlayerVirtualWorld(playerid,PlayerInfo[playerid][pHouseKey]+50);
						return 1;
					}
				}
			}
		}
		if(PlayerInfo[playerid][pDCTime]==1 && PlayerInfo[playerid][pFrom] != -1)//проверка выбора спавна и наличия ключей
		{
			if(GetString(HotelInfo[PlayerInfo[playerid][pFrom]][vladelec],Name(playerid)))//проверка соответствия имени и
			{
				Fix_SetPlayerInterior(playerid,2);
				Fix_SetPlayerVirtualWorld(playerid,PlayerInfo[playerid][pFrom]+1);
				return 1;
			}
			else
			{
				PlayerInfo[playerid][pFrom]=-1;
			}
		}
		else if(PlayerInfo[playerid][pDCTime]==3 && GetTeamID(playerid) != 0)
		{//ниже если состоит в организации.
			J_SetPlayerFacingAngle(playerid,SpawnInfo[PlayerInfo[playerid][pMember]][3]);
			Fix_SetPlayerInterior(playerid,SpawnIntWorld[PlayerInfo[playerid][pMember]][0]);
			Fix_SetPlayerVirtualWorld(playerid,SpawnIntWorld[PlayerInfo[playerid][pMember]][1]);
			return 1;
		}
	}
	Fix_SetPlayerInterior(playerid, 0);
	Fix_SetPlayerVirtualWorld(playerid, 0);
	PlayerInfo[playerid][pDCTime] = 0;
	return true;
}
