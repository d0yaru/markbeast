CMD:capture(playerid, params[])
{
	if(!IsAtGang(playerid)) return SendClientMessage(playerid, COLOR_GREY, "Вы должны быть в банде, чтобы учавствовать в войнах за территорию.");
	if(PlayerInfo[playerid][pRank] < 9) return SendClientMessage(playerid, COLOR_GREY, "Для захвата зоны необходимо иметь ранг не менее 9-го.");
	new h, m, s; gettime(h,m,s);
	if(h<11 || h>22) return SendClientMessage(playerid, COLOR_GREY, "Захват можно проводить не раньше 11:00 и не позже 22:59");
	if(CaptureTime[0]>0 || CaptureTime[1]>0) return SendClientMessage(playerid, COLOR_GREY, "Уже идет атака на один из районов города.");
	new Atac = GetTeamID(playerid);

	if(RecoveryTime[Atac] > 0)
	{
		new str[120];
		format(str,sizeof(str),"Вы еще не восстановились после предыдущей войны за территорию, начать новую можно через {FFFFFF} %d {AFAFAF}секунд",RecoveryTime[Atac]);
		return SendClientMessage(playerid,COLOR_GREY,str);
	}

	new zone = GetPlayerGangZone(playerid);
	if(zone == -1) return SendClientMessage(playerid, COLOR_GREY, "Вы не находитесь ни на одной из территорий");
	if(!IsGangToZone(Atac, zone, 50.0)) return SendClientMessage(playerid, COLOR_GREY, "Для захвата зоны хотя-бы половина всех членов банды должна присутствовать на захвате");
	if(GZInfo[zone][gzFracID] == Atac) return SendClientMessage(playerid, COLOR_GREY, "Это и так ваша территория");

	if(RecoveryTime[GZInfo[zone][gzFracID]] > 0)
	{
		new str[120];
		format(str,sizeof(str),"Противник еще не восстановился после предыдущей войны, начать новую можно через {FFFFFF} %d {AFAFAF}секунд",RecoveryTime[GZInfo[zone][gzFracID]]);
		return SendClientMessage(playerid,COLOR_GREY,str);
	}
	if(!GZInfo[zone][gzFracID])
	{
		GangZoneHideForAll(GangZone[zone]);
		GangZoneShowForAll(GangZone[zone],TeamColors[Atac]);
		GZInfo[zone][gzFracID] = Atac;
		RecoveryTime[Atac] = 1800;
		SendTeamMessage(Atac,COLOR_PROJECT,"Ваша банда захватила новую территорию");
		UpdateDorm(Atac);
		SaveGangZone(zone);
	}else{
		if(IsGangConnected(GZInfo[zone][gzFracID])<3) return SendClientMessage(playerid, COLOR_GREY, "Для захвата зоны должен быть онлайн хотябы 3 членов банды владеющей этой зоной");
		SendTeamMessage(Atac,COLOR_PROJECT,"Ваша банда начала захват зоны, продержитесь 10 минут");
		SendTeamMessage(GZInfo[zone][gzFracID],COLOR_RED,"На 1-у из ваших зон напали, у вас 10 минут чтобы отбить зону");
		//
		new string[300];
		format(string,sizeof(string),"[A] Банда %s [%s(%d)] начала захват територии банды %s",OrgInfo[Atac][oName], PN(playerid), playerid,OrgInfo[GZInfo[zone][gzFracID]][oName]);
		SendAdminsMessage(COLOR_ORANGE,string);
		///
		CaptureZone = zone;
		CaptureDef = GZInfo[zone][gzFracID];
		CaptureAtac = Atac;
		CaptureAtacKill = 0;
		CaptureDefKill = 0;
		CaptureTime[0] = 10;
		CaptureTime[1] = 0;//10 минут
		GangZoneFlashForAll(GangZone[CaptureZone],TeamColors[CaptureAtac]);
	}
	return true;
}
