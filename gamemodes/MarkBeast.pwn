new PlayerEditingSlot[MAX_PLAYERS];
StartEditingMeuble(playerid)
{
	new houseid = PlayerInfo[playerid][pHouseKey];
	if(GetPlayerVirtualWorld(playerid) != houseid+50) return SendClientMessage(playerid, COLOR_GREY, "Вы не у себя дома!");
	new SlotID = GetClosestMeuble(playerid, houseid);
	if(SlotID == -1) return SendClientMessage(playerid, COLOR_GREY, "У вас дома нет мебели!");
	PlayerEditingSlot[playerid] = SlotID;
	new model = HouseInfo[houseid][hMeublesModel][SlotID];
	
    if(EditModel[playerid] != -1)
    {
        DestroyPlayerObject(playerid, EditModel[playerid]);
        EditModel[playerid] = -1;
    }
    
    new Float:X,Float:Y,Float:Z, Float:A;
    GetPlayerPos(playerid,X,Y,Z);
	GetPlayerFacingAngle(playerid, A);
	ShiftCords(0, X, Y, Z, A, 3.0);
    EditModel[playerid] = CreatePlayerObject(playerid, model, X, Y, Z, 0.0, 0.0, 0.0);
    EditPlayerObject(playerid, EditModel[playerid]);
	return 1;
}
public OnPlayerEditObject(playerid, playerobject, objectid, response, Float:fX, Float:fY, Float:fZ, Float:fRotX, Float:fRotY, Float:fRotZ)
{
	if(playerobject)
	{
		if(objectid == PreviewObject[playerid])
		{

			if(response == EDIT_RESPONSE_FINAL)
			{
				if(PlayerInfo[playerid][pMeuble] <= 1) return SendClientMessage(playerid, COLOR_GREY, "У вас нет мебели!");
				new i  = PlayerInfo[playerid][pHouseKey];
				if(GetPlayerVirtualWorld(playerid) != i+50) return SendClientMessage(playerid, COLOR_GREY, "Вы не у себя дома!");


				if(PreviewObject[playerid] != -1)
			    {
			        DestroyPlayerObject(playerid, PreviewObject[playerid]);
			        PreviewObject[playerid] = -1;
			    }
				IsertMeuble(playerid, i, PlayerInfo[playerid][pMeuble], fX, fY, fZ, fRotX, fRotY, fRotZ);
				PlayerInfo[playerid][pMeuble] = 0;
				return SendClientMessage(playerid, COLOR_GREY, "Вы успешно установили мебель!");
			}

			if(response == EDIT_RESPONSE_CANCEL)
			{
				if(PreviewObject[playerid] != -1)
			    {
			        DestroyPlayerObject(playerid, PreviewObject[playerid]);
			        PreviewObject[playerid] = -1;
			    }
			    return SendClientMessage(playerid, COLOR_GREY, "Вы отменили установку мебели!");
			}
		}
		if(objectid == EditModel[playerid])
		{
			if(response == EDIT_RESPONSE_FINAL)
			{
				new i  = PlayerInfo[playerid][pHouseKey];
				if(GetPlayerVirtualWorld(playerid) != i+50) return SendClientMessage(playerid, COLOR_GREY, "Вы не у себя дома!");


				if(EditModel[playerid] != -1)
			    {
			        DestroyPlayerObject(playerid, EditModel[playerid]);
			        EditModel[playerid] = -1;
			    }
			    new SlotID = PlayerEditingSlot[playerid];
			    DestroyDynamicObject(HouseInfo[i][hMeublesID][SlotID]);
				UpdateMeuble(i, HouseInfo[i][hMeublesModel][SlotID], fX, fY, fZ, fRotX, fRotY, fRotZ, SlotID);
				return SendClientMessage(playerid, COLOR_GREY, "Вы успешно передвинули мебель!");
			}

			if(response == EDIT_RESPONSE_CANCEL)
			{
				if(EditModel[playerid] != -1)
			    {
			        DestroyPlayerObject(playerid, EditModel[playerid]);
			        EditModel[playerid] = -1;
			    }
			    return SendClientMessage(playerid, COLOR_GREY, "Вы отменили перестановку!");
			}
		}
	}
	return true;
}
