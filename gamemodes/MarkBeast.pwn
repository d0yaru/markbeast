CMD:fpark(playerid, params[])
{
	new Float:vhealth;
	if(GetPlayerState(playerid) != 2) return SendClientMessage(playerid,COLOR_RED,"Вы должны быть за рулём семейного авто!");
	if(PlayerInfo[playerid][pFam] <= 0) return SendClientMessage(playerid, COLOR_GREY, "У вас нет семьи.");
	new carid = GetPlayerVehicleID(playerid);
	if(carid > 0)
	{
		new FamVeh_ID = GetFamVehicleID(carid);
		if(FamVehInfo[FamVeh_ID][FamID] != PlayerInfo[playerid][pFam]) return SendClientMessage(playerid,COLOR_RED,"Этот транспорт не принадлежит вашей семье!");

		new panels,doors_,lights_,tires;
		GetVehicleDamageStatus(carid,panels,doors_,lights_,tires);

		GetVehiclePos(carid,FamVehInfo[FamVeh_ID][FX_Cord],FamVehInfo[FamVeh_ID][FY_Cord],FamVehInfo[FamVeh_ID][FZ_Cord]);
		GetVehicleZAngle(carid,FamVehInfo[FamVeh_ID][FRotation]);
		GetVehicleHealth(carid,vhealth);
		DestroyVehicleEx(carid);


		FamVehID[FamVeh_ID] = J_CreateVehicle(FamVehInfo[FamVeh_ID][fmodelid],FamVehInfo[FamVeh_ID][FX_Cord],FamVehInfo[FamVeh_ID][FY_Cord],FamVehInfo[FamVeh_ID][FZ_Cord],FamVehInfo[FamVeh_ID][FRotation],FamVehInfo[FamVeh_ID][FColor_1],FamVehInfo[FamVeh_ID][FColor_2],60000);


		FamRealVehicleID[FamVehID[FamVeh_ID]] = FamVeh_ID;

		new carnnum[64];
		format(carnnum,sizeof(carnnum),"%s",FamInfo[GetPlayerFamID(playerid)][fName]);
		SetVehicleNumberPlate(carid,carnnum);

		AC_PutPlayerInVehicle(playerid,carid,0);
		J_SetVehicleHealth(carid,vhealth);

		UpdateVehicleDamageStatus(carid,panels,doors_,lights_,tires);
		if(!IsAVelik(carid)) SetEngineStatus(carid,false, playerid);
		else SetEngineStatus(carid,true, playerid);

		SaveFamVeh(FamVeh_ID);
		return true;
	}
	SendClientMessage(playerid,COLOR_RED,"Вы должны быть за рулём семейного авто!");
	return true;
}
