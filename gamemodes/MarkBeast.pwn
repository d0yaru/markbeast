CMD:spveh(playerid, params[])
{
	if(PlayerInfo[playerid][pAdmin] >=1)
	{
		new Float:pos;
		if(sscanf(params,"f",pos)) return SendClientMessage(playerid,COLOR_PROJECT,"Используйте:{FFFFFF} /spveh [радиус]");
		if(pos > 10.0)
		{
			if(PlayerInfo[playerid][pAdmin] < 3) return SendClientMessage(playerid,COLOR_PROJECT,"Ваш лвл не позволяет спавнить в радиусе дальше 10.0");
		}
		new Float:car_x,Float:car_y,Float:car_z;
		new bool:Occuped[MAX_VEHICLES];
		foreach(i)
		{
			if(IsPlayerInAnyVehicle(i))
			{
				new carid = GetPlayerVehicleID(i);
				Occuped[carid] = true;
				if(IsTrailerAttachedToVehicle(carid)) Occuped[GetVehicleTrailer(carid)] = true;
			}
		}
		for(new i = 0; i < MAX_VEHICLES;i ++)
		{
			if(i == BotVehicles[0] ||  i == BotVehicles[1])continue;
			if(IsVehicleConnected(i))
			{
				if(!Occuped[i])
				{
					GetVehiclePos(i,car_x,car_y,car_z);
					if(IsPlayerInRangeOfPoint(playerid,pos,car_x,car_y,car_z))
					{
						SetVehicleToRespawn(i);
						SetEngineStatus(i, false, -1);
					}
				}
			}
		}
		CMDstr ="";
		format(CMDstr,sizeof(CMDstr),"[A] Администратор %s[%d] заспавнил транспорт в радиусе %f!",PN(playerid),playerid,pos);
		SendAdminsMessage(COLOR_GREY,CMDstr);
	}
	return true;
}
