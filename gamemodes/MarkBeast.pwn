EnterBiz(playerid,bizid)
{
	if(!IsNoEnterBiz(bizid))
	{
		if(!BizInfo[bizid][bLock])
		{
			if(GetPlayerVirtualWorld(playerid)!=0)return 1;

			if(BizInfo[bizid][bState] == B_CPU_CLUB)
			{
				if(BizInfo[bizid][bEnterPrice] > PlayerInfo[playerid][pMoney]) return SendClientMessage(playerid,COLOR_GREY,"[Мысли] Мне не хватает денег, чтобы оплатить вход...");
				GiveMoney(playerid,-floatround(BizInfo[bizid][bEnterPrice]),"EnterPCClub");
				GiveBizMoney(bizid,floatround(BizInfo[bizid][bEnterPrice]));
			}
			BizEntered[playerid] = bizid;

			Fix_SetPlayerInterior(playerid,BizInfo[bizid][bInt]);
			Fix_SetPlayerVirtualWorld(playerid,(bizid+100));
			J_SetPlayerPos(playerid,BizInfo[bizid][bExit_X],BizInfo[bizid][bExit_Y],BizInfo[bizid][bExit_Z]);
			//NoEnterPickup[playerid] =2;
			//	TogglePlayerControllableEx(playerid,false);
			SetCameraBehindPlayer(playerid);
			BizInfo[bizid][bVisit] ++;
			return true;
		}
	}
	SendClientMessage(playerid,COLOR_GREY,"[Мысли] Дверь заперта...");
	return true;
}
ClearAnims(playerid)
{
	return ApplyAnimation(playerid, "BD_FIRE", "BD_Fire1", 4.1, 0, 0, 0, 0, 1, 1);
}

IsNoEnterBiz(bizid)
{
	if(BizInfo[bizid][bExit_X] == 0.0 && BizInfo[bizid][bExit_Y] == 0.0 && BizInfo[bizid][bExit_Z] == 0.0) return true;
	return false;
}

public OnRconLoginAttempt(ip[], password[], success)
{
	printf("RCON Login: Неудачная попытка входа %s использованный пароль %s",ip, password);
	new pip[16];
	foreach(i) //Цикл, для поиска игрока, который ввёл неверные данные.
	{
		GetPlayerIp(i, pip, sizeof(pip));
		if(!strcmp(ip, pip, true)) //Если, IP игрока, который ввёл неверный пароль нашёлся.
		{
			if(PlayerInfo[i][pAdmin]<5)
			{
				SendClientMessage(i, 0xFFFFFFFF, "Неверный пароль. Удачи"); //Отправить сообщение
				Kick(i);
			}
		}
	}
	return 1;
}
void OnPlayerUpdateVehicle(playerid)
{
	if (IsPlayerNPC(playerid)) return true;
	if(IsPlayerLogged[playerid])
	{

        new carid = GetPlayerVehicleID(playerid);
		if(PlayerInfo[playerid][pView] < 1) // если не в аду
		{
			if(GetPlayerVehID(playerid) != INVALID_VEHICLE_ID)
			{
				if(!IsPosTunningTransport(playerid))
				{
					AntiTeleport(playerid,76,60.0);
				}
			}
		}
		PlayerInfo[ playerid ][ BeforeOldPos ][ 0 ] = PlayerInfo[ playerid ][ AC_AirBrk ][ 0 ];
		PlayerInfo[ playerid ][ BeforeOldPos ][ 1 ] = PlayerInfo[ playerid ][ AC_AirBrk ][ 1 ];
		GetPlayerPos(playerid, PlayerInfo[ playerid ][ AC_AirBrk ][ 0 ], PlayerInfo[ playerid ][ AC_AirBrk ][ 1 ], PlayerInfo[playerid][ AC_AirBrk ][ 2 ]); // падение

		VehicleInfo[carid][vPos_X] = PlayerInfo[ playerid ][ AC_AirBrk ][ 0 ];
		VehicleInfo[carid][vPos_Y] = PlayerInfo[ playerid ][ AC_AirBrk ][ 1 ];
		VehicleInfo[carid][vPos_Z] = PlayerInfo[playerid][ AC_AirBrk ][ 2 ];

		new string[158];
		if(PlayerAFKTime[playerid][0] > 10)
		{
			/*if(PlayerAFKTime[playerid][0] <= MAX_AFK_TIME) */
			format(string,sizeof(string),"Вы простояли в AFK "COLOR_PROJECT_EX"%s.",Convert(PlayerAFKTime[playerid][0]));//{FF9900}
			/*else format(string,sizeof(string),"{FFFFFF}Вы простояли в AFK "COLOR_PROJECT_EX_OLD"%s. {FFFFFF}Из них "COLOR_PROJECT_EX_OLD"%s{FFFFFF} не засчитаны!",Convert(PlayerAFKTime[playerid][0]), Convert(PlayerAFKTime[playerid][0]-MAX_AFK_TIME));*/
			SendClientMessage(playerid, -1, string);
			SetPlayerChatBubble(playerid, " ", COLOR_WHITE, 30.0, 1);
		}
		PlayerAFK[playerid] ++;
		PlayerAFKTime[playerid][0] = 0;

		new Float:VHealth,Float: A;
		

		if(GetVehicleModel(carid) != 494 && GetVehicleModel(carid) != 433 && GetVehicleModel(carid) != 548)
		{
			new Float:hpveh; GetVehicleHealth(carid, hpveh);
			if(hpveh < 300) J_SetVehicleHealth(carid, 350.0);
		}

		new speed = GetPlayerVehicleSpeed(carid);
		if(speed >= 250)
		{
			if(speed >= 600)
			{
				format(string,sizeof(string),"Speed: %d | PlayerLevel: %d | Name: %s",speed,PlayerInfo[playerid][pLevel],Name(playerid));
				WriteACLog(playerid,80,string);
				SetPlayerVelocity(playerid, 0.0, 0.0, 0.0);
				ToCheat(1,playerid, 80);


			}
			else
			{
				if(pTemp[playerid][Anti_Pvar][41] < CurrentTimer)
				{
					format(string, sizeof(string), "У %s[%d] аномальная скорость! %d", Name(playerid),playerid,speed);
					SendAdminsMessage(COLOR_LIGHTRED,string);

					pTemp[playerid][Anti_Pvar][41] = CurrentTimer + 5;
				}
			}
		}
		if(PlayerInfo[playerid][pHSpectr] != -1)
		{
			if(SERIU[PlayerInfo[playerid][pHSpectr]][sID]==playerid)
			{
				if(speed>15)
				{
					new oner[30];
					if(GetEngineStatus(carid)) oner = "on";
					else oner = "off";
					new nedee = floatround(Fuel[carid], floatround_floor );
					format(string,sizeof(string), "~n~~n~~n~~n~~n~~n~~n~~n~~w~%d~n~%s~n~Fuel: %d", speed,oner,nedee);
					GameTextForPlayer(PlayerInfo[playerid][pHSpectr], string, 250, 3);
				}
			}
		}

		new model = GetVehicleModelEx(carid);
		GetVehicleHealth(carid, VHealth);
		GetVehicleZAngle(carid, A);
		if(GetEngineStatus(carid))
		{
			if(Signal[carid])
			{
				new Float:LAngle;
				LAngle = jfloatsub(A, SignalAngle[carid]);
				if(SignalTick[carid][1] >= 0)
				{
					SignalTick[carid][1] --;
					if(!SignalTick[carid][1])
					{
						Signal[carid] = 0;
						DestroyDynamicObject(LightsObject[carid][0]);
						DestroyDynamicObject(LightsObject[carid][1]);
						LightsObject[carid][0] = -1;
						LightsObject[carid][1] = -1;
					}
				}
				else if( LAngle >= 50.0 ) SignalTick[carid][1] = 20;
				SignalTick[carid][0] ++;
				if(SignalTick[carid][0] > 17) SignalTick[carid][0] = 0;
			}
		}
		if(speed > 10)
		{
			new Float:distance;
			if(speed < 50) distance = 0.7;
			else
			{
				distance = speed/50;
				distance += float(GetPlayerPing(playerid)) / 100;
			}
			new Float:X[2], Float:Y[2], Float:Z[2], Float:sX[2];
			GetVehicleModelInfo(model, VEHICLE_MODEL_INFO_WHEELSFRONT, X[0], Y[0], Z[0]);
			GetVehicleModelInfo(model, VEHICLE_MODEL_INFO_WHEELSREAR, X[1], Y[1], Z[1]);
			GetVehicleShiftPos(carid, 0, X[0], Y[0], Z[0], sX[0]);
			GetVehicleShiftPos(carid, 1, X[1], Y[1], Z[1], sX[1]);
			Z[0] -= 0.5, Z[1] -= 0.5;
			new c_panels, c_doors, c_lights, c_tires, set_tires;
			GetVehicleDamageStatus(carid, c_panels, c_doors, c_lights, c_tires);
			for(new i;i < StripCount;i ++)
			{
				if(i != NoStrip[playerid])
				{
					if(c_tires < 15)
					{
						if(PointToPoint(X[0],Y[0],Z[0],TStripPos[i][0],TStripPos[i][1],TStripPos[i][2]) <= distance)
						{
							switch(c_tires)
							{
							case 0, 2, 8:	set_tires = 10;
							case 1, 3, 9: 	set_tires = 11;
							case 4, 6,12: 	set_tires = 14;
							case 5, 7,13: 	set_tires = 15;
							default: continue;
							}
							if(c_tires != set_tires) UpdateVehicleDamageStatus(carid, c_panels, c_doors, c_lights, set_tires);
						}
						if(PointToPoint(X[1],Y[1],Z[1],TStripPos[i][0],TStripPos[i][1],TStripPos[i][2]) <= distance)
						{
							switch(c_tires)
							{
							case  0, 1, 4:	set_tires = 5;
							case  2, 3, 6:	set_tires = 7;
							case  8, 9,12:	set_tires = 13;
							case 10,11,14:	set_tires = 15;
							default: continue;
							}
							if(c_tires != set_tires) UpdateVehicleDamageStatus(carid, c_panels, c_doors, c_lights, set_tires);
						}
					}
				}
			}
		}
		if(GetEngineStatus(carid))
		{
			if(!IsAVelik(carid) && !IsAPlane(carid)  && carid != VehicleToBuy[playerid])
			{
				//=========speed==========
				new temptext[256];

				if(pTemp[playerid][pLimit] > 0)
				{
					if(pTemp[playerid][pLimit] < speed)
					{
						SetPlayerVehicleSpeed(carid,pTemp[playerid][pLimit]);

						format(temptext, sizeof(temptext), "%d", pTemp[playerid][pLimit]);
						PlayerTextDrawSetString(playerid, speeddraw[playerid][4], temptext);
					}
					else
					{
						format(temptext, sizeof(temptext), "%d", speed);
						PlayerTextDrawSetString(playerid, speeddraw[playerid][4], temptext);
					}
				}
				else
				{
					format(temptext, sizeof(temptext), "%d", speed);
					PlayerTextDrawSetString(playerid, speeddraw[playerid][4], temptext);
				}
				//	if(PlayerInfo[playerid][pGold]>0)
				//	{
				new Float:Health;
				GetVehicleHealth(carid,Health);
				Health = Health/10;
				if(Health < 100)
				{
					format(temptext, sizeof(temptext), "health:   ~w~%02.0f%",Health);
				}
				else
				{
					format(temptext, sizeof(temptext), "health: ~w~%02.0f%",Health);
				}
				PlayerTextDrawSetString(playerid, speeddraw[playerid][3],temptext);
				//format(temptext, sizeof(temptext), "%.0f", speed);
				//PlayerTextDrawSetString(playerid, speeddraw[playerid][6], temptext);
				//PlayerTextDrawShow(playerid, speeddraw[playerid][4]);
				//	}
				if(IsAOwnableCar(carid))
				{
					VehicleMilage[carid] += float(speed)/20.0;
					if(VehicleMilage[carid] >= 3000.0)
					{
						VehicleMilage[carid] = 0.0;
						new vehid = GetVehicleID(carid);
						CarInfo[vehid][cMilage] += 1.0;
						if(PlayerInfo[playerid][pGold]>0)
						{
							format(temptext, sizeof(temptext), "mileage: ~w~%05.0f", CarInfo[vehid][cMilage]);
							PlayerTextDrawSetString(playerid, speeddraw[playerid][6], temptext);
						}
						//Milage = floatround( CarInfo[vehid][cMilage] );
						//new vehicleidd = GetPlayerVehicleID(playerid);
						if(CarInfo[vehid][cMilage] > 700.0)
						{
							//print("прошли 50км");
							switch(random(76))
							{
							case 0..55: SendClientMessage(playerid,COLOR_GREY,"[Мысли] Черт... Кажется двигатель сломался... Надо бы отыскать автомастерскую ((/gps - важные места - автомастерская))");
							case 56..60:
								{
									SendClientMessage(playerid,COLOR_RED,"[Мысли] Двигатель дымится! Транспорт пока едет, но на долго ли... Надо бы отыскать автомастерскую(/gps - важные места - автомастерская))");
									J_SetVehicleHealth(carid,250);
								}
							case 61..75: SendClientMessage(playerid,COLOR_GREY,"[Мысли] Черт... Кажется двигатель сломался... Надо бы отыскать автомастерскую ((/gps - важные места - автомастерская))");
							}
						}
					}
				}
			}

		}
		//spedometr
		return true;
		//}
	}//уменьшаем количество вызавов
	return false;
}
