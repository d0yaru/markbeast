							else
							{
								ToCheat(1,playerid, 39);
							}
							return true;
						}
					}
				}

				if(WeaponInfo[playerid][weap] == false)
				{
				    if(!IsPlayerInAnyVehicle(playerid))
				 	{

						pTemp[playerid][PTimeOut]= pTemp[playerid][PTimeOut]+ 2;

						if(PlayerAFKTime[playerid][0] > 0){pTemp[playerid][PTimeOut]--;}


						if(pTemp[playerid][PTimeOut]>= 4)// 3 шанса
						{
							pTemp[playerid][PTimeOut]= 0;


							string  = "";
							format(string,sizeof(string),"WeaponInfo[playerid][%d]: %d | PlayerLevel: %d | Name: %s",weap,WeaponInfo[playerid][weap],PlayerInfo[playerid][pLevel],Name(playerid));
							WriteACLog(playerid,38,string);


							_ResetGun(playerid);
							if(PlayerInfo[playerid][pView] > 0) // если в аду кикаем
							{
								SendClientMessage(playerid, -1,"Читы на оружие запрещены! код 1");
								Kick(playerid);
								return true;
							}
							else
							{
								ToCheat(1,playerid, 38);
							}
							return true;
						}
					}
				}
			}

		}
	}
	if(pTemp[playerid][PTimeOut]> 0)// очистка подозрений
	{
		pTemp[playerid][PTimeOut]--;
	}

	if(GunWarnings[playerid] > 0) {GunWarnings[playerid] = GunWarnings[playerid] - 1;} // очистка подозрений
	return true;
}

