GiveDonate(playerid,money,reason[] = "None")
{
	if(!IsPlayerLogged[playerid]) return 0;

	new string[300];
	PlayerInfo[playerid][pVirMoney] += money;

	format(string,sizeof(string), "UPDATE  `usrboston` SET  `VirMoney` = `VirMoney`+'%d' WHERE  `NickName`='%s';", money, Name(playerid));
	mysql_tquery(mysql, string);

	if(money > 0){format(string,sizeof(string) , "%s получил %d$ за %s", Name(playerid),money,reason);}
	else {format(string,sizeof(string) , "%s отдал %d$ за %s", Name(playerid),money,reason);}
	WriteDonateLog(string);
	return true;
}
WriteDonateLog(string[])
{
	new write[256],minute,second,hour,day,month,year;
	gettime(hour,minute,second);
	getdate(year,month,day);
	format(write, sizeof(write), "[Logs]/[Donate]/%04.d-%02.d-%02.d.log",year,month,day);
	new File:hFile = fopen(write, io_append);
	format(write, sizeof(write), "[%d.%02d.%02d | %02d:%02d:%02d] %s\r\n",day,month,year,hour,minute,second,string);
	for(new i; i < strlen(write); i ++) fputchar(hFile, write[i], false);
	fclose(hFile);
	return true;
}
