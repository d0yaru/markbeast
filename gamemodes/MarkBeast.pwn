public OnPlayerEnterVehicle(playerid, vehicleid, ispassenger)
{
    new string[150];
    new tempvehmodel = GetVehicleModel(vehicleid);
	if(tempvehmodel!=570 && tempvehmodel!=449)
	{

		EnterVehT[playerid] = GetTickCount();
	    
	/*	new Float:x,Float:y,Float:z;
		GetVehiclePos(vehicleid,x,y,z);
		if(!IsPlayerInRangeOfPoint(playerid,20,x,y,z) && GetPlayerVehicleSpeed(vehicleid)<50)
		{
			ToCheat(0,playerid);
			//return true;
		}*/
	}
	else
	{
	    EnterVehT[playerid] = 1;
	}
    
	if(!Controllable[playerid]) return ClearAnimations(playerid);
	if(BotVehicles[1] == vehicleid)
	{
		ExitCar(playerid);
		//	 ClearAnimations(playerid);
		return true;
	}

	
	VehicleID[playerid]=vehicleid;
	
	EnterVehicle[playerid] = vehicleid;
	new driver = 0;
	for(new i = 0, j = GetPlayerPoolSize(); i <= j; i++)
	{
		if(!IsPlayerLogged[i]) continue;
		if(EnterVehicle[i] == vehicleid)
		{
			driver++;
			break;
		}
	}
	if(GetVehicleSeat(vehicleid, 0) != INVALID_PLAYER_ID && VehicleInfo[vehicleid][vTeam]>0 && !ispassenger)
	{
		ExitCar(playerid),SendClientMessage(playerid,COLOR_ORANGE, "Место водителя занято!");
		return 1;
	}
	if(driver==0 && !ispassenger)
	{
		format(string,sizeof(string),"[A] %s[%d] возможно использует чит чтобы тп кар",PN(playerid),playerid);
		SendAdminsMessage(COLOR_GREY,string);
	}

	if(!ispassenger)
	{
		GetVehicleZAngle(vehicleid,vRut[playerid][0]);
		CheckEnterCar(playerid, vehicleid);
		CheckEnterCarOnce(playerid, vehicleid, false);
	}
	if(IsAOwnableCar(vehicleid))
	{
		new number = GetVehicleID(vehicleid);
		if(CarInfo[number][cLock])
		{
			if(PlayerInfo[playerid][pAdmin] < ADM_GRAND)
			{
				ClearAnimations(playerid);
				SendClientMessage(playerid,COLOR_GREY, "Этот транспорт закрыт!");
			}
			if(CarInfo[number][cAlarmON])
			{
				SetAlarmStatus(vehicleid,true);
				AlarmTime[vehicleid] = 20;
				foreach(i)
				{
					if(IsPlayerHaveCar(i,vehicleid))
					{
						new Float:X,Float:Y,Float:Z;
						GetVehiclePos(vehicleid, X,Y,Z);
						format(string,sizeof(string),"{FFFFFF}На транспорте [%s] сработала сигнализация",OwnCarNumber(vehicleid));
						SendClientMessage(i,COLOR_YELLOW,string);
						SendClientMessage(i,-1,"Местоположение транспорта указано на карте!");
						EnableGPSForPlayer(i,X,Y,Z);
						break;
					}
				}
			}
		}
	}

	if(IsAOwnableFamCar(vehicleid))
	{
		new FamVeh_ID = GetFamVehicleID(vehicleid);
		if(FamVehInfo[FamVeh_ID][FClose])
		{
			ClearAnimations(playerid);
		}
		return true;
	}
	return true;
}
