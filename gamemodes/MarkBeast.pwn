							{
								SetSpawnInfoEx(i, skininjail, JailPos[3][0],JailPos[3][1],JailPos[3][2], 180.0);//lv
								PlayerInfo[i][pSprunks]=2; //lv
								Fix_SetPlayerInterior(i,SpawnIntWorld[TEAM_PDLV][0]);
								Fix_SetPlayerVirtualWorld(i,SpawnIntWorld[TEAM_PDLV][1]);
								SetPlayerTime(i,12,0);
							}
							_ResetGun(i);
							PlayerInfo[i][pWanted] = 0;
							SetPlayerWantedLevel(i,0);
							PlayerSpawn(i);
							PlayerInfo[i][pDrugs] = 0;
							PlayerInfo[i][pMats] = 0;
							//if(PlayerInfo[i][pMoney]>GLS[16])GiveMoney(i, -GLS[16],"Prison"),aml(52, i, "", "ареста", GLS[16]);
							format(string, sizeof(string), ">> Вы были посаженны в тюрьму на %d минут.", (PlayerInfo[i][pJailTime]/60));
							SendClientMessage(i, COLOR_ADMINRED, string);
							SendClientMessage(i, COLOR_LIGHTBLUE, ">> Используйте /advokats чтобы найти адвоката который вам поможет выйти отсюда!");
							format(string, sizeof(string), ">> Вы посадили игрока %s в тюрьму на %d минут.", PN(i),(PlayerInfo[i][pJailTime]/60));
							SendClientMessage(playerid, COLOR_GREEN, string);

							TogglePlayerControllableEx(i, true);
							num = 4;
							SetPlayerSkinEx(i,skininjail);
							break;
						}
						else if(num!=4)num = 2;
					}
					else if(num!=4)num = 1;
				}
			}
		}
		if(num==1) SendClientMessage(playerid,-1,"Вы не можете посадить игрока который не находится в розыске!");
		if(num ==2) SendClientMessage(playerid,-1,"Игрок которого вы хотите посадить должен находится у вас в машине!");
		num=0;
		TogglePlayerDynamicCP(playerid, JailCP[jail], false);
		return true;
	}
	return false;
}
