					{
						SendClientMessage(id,-1,"К вам в такси сел пассажир, но вы не получите с него прибыли, так-как вы не установили тариф!");
						SendClientMessage(playerid,-1,"Этот таксист еще не установил тариф за свои услуги!");
					}
				}
			}
		}
		////

	}
	if(oldstate == 2 || oldstate == 3 )
	{
