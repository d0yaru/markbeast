CMD:flowers(playerid,params[])
{
	new string[300];
	if(PlayerInfo[playerid][pJailTime] > 0) return SendClientMessage(playerid, COLOR_GREY, "На время пребывания в тюрьме нельзя подарить цветы!");
	if(sscanf(params,"u",params[0])) return SendClientMessage(playerid, COLOR_PROJECT, "Используйте: {FFFFFF}/flowers [id]");
	new weapon = GetPlayerWeapon(playerid);
	new ammo = GetPlayerAmmo(playerid);
	if(weapon != 14) return SendClientMessage(playerid, COLOR_GREY, "У тебя в руках нет цветов!");
	if(PlayerInfo[playerid][pGuns][GetWeaponSlot(weapon)] != weapon)return false;
	if(IsPlayerConnected(params[0]))
	{
		if(playerid == params[0]) return SendClientMessage(playerid, COLOR_GREEN, "Вы не можете передать цветы сами себе!");
		if(!ProxDetectorS(3.0,playerid,params[0])) return SendClientMessage(playerid,COLOR_GREY,"Вы далеко от игрока");
		format(string,sizeof(string),"Подарил цветы %s",PN(params[0]));
		SetPlayerChatBubble(playerid, string, COLOR_PURPLE, 30.0, 5000);
		_GiveGun(params[0],weapon,ammo);

		new rguns[13],sguns[13];
		for(new i;i<13;i++)
		{
			rguns[i] = PlayerInfo[playerid][pAmmos][i];
			sguns[i] = PlayerInfo[playerid][pGuns][i];
		}
		_ResetGun(playerid);

		for(new x; x < 13; x ++)
		{
			if(sguns[x]!=14) 	_GiveGun(playerid,sguns[x],rguns[x]);
		}

		format(string,sizeof(string),"{FFFFFF}Вы успешно подарили цветы игроку %s.",PN(params[0]));
		SendClientMessage(playerid,COLOR_PROJECT,string);
		format(string,sizeof(string),"{FFFFFF}Игрок %s[%d] подарил вам цветы!",PN(playerid),playerid);
		SendClientMessage(params[0],COLOR_PROJECT,string);

		new Float:angle;
		GetPlayerFacingAngle(playerid, angle);
		J_SetPlayerFacingAngle(params[0], angle + 180);

		ApplyAnimation(playerid,"CRIB","CRIB_Use_Switch",4.0,0,0,0,0,0);
		ApplyAnimation(params[0],"CRIB","CRIB_Use_Switch",4.0,0,0,0,0,0);

	}
	return true;
}
