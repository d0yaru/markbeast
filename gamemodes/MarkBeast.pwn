Fix_SetPlayerVirtualWorld(playerid, world, bool:freeze = true)
{
	if(freeze)
	{
		NoEnterPickup[playerid] =2;
		TogglePlayerControllableEx(playerid,false);
	}

	pTemp[playerid][IntAndWorld][0] = world;
	if(pTemp[playerid][TimerIntAndWorld] == -1 && IsPlayerLogged[playerid])
	{
		pTemp[playerid][TimerIntAndWorld] = SetPlayerTimer(playerid,"CheckTime",250,0);
	}
	return SetPlayerVirtualWorld(playerid, world);
}
Fix_SetPlayerInterior(playerid, inter, bool:freeze = true)
{
	if(freeze)
	{
		NoEnterPickup[playerid] =2;
		TogglePlayerControllableEx(playerid,false);
	}

	pTemp[playerid][IntAndWorld][1] = inter;
	if(pTemp[playerid][TimerIntAndWorld] == -1 && IsPlayerLogged[playerid])
	{
		pTemp[playerid][TimerIntAndWorld] = SetPlayerTimer(playerid,"CheckTime",250,0);
	}
	return SetPlayerInterior(playerid, inter);
}
