	SendClientMessage(playerid,COLOR_WHITE,string);
	SendTeamMessage(gang,-1,"Ваша банда захватила новую территорию");

	format(string, 400, "UPDATE `zonesinfo` SET `GangID` = '%i', `Min_X` = '%.4f', `Min_Y` = '%.4f', `Max_X` = '%.4f', `Max_Y` = '%.4f' WHERE `ID` = '%i'",
	GZInfo[gang][gzFracID],
	GZInfo[zone][gzMin_X],
	GZInfo[zone][gzMin_Y],
	GZInfo[zone][gzMax_X],
	GZInfo[zone][gzMax_Y],zone + 1);
	UpdateDorm(gang);
	UpdateDorm(lastgang);
	SaveGangZone(zone);
	return 1;
}
