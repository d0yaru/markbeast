CheckEnterCar(playerid, vehicleid)
{
	if(VehicleInfo[vehicleid][vJob] != 0)
	{
		/*	new hour,minuite,second;
		gettime(hour,minuite,second);
		if(IsAtFrakcia(playerid, 1) && (hour > 10 && hour < 20 ))
		{
