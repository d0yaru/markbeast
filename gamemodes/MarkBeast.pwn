				    format(string, sizeof(string), "%s[%d] Возможно раскидывает машины! (Ехал без водителя | Дистанция: %.1f) Код 108", Name(playerid),playerid,fDistance);
					SendAdminsMessage(COLOR_LIGHTRED,string);
				}
				J_SetVehiclePos(vehicleid,VehicleInfo[vehicleid][vPos_X],VehicleInfo[vehicleid][vPos_Y],VehicleInfo[vehicleid][vPos_Z]);
	            return 0;
	        }
	        else if(vel_x > 0.3 || vel_y > 0.3 || vel_z > 0.3)
	        {
	            J_SetPlayerPos(playerid, new_x, new_y, new_z+2.5);
	            ApplyAnimation(playerid, "ped", "BIKE_fallR", 4.0, 0, 1, 0, 0, 0,0);
	        }
	    }
	    else
	    {
	        if(GetVehicleDistanceFromPoint(vehicleid, new_x, new_y, new_z) > 7)
	        {
	        	J_SetVehiclePos(vehicleid,VehicleInfo[vehicleid][vPos_X],VehicleInfo[vehicleid][vPos_Y],VehicleInfo[vehicleid][vPos_Z]);

	            return false;
	        }
	    }

	    new Float:VehPos[3];
		GetVehiclePos(vehicleid,VehPos[0],VehPos[1],VehPos[2]);
	    VehicleInfo[vehicleid][vPos_X] = VehPos[0];
		VehicleInfo[vehicleid][vPos_Y] = VehPos[1];
		VehicleInfo[vehicleid][vPos_Z] = VehPos[2];
	}
	return 1;
}
