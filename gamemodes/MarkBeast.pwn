					DestroyObject(Mysor_obj[playerid][1]);
					DestroyObject(Mysor_obj[playerid][2]);
					DestroyObject(Mysor_obj[playerid][3]);*/
					pTemp[playerid][Anti_Pvar][1] = 0;
					DisablePlayerRaceCheckpoint( playerid );
				}
			}
			if(!IsAPlane(vehicleid))
			{
				if(IsABike(vehicleid))
				{
					if(IsPlayerAttachedObjectSlotUsed(playerid, 0)) RemovePlayerAttachedObject(playerid, 0);
					if(PlayerInfo[playerid][pSlotItem][0]!=0) GiveItem(playerid,PlayerInfo[playerid][pSlotItem][0]);
				}
				if(!IsAVelik(vehicleid))
				{
					if(PlayerInfo[playerid][pGold]>0)
					{
						if(Speedometr[playerid])
						{
							Speedometr[playerid] = false;
							for(new i; i<7; i++)PlayerTextDrawDestroy(playerid, speeddraw[playerid][i]);
							for(new i; i<4; i++)TextDrawHideForPlayer(playerid, speedbox[i]);
						}

					}
					if(FakeDrunkLevel[playerid] > 0)
					{
						SetPlayerDrunkLevel(playerid,0);
						FakeDrunkLevelTime[playerid] = 0;
						FakeDrunkLevel[playerid] = 0;
					}
				}
			}
			if(vehicleid != INVALID_VEHICLE_ID && GetVehicleSeat(vehicleid, seat) == playerid) VehicleSeat[vehicleid][seat] = INVALID_PLAYER_ID;
			EnterVehicle[playerid] = INVALID_VEHICLE_ID;
			if(CarTest[playerid] != -1 && VehicleInfo[vehicleid][vTeam] == TEAM_LICENSERS)
			{
				new i = CarTest[playerid];
				if(GetVehicleModel(vehicleid)==512)
				{
					TogglePlayerDynamicRaceCP(playerid, FTest_CP[i], false);
					SendClientMessage(playerid,-1,"Вы провалили тест т.к вышли из самолёта.");
				}
				else
				{
					TogglePlayerDynamicRaceCP(playerid, Test_CP[i], false);
					SendClientMessage(playerid,-1,"Вы провалили тест т.к вышли из авто.");
				}
				CarTest[playerid] = -1;
			}

			if(IsATaxi(vehicleid))
			{
				if(!PlayerSeat[playerid])
				{
					if(PlayerInfo[playerid][pJob] == JOB_TAXI)
					{
						if(TaxiTarif[playerid] > 0)
						{
							pTemp[playerid][Anti_Pvar][5] = vehicleid;
							SetPlayerTimer(playerid,"DisableTarif",2500,0);
						}
					}
				}
				else if(PlayerSeat[playerid] > 0)
				{
					new id = GetVehicleSeat(vehicleid, 0);
					if(id != INVALID_PLAYER_ID)
					{
