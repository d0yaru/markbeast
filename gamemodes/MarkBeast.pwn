CMD:sellgangzone(playerid, params[])
{
	new zone = GetPlayerGangZone(playerid);
	new giveplayerid, count, string[100];
	if(sscanf(params, "ud", giveplayerid, count)) return SendClientMessage(playerid, COLOR_PROJECT, "Используйте: {FFFFFF}/sellgangzone [ID/Name] [Цена]");
	if(!ProxDetectorS(2.0, playerid, giveplayerid, 1)) return SendClientMessage(playerid,COLOR_RED,"Вы далеко от игрока!");
	if(!IsAtGang(playerid) || !IsAtGang(giveplayerid))return SendClientMessage(playerid, COLOR_GREY, "Либо вы либо тот кому вы хотите продать зону , не состоит в банде!");
	if(GZInfo[zone][gzFracID] != PlayerInfo[playerid][pMember]) return SendClientMessage(playerid, COLOR_GREY, "Это не ваша зона");
	if(giveplayerid == playerid) return SendClientMessage(playerid, COLOR_GREY, "Вы не можете продать самому себе!");
	if(PlayerInfo[playerid][pLeader]==0 || PlayerInfo[giveplayerid][pLeader]==0) return SendClientMessage(playerid, COLOR_GREY, "Только лидер лидеру может продавать зоны!");
	if(CaptureZone == zone) return SendClientMessage(playerid, COLOR_GREY, "Эту зону уже захватывают");
	if(count<0)return SendClientMessage(playerid,COLOR_RED,"Вы указываете отрицательную сумму? серьезно? вы хотите ему заплатить?");
	if(PlayerInfo[giveplayerid][pMoney]< count)return SendClientMessage(playerid,COLOR_RED,"У этого бандита недостаточно денег чтобы оплатить!");
	SellGzone[giveplayerid] = count;
	SellPGzone[giveplayerid] = playerid;
	SendClientMessage(playerid,COLOR_GREY,"Вы отправили предложение о покупке територии!");
	format(string,sizeof(string),"Игрок %s предлагает купить у него територию за %d$\nВы согласны?",PN(playerid),count);
	ShowPlayerDialogEx(giveplayerid,377,0,"Информация",string,"Согласен","Отклонить");
	return 1;
}
