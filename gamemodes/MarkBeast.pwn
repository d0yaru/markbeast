CMD:sban(playerid, params[])
{
	if(PlayerInfo[playerid][pAdmin] < 4)return false;
	if(ADuty[playerid]!=228) return cmd_apanel(playerid,"");
	if(pTemp[playerid][Anti_Pvar][30]>CurrentTimer)
	{
		SendClientMessage(playerid,COLOR_RED,"Можно раз в 10 секунд!");
		pTemp[playerid][Anti_Pvar][33] = CurrentTimer+3;
		return false;
	}
	new id, string[150], days, reason[200];
	if(sscanf(params,"uds[150]", id, days, reason)) return SendClientMessage(playerid, COLOR_PROJECT, "Используйте:{FFFFFF} /sban [id] [days 1-30] [Причина]");
	if(!IsPlayerConnected(id)) return SendClientMessage(playerid, -1, "Игрок не в сети");
