CMD:invite(playerid, params[])
{
	if(sscanf(params,"u",params[0])) return SendClientMessage(playerid,COLOR_PROJECT,"Используйте: {FFFFFF}/invite [id]");
	if(!IsPlayerConnected(params[0]))  return SendClientMessage(playerid,-1 ,"Этого игрока нет в сети!");
	if(CuffedTime[params[0]]>0)return SendClientMessage(playerid,COLOR_RED,"Игрок находится в шоковом состоянии!");
	if(PlayerInfo[playerid][pLeader] == 0 && PlayerInfo[playerid][pRank] < 9) return SendClientMessage(playerid,COLOR_RED,"Отказано в доступе!");
	if(PlayerInfo[params[0]][pMember] > 0) return SendClientMessage(playerid, COLOR_GREY, "Игрок состоит в другой организации!");
	if(!IsPlayerLogged[params[0]])return SendClientMessage(playerid, COLOR_GREY, "Игрок не вошел вигру");
	//if(PlayerInfo[playerid][pMember]==TEAM_FBI)return SendClientMessage(playerid,COLOR_RED,"В FBI можно попасть только переводом из полиции!");
	if(IsAtFrakcia(playerid) && PlayerInfo[params[0]][pBL]!=0)
	{
		SendClientMessage(playerid,COLOR_GREY,"Вы не можете принять этого игрока. Возможные причины:");
		SendClientMessage(playerid,COLOR_GREY,"- Этот игрок находится в чёрном списке закона");
		return true;
	}
	if(Clother[params[0]]!=-1)return 0;
	if(GetPlayerVirtualWorld(playerid)!=GetPlayerVirtualWorld(params[0]))return 0;
	if(!ProxDetectorS(5.0, playerid, params[0])) return SendClientMessage(playerid,COLOR_GREY,"Вы должны находиться рядом с игроком");
	if(InviteOffer[params[0]] != INVALID_PLAYER_ID) return SendClientMessage(playerid,COLOR_GREY,"Этому игроку уже было выслано приглашение на вступление в организацию");
	new Leader[15];
	CMDstr = "";
	if(PlayerInfo[params[0]][pWarns] > 0) return SendClientMessage(playerid,-1,"Игрока с варном нельзя принять в организацию!");
	if(IsAtFrakcia(playerid, 1) && PlayerInfo[params[0]][pZKP] <11)
	{
		SendClientMessage(playerid,-1,"Законопослушность этого гражданина не позволяет ему вступить в организацию! (Необходимо 11 законопослушности!)");
		SendClientMessage(params[0],-1,"Вас не смогли принять в организацию, тк. у вас слишком мало законопослушности! (Необходимо 11 законопослушности!)");
		return false;
	}
	if(PlayerInfo[playerid][pLeader] > 0) Leader = "Лидер";
	else Leader = "Зам. лидера";
	InviteOffer[params[0]] = playerid;
	format(CMDstr,100,"Вы приглашаете игрока %s к себе в организацию",Name(params[0]));
	SendClientMessage(playerid,COLOR_LIGHTBLUE,CMDstr);
	format(CMDstr,100,"%s %s приглашает вас к себе в организацию %s",Leader, Name(playerid),OrgInfo[GetTeamID(playerid)][oName]);
	ShowPlayerDialogEx(params[0],8,0,"Инвайт",CMDstr,"Принять","Отмена");

	return true;
}
