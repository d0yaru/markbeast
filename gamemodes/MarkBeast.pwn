GetClosestPlayToObject(Float:object_x, Float:object_y, Float:object_z, &Float:DistantionToHem)
{
	new Float:min_dis = 99999.99,
Float:player_dis,
	player = INVALID_PLAYER_ID;
	foreach (playerid)
	{
		if (GetPlayerState(playerid) == PLAYER_STATE_SPECTATING) continue;
		if(!HellsGateOpen)
		{
			if (GetPlayerVirtualWorld(playerid) != 1229) continue; // Обрати внимание какой вид имеют стандартные функции!
			if (!IsPlayerInRangeOfPoint(playerid, 450.0, -652.2886,-2120.5735,1608.5018)) continue;
		}
		else
		{
		    if(PlayerInfo[playerid][pHeal] > 0) continue;
		    if (IsPlayerInRangeOfPoint(playerid, 150.0, -402.2298,2228.3242,42.4258)) continue;
		    for(new i;i<sizeof(antidm);i++)
			{
				if(IsPlayerInRangeOfPoint(playerid, 100.0, antidm[i][0],antidm[i][1],antidm[i][2]))
				{
					continue;
				}
			}
		}
		if (IsPlayerDeath[playerid] == true || PlayerAFKTime[playerid][0] > 0 ) continue; // А где сами функции и почему имена у них разные? И предыдущий комментарий сюда применим.
		if(Zombie_Adapted[playerid] != 0) continue;
		
		if(IsPlayerInRangeOfPoint(playerid, 30.0, -899.2206,-2090.9507,1610.0308)) continue;

		player_dis = GetPlayerDistanceFromPoint(playerid, object_x, object_y, object_z);
		if (player_dis < min_dis)
		{
			/*if(player_dis < StreamDistance)
			{
				new Float:Pos[3];
				GetPlayerPos(playerid,Pos[0],Pos[1],Pos[2]);
				if(CA_RayCastLine(object_x, object_y, object_z+0.75, Pos[0],Pos[1],Pos[2]+0.75, Pos[0],Pos[1],Pos[2]))continue;
			}*/
			min_dis = player_dis;
			player = playerid;
			DistantionToHem = player_dis;
		}
	}
	return player;
}
