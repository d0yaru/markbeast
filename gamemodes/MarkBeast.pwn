CMD:hi(playerid, params[])
{
	if(!IsPlayerInAnyVehicle(playerid))
	{
		new giveplayerid;
		if(!sscanf(params,"d",giveplayerid))
		{
			if(giveplayerid == playerid)  return  SendClientMessage(playerid,COLOR_GREY,"Вы указали свой ID");
