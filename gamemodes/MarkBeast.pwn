ProxDetector(Float:radi, playerid, string[], color, house = 0)
{
	if(IsPlayerConnected(playerid))
	{
		for(new i;i < strlen(string);i ++)
		{
			if(string[i] == '%') string[i] = '#';
		}
		new Float:X, Float:Y, Float:Z;
		GetPlayerPos(playerid, X, Y, Z);
		new NEAR_HOUSE = InfoHouse[playerid];
		new IN_HOUSE = HouseEntered[playerid];
		new HOUSE = -1;
		if(NEAR_HOUSE != -1 && IsPlayerInDynamicCP(playerid, HouseCP[NEAR_HOUSE])) HOUSE = NEAR_HOUSE;
		else if(IN_HOUSE != -1 && IsPlayerInRangeOfPoint(playerid, 3.0, HouseInfo[IN_HOUSE][hExit_X], HouseInfo[IN_HOUSE][hExit_Y], HouseInfo[IN_HOUSE][hExit_Z])) HOUSE = IN_HOUSE;

		new virtualworld = GetPlayerVirtualWorld(playerid);
		foreach(i)
		{
			if(!IsPlayerLogged[playerid]) continue;
			if(house && HOUSE != -1)
			{
				if(IsPlayerInRangeOfPoint(i, 4.0, HouseInfo[HOUSE][hExit_X], HouseInfo[HOUSE][hExit_Y], HouseInfo[HOUSE][hExit_Z])
						|| IsPlayerInRangeOfPoint(i, 4.0, HouseInfo[HOUSE][hEnter_X], HouseInfo[HOUSE][hEnter_Y], HouseInfo[HOUSE][hEnter_Z])) SendClientMessage(i, color, string);
			}
			else
			{
				if(virtualworld != GetPlayerVirtualWorld(i)) continue;
				if (IsPlayerInRangeOfPoint(i,radi,X,Y,Z)) SendClientMessage(i, color, string);
			}
		}
	}
	return true;
}
void Fillup(playerid, carid, Float:gas)
{
	if(IsPlayerConnected(playerid))
	{
		if(IsPlayerInVehicle(playerid,carid))
		{
			new string[100];
			if(IsAtGasStation(playerid))
			{
				Fuel[carid] += gas;
				BeforeFill[carid] += gas;
				if(BeforeFill[carid] >= gas || Fuel[carid] >= 100.0)
				{
					if(Fuel[carid] >= 100.0) Fuel[carid] = 100.0;
					TogglePlayerControllableEx(playerid, true);
					format(string,sizeof(string),"Ваш транспорт заправлен {FFFFFF}%.1f{1578af} литрами топлива.",gas);
					SendClientMessage(playerid,COLOR_ORANGE,string);
					cmd_engine(playerid,"");
				}
				else SetTimerEx("Fillup",200,false,"idd",playerid,carid,gas);
			}
		}
		else
		{
			if(IsAtGasStationC(carid))Fuel[carid] = gas;
		}
	}
	return true;
}
