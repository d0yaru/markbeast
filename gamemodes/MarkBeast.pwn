public OnPlayerEnterDynamicRaceCP(playerid, checkpointid)
{
	//	new string[300];
	if(!VehTest[playerid])
	{
		new test = CarTest[playerid];
		if(test != -1 && checkpointid == FTest_CP[test])
		{
			TogglePlayerDynamicRaceCP(playerid,FTest_CP[test],false);
			if( (test+1) == sizeof(FTest_CP) )
			{
				SendClientMessage(playerid,-1,"Вы успешно сдали практику на право летать на самолёте. Держите ваше удостоверения.");
				PlayerInfo[playerid][pFlyLic] = YES_LICENSE;
				ExitCar(playerid);
				VehicleInfo[GetPlayerVehicleID(playerid)][vPlayer] = -1;
				SetVehicleToRespawn(GetPlayerVehicleID(playerid));
				CarTest[playerid] = -1;
			}
			else
			{
				CarTest[playerid] ++;
				new i = CarTest[playerid];
				TogglePlayerDynamicRaceCP(playerid,FTest_CP[i],true);
			}
		}
	}
	else
	{
		new test = CarTest[playerid];
		if(test != -1 && checkpointid == Test_CP[test])
		{
			TogglePlayerDynamicRaceCP(playerid,Test_CP[test],false);
			if( (test+1) >= sizeof(Test_CP) )
			{
				new vehid = GetPlayerVehicleID(playerid);
				new Float:healthOfCar;
				GetVehicleHealth(vehid,healthOfCar);


				ExitCar(playerid);

				SetVehicleToRespawn(vehid);
				CarTest[playerid] = -1;

				if(healthOfCar < 950)
				{
					SendClientMessage(playerid,-1,"Вы проволили тест так как сильно побили машину!");
					return 1;
				}
				SendClientMessage(playerid,-1,"Вы успешно сдали тест на вождение! Держите своё вод.удостоверение.");
				PlayerInfo[playerid][pCarLic] = YES_LICENSE;
			}
			else
			{
				CarTest[playerid] ++;
				new i = CarTest[playerid];
				TogglePlayerDynamicRaceCP(playerid,Test_CP[i],true);
			}
		}
	}
	return true;
}
