		if(PlayerInfo[params[0]][pBL]) return SendClientMessage(playerid,COLOR_GREY,"Игрок уже в черном списке организации");
		if(params[0]==playerid)return false;
		PlayerInfo[params[0]][pBL] = 1;
		CMDstr = "";
		format(CMDstr,115,"Вы занесли %s(%i) в чёрный список закона. Причина: %s", Name(params[0]), params[0], params[1]);
