			ShowPlayerDialogEx(playerid,0,0,"Этот аккаунт заблокирован!",string,"Ок","");
			J_Kick(playerid);
			return true;
		}else{
			format(string, sizeof(string), "UPDATE `usrboston` SET `Warns`='0' WHERE `NickName` = '%s' LIMIT 1", PN(playerid));
			mysql_tquery(mysql, string);
			format(string, sizeof(string), "DELETE FROM `bannames` WHERE `Name` = '%s' LIMIT 1", PN(playerid));
			mysql_tquery(mysql, string);
		}
	}
	VehicleID[playerid]=INVALID_VEHICLE_ID;

	format(string, sizeof(string), "SELECT `ID` FROM `usrboston` WHERE `NickName` = '%s' LIMIT 1", PN(playerid));
	mysql_tquery(mysql, string, "MysqlLoadingIDAccount", "d", playerid);
