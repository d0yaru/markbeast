//В таймер
WFN_OnPlayerUpdate(playerid)
{
	if(WFN_Showed[playerid] && WFN_Rate[playerid] == 0)
	{
		if(WFN_ObjectRate[playerid] == -1)	return 1;

		new Float: p_pos[3];

		new wheel_id = WFN_GetWheelID(playerid);
		if(wheel_id == -1)
		{
			return 0;
		}

		GetDynamicObjectPos(WFN_ObjectRate[playerid], p_pos[0], p_pos[1], p_pos[2]);

		new keys, updown, leftright;
		GetPlayerKeys(playerid, keys, updown, leftright);

		if(updown > 0)
		{
			if(WFN_RatePos[playerid][0] == 0)
			{
				WFN_RatePos[playerid][0] = 1;

				p_pos[0] = p_pos[0] - (0.3522 * floatsin(-WFN_Info[wheel_id][whRot], degrees));
				p_pos[1] = p_pos[1] - (0.3522 * floatcos(-WFN_Info[wheel_id][whRot], degrees));

				SetDynamicObjectPos(WFN_ObjectRate[playerid], p_pos[0], p_pos[1], p_pos[2]);
				SetPlayerCameraLookAt(playerid,  p_pos[0], p_pos[1], p_pos[2]);
			}
		}
		else if(updown < 0)
		{
			if(WFN_RatePos[playerid][0] == 1)
			{
				WFN_RatePos[playerid][0] = 0;

				p_pos[0] = p_pos[0] + (0.3522 * floatsin(-WFN_Info[wheel_id][whRot], degrees));
				p_pos[1] = p_pos[1] + (0.3522 * floatcos(-WFN_Info[wheel_id][whRot], degrees));

				SetDynamicObjectPos(WFN_ObjectRate[playerid], p_pos[0], p_pos[1], p_pos[2]);
				SetPlayerCameraLookAt(playerid,  p_pos[0], p_pos[1], p_pos[2]);
			}
		}

		if(leftright > 0)
		{
			if(WFN_RatePos[playerid][1] < 2)
			{
				WFN_RatePos[playerid][1]++;

				p_pos[0] = p_pos[0] + (0.4212 * floatsin(-WFN_Info[wheel_id][whRot] + 90, degrees));
				p_pos[1] = p_pos[1] + (0.4212 * floatcos(-WFN_Info[wheel_id][whRot] + 90, degrees));

				SetDynamicObjectPos(WFN_ObjectRate[playerid], p_pos[0], p_pos[1], p_pos[2]);
				SetPlayerCameraLookAt(playerid,  p_pos[0], p_pos[1], p_pos[2]);
			}
		}
		else if(leftright < 0)
		{
			if(WFN_RatePos[playerid][1] > 0)
			{
				WFN_RatePos[playerid][1]--;

				p_pos[0] = p_pos[0] - (0.4212 * floatsin(-WFN_Info[wheel_id][whRot] + 90, degrees));
				p_pos[1] = p_pos[1] - (0.4212 * floatcos(-WFN_Info[wheel_id][whRot] + 90, degrees));

				SetDynamicObjectPos(WFN_ObjectRate[playerid], p_pos[0], p_pos[1], p_pos[2]);
				SetPlayerCameraLookAt(playerid,  p_pos[0], p_pos[1], p_pos[2]);
			}
		}
	}
	return 1;
}
