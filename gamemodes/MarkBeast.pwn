CMD:givemats(playerid, params[])
{
	if(!IsPlayerConnected(playerid)) return SendClientMessage(playerid, COLOR_GREY, "Не верный id");
	new giveplayerid, count, string[100];
	if(sscanf(params, "ud", giveplayerid, count)) return SendClientMessage(playerid, COLOR_PROJECT, "Используйте: {FFFFFF}/givemats [ID/Name] [Кол-во]");
	if(!IsPlayerConnected(giveplayerid) || !IsPlayerLogged[giveplayerid]) return false;
	if(!ProxDetectorS(3.0, giveplayerid, playerid)) return SendClientMessage(playerid, COLOR_GREY, "Вы далеко от игрока!");
	if(count < 1 || count > PlayerInfo[playerid][pMats]) return SendClientMessage(playerid, COLOR_GREY, "У вас нет столько патронов");
	if((PlayerInfo[giveplayerid][pMats] + count) > 1000) return SendClientMessage(playerid, COLOR_GREY, "Этот игрок не сможет унести с собой больше 1000 патронов");
