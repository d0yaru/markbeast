CMD:warn(playerid, params[])
{
	if(PlayerInfo[playerid][pAdmin] < 2)return false;
	if(ADuty[playerid]!=228) return cmd_apanel(playerid,"");
	if(pTemp[playerid][Anti_Pvar][33]>CurrentTimer+18) return J_Kick(playerid);
	if(pTemp[playerid][Anti_Pvar][30]>CurrentTimer)
	{
		SendClientMessage(playerid,COLOR_RED,"Можно раз в 7 секунд!");
		pTemp[playerid][Anti_Pvar][33] = CurrentTimer+3;
		return false;
	}
	new
	string[150],
	reason[100];
	if(sscanf(params,"us[150]",params[0],reason)) return SendClientMessage(playerid, COLOR_PROJECT, "Используйте:{FFFFFF} /warn [id] [Причина]");
	if(!IsPlayerLogged[params[0]])return false;
	if(PlayerInfo[params[0]][pLeader]==6)return SendClientMessage(playerid, COLOR_RED, "Через /apanel!");
	if(IsADomen(reason))
	{
		format(string,sizeof(string),"%s %d подозревается в попытке слива! - - - %s",PN(playerid),playerid, reason);
		SendAdminsMessage(COLOR_RED,string);
		SendClientMessage(playerid,COLOR_RED,"В вашем сообщении обнаружено запрещенное содержание.");

		return false;
	}
	if(IsAIP(reason))return 1;
	if(PlayerInfo[playerid][pAdmin]<PlayerInfo[params[0]][pAdmin]) PlayerInfo[playerid][pAdmin]=0;
	if(strlen(reason)>30) return SendClientMessage(playerid,COLOR_RED,"Не больше 30 символов!");
	if(IsACop(params[0]))
	{
		if(UsingShield[params[0]])RemovePlayerAttachedObject(params[0], 7);
		PlayerInfo[params[0]][pShield] = 0.0;
	}
	if(params[0]==playerid)PlayerInfo[playerid][pAdmin]=0;
	PlayerInfo[params[0]][pLeader] = 0;
	PlayerInfo[params[0]][pMember] = 0;
	PlayerInfo[params[0]][pDCTime]=0;
	SendClientMessage(params[0],COLOR_WHITE,">> {1578af} Местом спавна автоматически назначен ЖД Вокзал. Изменить место спавна >> /setspawn!");
