	new string[500];
	new allzone = 0;
	new urik[30];

	if(OrgInfo[dormid][oOb4ak] == 1) urik="{A92424}Закрыт{FFFFFF}";
	if(OrgInfo[dormid][oOb4ak] == 0) urik = "{3FA924}Открыт{FFFFFF}";

	if((dormid>=11 && dormid<=15) || dormid == 25)
	{
		for(new d = 1; d < GANGZONES; d++)
