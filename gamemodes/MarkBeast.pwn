PlayerUnleader(playerid = INVALID_PLAYER_ID, Name[] = "None")
{
	new string[144];
	if(playerid != INVALID_PLAYER_ID)
	{
		if(IsACop(playerid))
		{
			if(UsingShield[playerid])RemovePlayerAttachedObject(playerid, 7);
			PlayerInfo[playerid][pShield] = 0.0;
		}
		PlayerInfo[playerid][pLeader] = 0;
		PlayerInfo[playerid][pMember] = 0;
		PlayerInfo[playerid][pDCTime]=0;
		SendClientMessage(playerid,COLOR_WHITE,">> {1578af} Местом спавна автоматически назначен ЖД Вокзал. Изменить место спавна >> /setspawn!");
		PlayerInfo[playerid][pRank] = 0;
		SetBumSkin(playerid);
		PlayerInfo[playerid][pJob] = 0;
		J_SetPlayerColor(playerid,TeamColors[0]);
		ResetSpawnInfo(playerid, 0);
		SaveAccount(playerid);
		SendClientMessage(playerid,COLOR_LIGHTBLUE,"Вы были сняты с поста лидера");
	}
	else
	{
		format(string, sizeof(string), "UPDATE `usrboston` SET `Leader` = '0',`Member` = '0', `Job` = '0',`Rank` = '0' WHERE `NickName` = '%s'",Name);//,`Skin` = '0'
		mysql_tquery(mysql, string);
	}
	return true;
}
