CMD:dropgun(playerid, param[])
{
	if(IsPlayerInAnyVehicle(playerid)) return false;
	new weapon = GetPlayerWeapon(playerid);
	new ammo = GetPlayerAmmo(playerid);
	
	if(ammo<1)return false;
	if(weapon < 1) return SendClientMessage(playerid, COLOR_GREY, "Оружие должно быть в руках");
	if(PlayerInfo[playerid][pGuns][GetWeaponSlot(weapon)] != weapon)return false;
	
	new slot = GetWeaponSlot(weapon);
	if(PlayerInfo[playerid][pAmmos][slot]<ammo)return false;
	new Float:x, Float:y, Float:z,Float:eagle;
	GetPlayerPos(playerid, x, y, z);
	GetPlayerFAngle(playerid,eagle);
	if(NotGoodGun(weapon)) return SendClientMessage(playerid, COLOR_WHITE, "Вы не можете выбросить это оружие.");
	new idgun = GunObjects[weapon];
	new idobject=-1;
	for(new i;i<MAX_DGUNS;i++)
	{
		if(dguns[i]==-1)
		{
			idobject=i;
			break;
		}
	}
	if(idobject==-1)
	{
		new min1=CurrentTimer;
		for(new i;i<MAX_DGUNS;i++)
		{
			if(dguntime[i]<min1)
			{
				min1=dguntime[i];
				idobject=i;
			}
		}
		DestroyDynamicObject(dguns[idobject]);
		DestroyDynamic3DTextLabel(DgunText[idobject]);
	}
	dguntime[idobject]=CurrentTimer;
	dpatrons[idobject]=ammo;
	dgunn[idobject]=weapon;
	dguns[idobject] = CreateDynamicObject(idgun, x, y, z-0.97, 96, 110, eagle, GetPlayerVirtualWorld(playerid));
	_GiveGun(playerid,weapon,-ammo);
	PlayerInfo[playerid][pGuns][slot] = 0;
	ApplyAnimation(playerid, "BOMBER","BOM_Plant_Crouch_In", 4.0, 0, 0, 0, 0, 0, 1);

	new buffer[100];

	format(buffer, sizeof(buffer), "Вы выбросили '%s'", GunNames[weapon]);
	SendClientMessage(playerid, COLOR_WHITE, buffer);

	format(buffer, sizeof(buffer), "выбросил(а) '%s'", GunNames[weapon]);
	SendPlayerMe(playerid,buffer);

	format(buffer, sizeof(buffer), "Чтобы подобрать %s введите "COLOR_PROJECT_EX"/pgun{FFFFFF} либо нажмите "COLOR_PROJECT_EX"ALT", GunNames[weapon]);
	DgunText[idobject] =CreateDynamic3DTextLabel(buffer, -1, x, y, z-0.971,15.0);

	return 1;
}
