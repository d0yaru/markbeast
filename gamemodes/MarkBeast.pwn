		{
			tima = "час(ов)";
			dima = floatround((seconds-times)/60/60,floatround_ceil);
		}
		else
		{
			tima = "минут";
			dima = floatround((seconds-times)/60,floatround_ceil);
		}
		if(times<seconds)
		{
			format(string,sizeof(string),"\
