	if(PlayerInfo[playerid][pFamRank] <= params[1]) return SendClientMessage(playerid, COLOR_GREY, "Вы не можете повысить/понизить игрока до вашего или выше ранга");
	if(params[1] < 1 || params[1] > 3) return SendClientMessage(playerid,COLOR_GREY,"Нельзя повысть/понизить до этого ранга");

	if(Clother[params[0]]!=-1)return 0;
	if(GetPlayerVirtualWorld(playerid)!=GetPlayerVirtualWorld(params[0]))return 0;

	if(!IsPlayerConnected(params[0])) return SendClientMessage(playerid, COLOR_GREY, "Игрок не найден");

	CMDstr = "";
	format(CMDstr,100,"%s %s до %d ранга",Name(playerid),PlayerInfo[params[0]][pFamRank] > params[1] ? ("понизил"):("повысил"),params[1]);
	SendClientMessage(params[0],COLOR_LIGHTBLUE,CMDstr);
	format(CMDstr,100,"Вы %sи игрока %s до %d ранга",PlayerInfo[params[0]][pFamRank] > params[1] ? ("понизил"):("повысил"),Name(params[0]),params[1]);
	SendClientMessage(playerid,COLOR_LIGHTBLUE,CMDstr);
	PlayerInfo[params[0]][pFamRank] = params[1];
	return true;
}
