public OnPlayerClickMap(playerid, Float:fX, Float:fY, Float:fZ)
{
	new string[100];
	if(Taxi_GO[playerid] == 1)
	{
		new carid = GetPlayerVehicleID(playerid);
		if(IsATaxi(carid))
		{
			new driverid = GetVehicleSeat(carid, 0);
			if(PlayerInfo[driverid][pJob] == JOB_TAXI)
			{
				if(TaxiTarif[driverid] > 0)
				{
					EnableGPSForPlayer(driverid,fX, fY, fZ);
					format(string, sizeof(string), "%s выбрал(а) пункт. Он отмечен на карте .", cPN(playerid));
					SendClientMessage(driverid, -1, string);
					SendClientMessage(playerid, COLOR_GREY, "Вы выбрали пункт назначения.");
					Taxi_GO[playerid] = 2;
					return true;
				}
			}
		}
	}
	if(PlayerInfo[playerid][pAdmin] > ADM_NONE)
	{
		if (GetPlayerState(playerid) == 2)
		{
			new tmpcar = GetPlayerVehicleID(playerid);
			J_SetVehiclePos(tmpcar, fX, fY, fZ+2);
		}
		else J_SetPlayerPos(playerid, fX, fY, fZ+1);
		Fix_SetPlayerVirtualWorld(playerid,0);
		Fix_SetPlayerInterior(playerid,0);
	}
	return true;
}
