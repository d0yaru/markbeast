CMD:tuisa(playerid, params[])
{
	if(IsPlayerConnected(playerid))
	{
		if(IsPlayerInAnyVehicle(playerid)) return SendClientMessage(playerid, COLOR_GREY, "Нельзя использовать в машине");
		new id,string[200];
		if(sscanf(params,"u",id)) return SendClientMessage(playerid,COLOR_PROJECT,"Используйте: {FFFFFF}/hi [id]");
		if(id==playerid)return false;
		if(IsPlayerConnected(id))
		{
			if(id != INVALID_PLAYER_ID)
			{
				new Float:X, Float:Y, Float:Z;
				GetPlayerPos(id, X, Y, Z);
				new Float:distance = GetPlayerDistanceFromPoint(playerid, X, Y, Z);
				if( distance < 1.5 && distance > 0.5 )
				{
					if(PlayerLie[id])return SendClientMessage(playerid, -1,"Повернитесь лицом к игроку");
					if(!GetPlayerFaceToPlayer(playerid, id, 25.0)) return SendClientMessage(playerid, -1,"Повернитесь лицом к игроку");
					if(!GetPlayerFaceToPlayer(id, playerid, 25.0)) return SendClientMessage(playerid, -1,"Этот игрок не повернут к вам лицом");
					new Float:angle;
					GetPlayerFacingAngle(playerid, angle);
					J_SetPlayerFacingAngle(id, angle + 180);
					format(string, sizeof(string), " %s пожал(а) руку %s", cPN(playerid),cPN(id));
					ProxDetector(25.0, playerid, string, COLOR_PURPLE);
					ApplyAnimation(playerid,"GANGS","hndshkfa",4.1, 0, 1, 1, 0, 0, 1);
					ApplyAnimation(id,"GANGS","hndshkfa",4.1, 0, 1, 1, 0, 0, 1);
					return true;
				}
				else if (distance < 20.0)
				{
					format(string,sizeof(string)," %s машет рукой %s",Name(playerid),Name(id));
					ProxDetector(25.0,playerid,string,COLOR_PURPLE);
					ApplyAnimation(playerid, "PED", "endchat_03", 4.1, 0, 1, 1, 0, 0, 1);
				}
				else SendClientMessage(playerid, -1,"Вы находитесь либо слишком близко от игрока, либо слишком далеко от игрока");
			}
		}
		else return SendClientMessage(playerid, -1,"Игрок не найден");
	}
	return true;
}
