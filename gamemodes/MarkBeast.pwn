CMD:giverank(playerid, params[])
{
	if(PlayerInfo[playerid][pMember]<=0)return false;
	if(PlayerInfo[playerid][pRank] < 9 && PlayerInfo[playerid][pLeader] == 0) return SendClientMessage(playerid,COLOR_RED,"Отказано в доступе!");
	if(sscanf(params,"ud",params[0],params[1])) return SendClientMessage(playerid,COLOR_PROJECT,"Используйте: {FFFFFF}/giverank [id] [rank]");
	if(!ProxDetectorS(8.0, playerid, params[0])) return SendClientMessage(playerid,COLOR_GREY,"Вы должны находиться рядом с игроком!");
	if(PlayerInfo[params[0]][pLeader] > 0 ) return SendClientMessage(playerid,COLOR_RED,"Вы не можете изменять ранг лидеру");
	if(params[1] < 1) return SendClientMessage(playerid,COLOR_GREY,"Нельзя повысть до этого ранга");

	if(Clother[params[0]]!=-1)return 0;
	if(GetPlayerVirtualWorld(playerid)!=GetPlayerVirtualWorld(params[0]))return 0;

	if(!IsPlayerConnected(params[0])) return SendClientMessage(playerid, COLOR_GREY, "Игрок не найден");
	if(PlayerInfo[playerid][pRank] <= PlayerInfo[params[0]][pRank]) return SendClientMessage(playerid, COLOR_GREY, "Вы не можете повысить/понизить этого игрока");
	if(PlayerInfo[playerid][pRank] == 9 && params[1] > 8) return SendClientMessage(playerid,COLOR_RED,"Нельзя выдать ранг больше 8");
	if(params[1] > 9) return SendClientMessage(playerid,COLOR_GREY,"Нельзя повысть до этого ранга");
	if(PlayerInfo[params[0]][pMember] != PlayerInfo[playerid][pMember]) return SendClientMessage(playerid, COLOR_GREY, "Игрок находиться в другой организации!");
	CMDstr = "";
	format(CMDstr,100,"Лидер %s %s до %d ранга",Name(playerid),PlayerInfo[params[0]][pRank] > params[1] ? ("понизил"):("повысил"),params[1]);
	SendClientMessage(params[0],COLOR_LIGHTBLUE,CMDstr);
	format(CMDstr,100,"Вы %sи игрока %s до %d ранга",PlayerInfo[params[0]][pRank] > params[1] ? ("понизил"):("повысил"),Name(params[0]),params[1]);
	SendClientMessage(playerid,COLOR_LIGHTBLUE,CMDstr);
	PlayerInfo[params[0]][pRank] = params[1];
	PlayerInfo[params[0]][pNeedMessage]=1;
	if(PlayerInfo[params[0]][pSex] == 2)
	{
		if(TeamDuty[params[0]])
		{
			SetPlayerSkinEx(params[0],GOrgSkins[GetTeamID(params[0])][params[1]]);
			TogglePlayerControllableEx(params[0],true);
		}
	}
	else
	{
		if(TeamDuty[params[0]])
		{
			SetPlayerSkinEx(params[0],MOrgSkins[GetTeamID(params[0])][params[1]]);
			TogglePlayerControllableEx(params[0],true);
		}
	}
	PlayerInfo[params[0]][pNeedMessage]=0;
	return true;
}
