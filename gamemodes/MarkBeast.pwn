OnZombieDeath(j,playerid)
{
	new i = 4;

	MostKills[playerid] ++; // плюс 1 килл

	if(PlayerInfo[playerid][pZombieQuest] > 0 && PlayerInfo[playerid][pZombieQuest] < 12)
	{
		PlayerInfo[playerid][pZombieQuest]++;
		if(PlayerInfo[playerid][pZombieQuest] >= 11)
		{
			PlayerInfo[playerid][pZombieQuest] = 11;
			SendClientMessage(playerid, -1, "Вы достали нужное количество ДНК, поспешите к Джону!");
		}
	}


	GameTextForPlayer(playerid, "+1 Kill", 2000, 3);

	TimeOut[j] = (30+random(61)); // отключаем на 15-45 секунд.
	StopObject(ObjectZombie[j][i]); // останавливаем

	foreach(player)
	{
		if(ID_Of_Sounder[player] == j)
		{
			//if(AudioStream[player] > CurrentTimer)
			//{
			StopMusic(player);
			//}
		}
	}

	new string[50],Float:ObjectPosition[3];
	new Rands = random(6);
	Rands = Rands +1;
	format(string,sizeof(string),"http://samp-x.ru/music/Death_%d.mp3",Rands);
	GetObjectPos(ObjectZombie[j][i],ObjectPosition[0],ObjectPosition[1],ObjectPosition[2]);
	PlayMusicForAllEx(string,ObjectPosition[0],ObjectPosition[1],ObjectPosition[2],15.99,2,MUSIC_ID_DEATH,j);


	ZombieNeedToBeSpawned[j] = 1; // указываем, что нужно заспавнить

	SetTimerEx("DestroyZombie",1000,0, "dd", j,i);

	return true;
