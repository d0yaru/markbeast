	if(id == playerid) return SendClientMessage(playerid,COLOR_GREY,"Себя нельзя изгнать.");

	PlayerInfo[id][pFam] = 0;
	PlayerInfo[id][pFamRank] = 0;

	format(string,sizeof(string),"{FFFFFF}%s выгнал вас из семьи.\nПричина: %s",Name(playerid), reason);
	SendClientMessage(id,COLOR_LIGHTBLUE,string);
	format(string,sizeof(string),"{FFFFFF}%s выгнал %s из семьи.\nПричина: %s",Name(playerid),Name(id), reason);
	SendFamMessage(PlayerInfo[playerid][pFam],COLOR_LIGHTBLUE,string);
	format(string,sizeof(string),"Вы выгнали %s. Причина: %s",Name(id), reason);
	SendClientMessage(playerid,COLOR_LIGHTBLUE,string);

	SaveAccount(id);
	if(FamLabel[id] != Text3D:INVALID_3DTEXT_ID)
	{
		DestroyDynamic3DTextLabel(FamLabel[id]);
		FamLabel[id] = Text3D:INVALID_3DTEXT_ID;
		return true;
	}
	return true;
}
