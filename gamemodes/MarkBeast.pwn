CMD:kiss(playerid,params[])
{
	new string[300];
	if(sscanf(params,"u",params[0])) return SendClientMessage(playerid, COLOR_PROJECT, "Используйте: {FFFFFF}/kiss [id]");
	if(IsPlayerInAnyVehicle(playerid) || IsPlayerInAnyVehicle(params[0]))return 1;
	if(animan[playerid]==1 || animan[params[0]]==1) return SendClientMessage(playerid,-1,"Вы уже используете анимации!");
	if(PlayerInfo[playerid][pHeal] > 0 || PlayerInfo[params[0]][pHeal] > 0)return SendClientMessage(playerid,-1,"В больнице нельзя!");
	if(Cuffed[playerid] || Cuffed[params[0]]) return SendClientMessage(playerid,-1,"Вы скованы и не можете двигаться");
	if(CuffedTime[playerid] || CuffedTime[params[0]]) return SendClientMessage(playerid,-1,"Вы парализованы!");
	if(IsPlayerConnected(params[0]))
	{
		if(playerid == params[0]) return SendClientMessage(playerid, COLOR_GREEN, "Вы не можете поцеловать сами себя!");
		if(!ProxDetectorS(3.0,playerid,params[0])) return SendClientMessage(playerid,COLOR_GREY,"Вы далеко от игрока");
		format(string,sizeof(string),"Поцеловал(a) %s",PN(params[0]));
		SetPlayerChatBubble(playerid, string, COLOR_PURPLE, 30.0, 5000);

		format(string,sizeof(string),"Поцеловал(a) %s",PN(playerid));
		SetPlayerChatBubble(params[0], string, COLOR_PURPLE, 30.0, 5000);

		new Float:angle;
		GetPlayerFacingAngle(playerid, angle);
		J_SetPlayerFacingAngle(params[0], angle + 180);

		ApplyAnimation(playerid,"BD_FIRE","GRLFRD_KISS_03",4.0,0,0,0,0,0);
		ApplyAnimation(params[0],"BD_FIRE","PLAYA_KISS_03",4.0,0,0,0,0,0);

	}
	return true;
}
