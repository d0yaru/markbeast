			}
			Controllable[playerid] = true;
			SetCameraBehindPlayer(playerid);
			if(VehicleInfo[vehicleid][vJob] == JOB_MUSORST)
			{
				if(PlayerInfo[playerid][pJob] == JOB_MUSORST)
				{
					SendClientMessage( playerid, 0xBFC0C2FF, "Доставка Мусора прекращена, Вы покинули водительское место." );
