CMD:engine(playerid, params[])
{
    if(!IsPlayerInAnyVehicle(playerid)) return SendClientMessage(playerid,COLOR_GREY, "Вы должны находится в транспорте");
	if(GetPlayerState(playerid) != 2) return false;
	if(pTemp[playerid][Anti_Pvar][13] > CurrentTimer) return false;//SendClientMessage(playerid,COLOR_GREY,"Пожалуйста, не флудите!");
	PlayerPlaySound(playerid, 17803, 0.0, 0.0, 0.0);
	new carid = GetPlayerVehicleID(playerid);
	if(IsAVelik(carid)) return false;
	if(GetEngineStatus(carid) < 1)
	{
		if(IsAOwnableFamCar(carid))
		{
			new FamVeh_ID = GetFamVehicleID(carid);
			if(FamVehInfo[FamVeh_ID][FamID] != PlayerInfo[playerid][pFam] || FamVehInfo[FamVeh_ID][FamID] == 0)
			{
				return SendClientMessage(playerid,COLOR_GREY,"[Мысли] У меня нет ключей от этого транспорта.");
			}
		}
		else
		{
			if(GetVehicleModel(carid)==437 || GetVehicleModel(carid)== 574)if(pTemp[playerid][pArendaCar]==-1)
			{
				ToCheat(0,playerid, 49);
				return true;
			}
		}
		if(IsAArendCar(carid))
		{
			new rentid = GetArendCarID(carid);
			if(ArendInfo[rentid][aRentID] != playerid) return false;
		}
		if(VehicleInfo[carid][vJob] != 0)
		{
			if(VehicleInfo[carid][vPlayer] != playerid) return false;
		}
		if(GetTeamID(playerid) != TEAM_LICENSERS)
		{
			if(VehicleInfo[carid][vTeam] == TEAM_LICENSERS)
			{
				if(pTemp[playerid][PArendedLicCar] == 0) return false;
			}
		}

		if(IsACar(carid) && !PlayerInfo[playerid][pCarLic] && CarTest[playerid] == -1) return false;
		if(IsABike(carid) && !PlayerInfo[playerid][pBikeLic]) return false;
		if(IsABoat(carid) && !PlayerInfo[playerid][pBoatLic]) return false;
		if(Fuel[carid] > 0.0)
		{
			if(IsASellCar(carid))return 1;
			if(!IsAOwnableCar(carid) || CarInfo[GetVehicleID(carid)][cKeyIN])
			{
				new Float:Health;
				GetVehicleHealth(carid, Health);
				if(Health > 1000) Health = 1000.0;
				else if(Health < 400) return SendClientMessage(playerid, COLOR_GREY, "[Мысли] Двигатель сломан. Чтобы завести его нужен механик (( Используйте телефон ))");
				new UNEngine = floatround(2000.0 - Health);
				if(UNEngine < 0) UNEngine = 0;
				else if(UNEngine > 2000) UNEngine = 2000;
				SetTimerEx("EngineVehicle", UNEngine, false, "ii", playerid, carid);
				if(PlayerInfo[playerid][pSex] == 2){SendPlayerMe(playerid,"попыталась завести двигатель");}
				else{SendPlayerMe(playerid,"попытался завести двигатель");}
				pTemp[playerid][Anti_Pvar][13] = CurrentTimer+3;
			}
			else return SendClientMessage(playerid,COLOR_GREY,"Ключи не вставлены");
		}
		else return SendClientMessage(playerid,COLOR_GREY,"Нет топлива");
	}
	else
	{
	    PlayerPlaySound(playerid, 17803, 0.0, 0.0, 0.0);
		SetEngineStatus(carid,false, playerid);
		if(PlayerInfo[playerid][pSex] == 2){SendPlayerMe(playerid,"заглушила двигатель");}
		else{SendPlayerMe(playerid,"заглушил двигатель");}
	}
	return true;
}
