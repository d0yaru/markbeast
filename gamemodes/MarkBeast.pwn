OldKicking(playerid)
{
	Kick(playerid);
}
void DisconnectPlayer(playerid)
{
	if (IsPlayerNPC(playerid)) return true;
	if(IsPlayerConnected(playerid)){Kick(playerid);}
	return true;
}
fix_kick(playerid)
{
	if(!IsPlayerConnected(playerid)) return true;
	new fix_ping = GetPlayerPing(playerid) + 25;

	if(fix_ping < 100){fix_ping = 100;}
	if(fix_ping > 1000){fix_ping = 1000;}

	return SetTimerEx("DisconnectPlayer", /*fix_ping>1000?1000:fix_ping*/ fix_ping, 0, "d", playerid);
}
