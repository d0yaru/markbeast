ModVehicle(carid, componentid)
{
	new vehicleid = GetTunID(carid);
	switch(componentid)
	{
	case 1000..1003,1014..1016,1023,1049,1050,1058,1060,1138,1139,1146,1147,1158,1162..1164: TunInfo[vehicleid][nSpoiler] = componentid;
	case 1004,1005,1011,1012: TunInfo[vehicleid][nHood] = componentid;
	case 1007,1017,1026,1027,1030,1031,1036,1039..1042,1047,1048,1051,1052,1056,1057,1062,1063,1069,1072,1090,1093..1095,1099,1101,1102,1106..1108,1119..1122,1124,1133,1134,1137: TunInfo[vehicleid][nSideskirt] = componentid;
	case 1008..1010: TunInfo[vehicleid][nNitro] = componentid;
	case 1013,1024: TunInfo[vehicleid][nLamps] = componentid;
	case 1018..1022,1028,1029,1034,1037,1043..1046,1059,1064.1066,1089,1092,1105,1113,1114,1126,1127,1129,1132,1135,1136: TunInfo[vehicleid][nExhaust] = componentid;
	case 1025,1073..1085,1096..1098: TunInfo[vehicleid][nWheels] = componentid;
	case 1006,1032,1033,1035,1038,1053..1055,1061,1067,1068,1088,1091,1103,1128,1130,1131: TunInfo[vehicleid][nRoof] = componentid;
	case 1086: TunInfo[vehicleid][nStereo] = componentid;
	case 1087: TunInfo[vehicleid][nHydraulics] = componentid;
	case 1100,1123,1125: TunInfo[vehicleid][nBullbar] = componentid;
	case 1109,1110: TunInfo[vehicleid][nRearBullbars] = componentid;
	case 1111,1112: TunInfo[vehicleid][nFrontSign] = componentid;
	case 1115,1116: TunInfo[vehicleid][nFrontBullbars] = componentid;
	case 1117,1152,1153,1155,1157,1160,1165,1166,1169..1174,1176,1179,1181,1182,1185,1188..1191: TunInfo[vehicleid][nFrontBumper] = componentid;
	case 1140,1141,1148..1151,1154,1156,1159,1161,1167,1168,1175,1177,1178,1180,1183,1184,1186,1187,1192,1193: TunInfo[vehicleid][nRearBumper] = componentid;
	case 1142..1145: TunInfo[vehicleid][nVents] = componentid;
	default: return false;
	}
	SaveTuning(carid, GetTuningName(componentid), componentid);
	return true;
