StartMeubleShow(playerid, CATEGORID)
{
    MeubleIDSelected[playerid] = 0;
	for(new i; i < sizeof(MeubleInfo); i++)
	{
	    if(CATEGORID == MeubleInfo[i][mType])
	    {
		    if(PlayerMeubleObject[playerid] != -1)
			{
			    DestroyPlayerObject(playerid, PlayerMeubleObject[playerid]);
			    PlayerMeubleObject[playerid] = -1;
			}
	        PlayerMeubleObject[playerid] = CreatePlayerObject(playerid, MeubleInfo[i][mMpdelid], MeubleInfo[i][mX], MeubleInfo[i][mY], MeubleInfo[i][mZ], MeubleInfo[i][mrX], MeubleInfo[i][mrY], MeubleInfo[i][mrZ]);

	    	SetPlayerCameraPos(playerid, 1401.6962,-1197.7854,1008.1650);
			SetPlayerCameraLookAt(playerid, MeubleInfo[i][mX], MeubleInfo[i][mY], MeubleInfo[i][mZ]);

			MeubleIDSelected[playerid] = i;

            CMDstr = "";
			format(CMDstr,sizeof(CMDstr),"Cost: %d",MeubleInfo[i][mCost]);
			PlayerTextDrawSetString(playerid, InfoText[playerid],CMDstr);
			PlayerTextDrawShow(playerid,InfoText[playerid]);


	        break;
	    }
	}
	return true;
}
NextMeubleShow(playerid, CATEGORID)
{
	for(new i = MeubleIDSelected[playerid]+1; i < sizeof(MeubleInfo); i++)
	{
	    if(CATEGORID == MeubleInfo[i][mType])
	    {
		    if(PlayerMeubleObject[playerid] != -1)
			{
			    DestroyPlayerObject(playerid, PlayerMeubleObject[playerid]);
