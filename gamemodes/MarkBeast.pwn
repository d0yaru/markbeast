public OnPlayerUpdate(playerid)
{
	new PlayerStateUpdate=GetPlayerState(playerid);
	if(PlayerStateUpdate==1) return OnPlayerUpdatePlayer(playerid);
	else if(PlayerStateUpdate==2) return OnPlayerUpdateVehicle(playerid);
	else if(PlayerStateUpdate==3) return OnPlayerUpdatePassenger(playerid);
	else if(PlayerStateUpdate==9) return OnPlayerUpdateSpectator(playerid);

	return 1;
}
