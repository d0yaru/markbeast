		SendClientMessage(playerid, COLOR_WHITE, " 5. Психически не здоров");
		return true;
	}
	if(!ProxDetectorS(3.0, playerid, id, 1))return SendClientMessage(playerid, COLOR_GREY, "Вы далеко от игрока");
	if(!IsPlayerConnected(id)) return SendClientMessage(playerid, COLOR_GREY, "Этот игрок не в сети.");
	if(!IsPlayerLogged[id]) return SendClientMessage(playerid, COLOR_GREY, "Этот игрок не в сети.");
	if(!IsValidPassport(id)) return SendClientMessage(playerid, COLOR_GREY, "У этого игрока нет паспорта.");
	if(!TeamDuty[playerid]) return SendClientMessage(playerid,-1,"Вы не на дежурстве!");
	if(status > 5 || status < -1) return SendClientMessage(playerid,-1,"Такой мед карты не бывает!");
	new Med[65];
	MedCardInfo[id][mcState] = status;
	switch(MedCardInfo[id][mcState])
	{
	case -1: Med = "Нет мед. карты";
	case 0: Med = "Не определен";
	case 1: Med = "Полностью здоров";
	case 2: Med = "Здоров, но запрещена работа с тяжёлой физической нагрузкой";
	case 3: Med = "Не большие проблемы со здоровьем";
	case 4: Med = "Большие проблемы со здоровьем, не рекомендуется для гос. работ";
	case 5: Med = "Психически не здоров";
	}
	SetString(MedCardInfo[id][mcGiver], Name(playerid));
	new year, month, day;
	getdate(year, month, day);
	format(string, sizeof(string), "%i/%i/%i", day, month, year);
	SetString(MedCardInfo[id][mcDate], string );
	format(string, sizeof(string),"{FFFFFF}Медик %s выдал вам медицинскую карту: %s", Name(playerid), Med );
	SendClientMessage(id, COLOR_LIGHTBLUE, string);
	format(string, sizeof(string),"{FFFFFF}Вы выдали медицинскую карту %s: %s", Name(id), Med );
	SendClientMessage(playerid, COLOR_LIGHTBLUE, string);

	format(string, sizeof(string), "UPDATE `passports` SET `McGiver` = '%s', `McDate` = '%s', `McState` = '%d' WHERE `Name` = '%s'",
	Name(playerid),
	MedCardInfo[id][mcDate],
	MedCardInfo[id][mcState],
	Name(id));
	mysql_tquery(mysql, string);
	return true;
}
