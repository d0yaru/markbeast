new CurrentMAxTrigger = 0;

#define TRIGGER_MEUBLESHOP  1

CreateTrigger(Float:x,Float:y,Float:z,ww,ii,string[], type)
{
	if(CurrentMAxTrigger < MAX_TRIGGERS)
	{
	    z = z - 1.77;
	    new Trigger = CreateDynamicObject(1317, x, y, z, 0.0, 0.0, 0.0, ww, ii);
	    SetDynamicObjectMaterial(Trigger,0,18646,"matcolours","blue",0xAA80FFDD);

		new i = CurrentMAxTrigger;
	    triggers[i] = CreateDynamicSphere(x,y,z+1.5,1.0,ww,ii);
	//	printf("triggers[i] = %d", triggers[i]);
		
	    Area_Type[triggers[i]] = type;

	    CreateDynamic3DTextLabel(string,0xFFFF00FF,x, y, z+2.0,10.0, INVALID_PLAYER_ID, INVALID_VEHICLE_ID, 0, ww, ii);

	    CurrentMAxTrigger++;
    }
    else printf("ERROR: MAX TRIGGERS LIMIT HAS BEEN EARNED");
}

