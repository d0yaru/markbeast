CMD:fmute(playerid, params[])
{
	if(!PlayerInfo[playerid][pLeader] && PlayerInfo[playerid][pRank] < 9) return SendClientMessage(playerid, COLOR_GREY, "Вам не доступна эта команда!");
	new id, time, reason[100], string[150];
	if(sscanf(params,"dds[100]", id, time, reason)) return SendClientMessage(playerid, COLOR_PROJECT,"Используйте: {FFFFFF}/fmute [id] [time] [reason]");
	if(IsADomen(reason)  && PlayerInfo[playerid][pAdmin]<6)
	{
		SendClientMessage(playerid,COLOR_RED,"В вашем сообщении обнаружено запрещенное содержание.");

		return false;
	}
	if(IsAIP(reason))return 1;
	if(PlayerInfo[playerid][pMuteTime]>0)return false;
	if(time>180)return SendClientMessage(playerid, -1,"Установить время заглушки можно не более на 60 минут!");
	if(!TeamDuty[playerid]) return SendClientMessage(playerid,-1,"Вы не на дежурстве!");
	if(GetTeamID(id) != GetTeamID(playerid)) return SendClientMessage(playerid, COLOR_GREY, "Этот игрок не состоит в твоей организации!");
	if(time == 0) return SendClientMessage(playerid, COLOR_GREY, "На 0 минут нельзя!");
	if(PlayerInfo[id][pLeader] > 0 ) return SendClientMessage(playerid,-1,"Вы не можете выдать мут лидеру");
	format(string,sizeof(string),"Сотрудник %s заглушил(а) игрока %s на %d минут. Причина: %s",Name(playerid),Name(id),time,reason);
	SendTeamMessage(GetTeamID(playerid), COLOR_ADMINRED, string);
	PlayerInfo[id][pFMuteTime] = time*60;
	return true;
}
