public OnVehicleMod(playerid, vehicleid, componentid)
{
	if(IsAtTunning(playerid))
	{
		if(!IsAOwnableCar(vehicleid))
		{
			ShowPlayerDialogEx(playerid,0,0,"","{CD1717}Ошибка!{FFFFFF}\n\n\nТюнинговать можно только личный транспорт!\n\n\n","{CD1717}Принять{FFFFFF}","");
			RemoveVehicleComponent(vehicleid,componentid);
			return 0;
		}
		if(GetPlayerInterior(playerid) == 0 && GetPlayerState(playerid) == PLAYER_STATE_DRIVER)
		{
			RemoveVehicleComponent(vehicleid,componentid);
			ToCheat(1,playerid, 33);
			return 0;
		}
		if(!VehicleInfo[vehicleid][vTeam] && !VehicleInfo[vehicleid][vJob] && !IsAArendCar(vehicleid))
		{
			ModVehicle(vehicleid, componentid);
			GiveBizMoney(BIZ_CU,300);
		}
		else
		{
			RemoveVehicleComponent(vehicleid, componentid);
			SendClientMessage(playerid, COLOR_WHITE, "Этот транспорт нельзя тюнинговать");
		}
	}
	else
	{
		if(GetPlayerState(playerid) == 2)
		{
			RemoveVehicleComponent(vehicleid, componentid);
			ToCheat(1,playerid, 34);
			return true;
		}
	}
	return true;
}
