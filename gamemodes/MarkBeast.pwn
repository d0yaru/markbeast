CMD:accept(playerid,param[])
{
	new string[400];
	new id=PanelPlayerID[playerid],Float: money = CenaRaboti[playerid] ;
	if(id < 0) return  SendClientMessage(playerid,-1,"Вам никто ничего не предлагал!");
	if(!IsPlayerLogged[id])return false;
	if(PlayerInfo[id][pTelNum]<=1)return false;

	new price = floatround( money, floatround_floor );
	if(PlayerInfo[playerid][pMoney]<price)return SendClientMessage(playerid,-1,"У вас не достаточно денег чтобы заплатить продавцу!");
	if(!ProxDetectorS(9.0, playerid, id, 1)) return SendClientMessage(playerid,-1,"Вы далеко от игрока!");
	if(price<1)return false;
	switch(Ingatre[playerid])
	{
	case 1:
		{
			format(string,sizeof(string),"%s успешно продал вам SIM карту с номером {52A32A}%d{FFFFFF} за {52A32A}%d$.",PN(id),PlayerInfo[id][pTelNum],price);
			SendClientMessage(playerid,-1,string);
			format(string,sizeof(string),"{FFFFFF}%s принял ваше предложение о покупке SIM карты. Вы получили {52A32A}%d$.",PN(playerid),price);
			SendClientMessage(id,COLOR_PROJECT,string);
			if(price > 10000)
			{
				format(string,sizeof(string),"Игрок %s[%d] [IP: %s] продал симку игроку %s[%d] [IP: %s] на счет $%d [$%d]",Name(id),id,PlayerIP(id),Name(playerid),playerid,PlayerIP(playerid),price);
				SendAdminsMessage(COLOR_RED, string);
			}
			PlayerInfo[playerid][pTelNum] = PlayerInfo[id][pTelNum];
			PlayerInfo[id][pTelNum] = 0;
			GiveMoney(playerid,-price,"SimKart");
			GiveMoney(id,price,"SimKart");
		}
	}
	CenaRaboti[playerid]=0;
	Ingatre[playerid]=0;
	PanelPlayerID[playerid]=-1;
	return true;
}
