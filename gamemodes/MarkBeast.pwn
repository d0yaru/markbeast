GetNumInText(const string[])
{
	new length = strlen(string);
	if (length == 0) return false;
	new countcifers = 0;
	for (new i = 0; i < length; i++)
	{
		if (string[i] >= '0' && string[i] <= '9'){countcifers++;}
	}
	return countcifers;
}
KickIP(IP[])
{
	foreach(i)
	{
		new pip[16];
		GetPlayerIp(i,pip,16);
		if(GetString(pip,IP))
		{
			SendClientMessage(i,-1,"Ваш IP был заблокирован");
			J_Kick(i);
		}
	}
	return true;
}
