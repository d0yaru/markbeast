void UseDrugs(playerid, step)
{
	if(IsPlayerConnected(playerid))
	{
		switch(step)
		{
		case 0:
			{
				step = step + 1;
				SetTimerEx("UseDrugs", 1000+random(500), 0, "dd", playerid,step);
				SendPlayerDo(playerid,"Пакет с наркотиками в кармане");
			}
		case 1:
			{
				step = step + 1;
				SendPlayerMe(playerid,"достал(а) пакет правой рукой из кармана ");
				SetTimerEx("UseDrugs", 1000+random(500), 0, "dd", playerid,step);
			}
		case 2:
			{
				step = step + 1;


				if(PlayerInfo[playerid][pDrugs] < 1) return SendClientMessage(playerid,COLOR_GREY, "У вас нет наркотиков");
				if(PlayerInfo[playerid][pHeal] > 0) return SendClientMessage(playerid,COLOR_GREY, "В больнице нельзя использовать наркотики.");
				if(UseDrugsTime[playerid] > 120) return false;
				new Float:Health;
				GetPlayerHealth(playerid,Health);
				Health += 30;
