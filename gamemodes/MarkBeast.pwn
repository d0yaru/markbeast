			{
				if (ProxDetectorS(1.5, playerid, giveplayerid))
				{
					if(PlayerInfo[playerid][pJailTime] <= 0 && PlayerInfo[giveplayerid][pJailTime] <= 0)
					{
						SetPlayerPosAndFaceToPlayer(giveplayerid,playerid);
					}
					CMDstr = "";
					format(CMDstr, sizeof(CMDstr), "пожал(а) руку %s'у",PN(giveplayerid));
					SendPlayerMe(playerid,CMDstr);
					ApplyAnimation(playerid,"GANGS","hndshkfa",4.0,0,0,0,0,0,1);
					ApplyAnimation(giveplayerid,"GANGS","hndshkfa",4.0,0,0,0,0,0,1);
					return true;
				}
				SendClientMessage(playerid, COLOR_GREY, "Он далеко от вас");
				return true;
			}
			SendClientMessage(playerid, COLOR_GREY, "[Ошибка] Человек не найден!");
			return true;
		}
		return SendClientMessage(playerid, COLOR_PROJECT, "Введите: {FFFFFF}/hi [id]");
	}
	return SendClientMessage(playerid, COLOR_GREY, "Нельзя использовать в машине");
}
