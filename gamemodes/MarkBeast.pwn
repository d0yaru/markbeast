CMD:dice(playerid, params[])
{
	new bool:okay =false;
	for(new i;i<sizeof(DiceTable);i++)
	{
		if(IsPlayerInRangeOfPoint(playerid,2.5,DiceTable[i][0],DiceTable[i][1],DiceTable[i][2]))
		{
			okay = true;
			break;
		}
	}
	if(!okay) return SendClientMessage(playerid, -1,"Вы не у стола для игры в кости!");
	if(Invite_To_Dice[playerid] != INVALID_PLAYER_ID)return SendClientMessage(playerid, -1,"Вы уже кому-то предложили сыграть!");
	if(Invited_To_Dice[playerid]!= INVALID_PLAYER_ID)return SendClientMessage(playerid, -1,"Вам уже кто-то предложил сыграть!");

	if(sscanf(params, "ud", params[0],params[1])) return SendClientMessage(playerid, COLOR_PROJECT,"Используйте: {FFFFFF}/dice [ID игрока] [ставка]");
	if(!IsPlayerConnected(params[0]))return SendClientMessage(playerid, -1,"Игрок не в сети!");
	if(PlayerInfo[playerid][pVIP] < 1 && PlayerInfo[playerid][pVipTime] < 1)
	{
		if(params[1] < 5 || params[1] > 10000)
		{
			SendClientMessage(playerid, -1,"Минимальная ставка 5$, максимальная 10 000$.");
			SendClientMessage(playerid, -1,"VIP игроки могут ставить до "COLOR_PROJECT_EX"1 000 000$.");
			return true;
		}
	}
	else
	{
		if(params[1] < 5 || params[1] > 1000000)
		{
			SendClientMessage(playerid, -1,"Минимальная ставка 5$, максимальная 1 000 000$.");
			return true;
		}
	}
	if(!ProxDetectorS(6.0, playerid, params[0])) return SendClientMessage(playerid, COLOR_GREY, "Игрок слишком далеко!");
	if(params[0] == playerid)  return SendClientMessage(playerid, COLOR_GREY, "Вы не можете играть с самим собой!");

	if(PlayerInfo[playerid][pMoney] < params[1]) return SendClientMessage(playerid,-1,"Вам не хватает средств для такой ставки.");
	SendClientMessage(playerid,-1,"Чтобы отказатся от своего предложения введите [/redice]");
	CMDstr = "";
	format(CMDstr, 80, "Вы предложили %s сыграть в кости. Ставка: %i",Name(params[0]),params[1]);
	SendClientMessage(playerid, -1, CMDstr);
	format(CMDstr, 80, "%s предлагает вам сыграть в кости. Ставка: %i",Name(playerid),params[1]);
	SendClientMessage(params[0], -1, CMDstr);
	SendClientMessage(params[0], COLOR_WHITE, "Введите {48FC23}/yes{FFFFFF} чтобы согласиться, {FF0000}/no{FFFFFF} чтобы отказаться.");
	Invite_To_Dice[playerid] = params[0];
	Invite_To_Dice_Money[playerid] = params[1];
	Invited_To_Dice[params[0]] = playerid;
	return true;
}
