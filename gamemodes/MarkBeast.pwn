IsertMeuble(playerid, i, mModel, Float:mX_Insert, Float:mY_Insert, Float:mZ_Insert, Float:mRX_Insert, Float:mRY_Insert, Float:mRZ_Insert)
{
	if(HouseInfo[i][hMeubles] >= 9) return SendClientMessage(playerid, -1, "Лимит мебели превышен!");
	for(new meubel_slot; meubel_slot < 10; meubel_slot++)
 	{
	  	if(HouseInfo[i][hMeublesID][meubel_slot] == 0)
	  	{

            HouseInfo[i][hMeubles]++;
            
            HouseInfo[i][hMeublesModel][meubel_slot] = mModel;
            HouseInfo[i][hMeublePosX][meubel_slot] = mX_Insert;

			HouseInfo[i][hMeublesID][meubel_slot] = CreateDynamicObject(HouseInfo[i][hMeublesModel][meubel_slot], mX_Insert, mY_Insert, mZ_Insert, mRX_Insert, mRY_Insert, mRZ_Insert, i+50, HouseInfo[i][hInt], -1, 75.0);
			
			CMDstr = "";
			strcat(CMDstr,"INSERT INTO `meuble` (`X`,`Y`,`Z`,`X_Rotation`,`Y_Rotation`,`Z_Rotation`,`modelid`,`houseid`) VALUE ");
