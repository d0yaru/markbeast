			SetPlayerCameraLookAt(playerid, MeubleInfo[i][mX], MeubleInfo[i][mY], MeubleInfo[i][mZ]);

			MeubleIDSelected[playerid] = i;

            CMDstr = "";
			format(CMDstr,sizeof(CMDstr),"Cost: %d",MeubleInfo[i][mCost]);
			PlayerTextDrawSetString(playerid, InfoText[playerid],CMDstr);



	        return true;
	    }
	}
	return false;
}
BackMeubleShow(playerid, CATEGORID)
{
	for(new i = MeubleIDSelected[playerid]-1; i > -1; i--)
	{
	    if(CATEGORID == MeubleInfo[i][mType])
	    {
