	if(PlayerInfo[playerid][pBomb]>=1)return SendClientMessage(playerid,COLOR_RED,"Вы уже сделали бомбу!");
	ApplyAnimation(playerid, "BOMBER", "BOM_Plant", 2, 0, 0, 0, 0, 0);
	PlayerInfo[playerid][pMats]-=450;
	CreteBomb[playerid]=30;
	SendClientMessage(playerid,COLOR_GREY,"Вы начали делать бомбу.");
	return true;
}
