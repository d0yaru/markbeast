CMD:bizwar(playerid)
{
	if(!IsAtMafia(playerid)) return SendClientMessage(playerid,COLOR_RED,"Вы не состоите в мафии");
	if(PlayerInfo[playerid][pLeader] == 0 && PlayerInfo[playerid][pRank] < 9) return SendClientMessage(playerid,COLOR_RED,"Доступно только лидеру и его заместителю!");
	if(BizWarState > 0) return SendClientMessage(playerid,COLOR_RED,"Сейчас уже идет сражение за бизнес!");
	foralbiz(i)
	{
		if(!IsPlayerInRangeOfPoint(playerid,10.0,BizInfo[i][bEnter_X],BizInfo[i][bEnter_Y],BizInfo[i][bEnter_Z])) continue;
		if(GetString(BizInfo[i][bOwner],"The State")) return SendClientMessage(playerid,COLOR_GREY,"Этим бизнесом никто не владеет!");
		if(BizInfo[i][bKrasher] == PlayerInfo[playerid][pMember]) return SendClientMessage(playerid,COLOR_GREY,"Этот бизнес и так под вашим контролем!");
		if(RecoveryTime[PlayerInfo[playerid][pMember]] > 0)
		{
			new str[120];
			format(str,sizeof(str),"Вы еще не восстановились после предыдущего сражения, новое сражение будет возможно через %d секунд",RecoveryTime[PlayerInfo[playerid][pMember]]);
			return SendClientMessage(playerid,COLOR_GREY,str);
		}
		if(IsGangConnected(BizInfo[i][bKrasher])<2) return SendClientMessage(playerid, COLOR_GREY, "Для захвата бизнеса, хотя-бы 2 члена вражеской организации должны быть онлайн");

		if(BizInfo[i][bKrasher] == 0)
		{
			BizInfo[i][bKrasher] = PlayerInfo[playerid][pMember];
			SendClientMessage(playerid,COLOR_PROJECT,"Теперь вы крышуете этот бизнес!");
			UpdateBiz(i,1);
			RecoveryTime[PlayerInfo[playerid][pMember]] = 7200;
		}
		else
		{
			if(RecoveryTime[BizInfo[i][bKrasher]] > 0)
			{
				new str[140];
				format(str,sizeof(str),"Вражеская организация еще не восстановилась после предыдущего сражения, новое сражение для них будет возможно через %d секунд",RecoveryTime[BizInfo[i][bKrasher]]);
				return SendClientMessage(playerid,COLOR_GREY,str);
			}
			ClickBiz[playerid] = i;
			//	ShowPlayerDialogEx(playerid,9612,2,"Место проведения стрелы:","- Лесопилка\n- Заброшенный Аэродром\n- Ферма Fort Carson\n- Заброшенная деревня","Принять","Отмена");
			pTemp[playerid][Anti_Pvar][23] = 4;
			return ShowPlayerDialogEx(playerid,591,0,"Информация","{FFFFFF}Каждой мафии предстоит взять с собой по 4 члена мафии в заброшенную деревню, у вас 10 минут, если все 8 членов двух мафий будут там, то начнется сражение!","В бой!","Отмена");
		}
	}
	return true;
}
