CMD:blacklist(playerid, params[])
{
	if(IsAtFrakcia(playerid) && PlayerInfo[playerid][pLeader]>0)
	{
		if(sscanf(params,"us[72]",params[0],params[1])) return SendClientMessage(playerid, COLOR_PROJECT, "Используйте:{FFFFFF} /blacklist [id] [причина]");
		if(!ProxDetectorS(4.0, playerid, params[0], 1)) return SendClientMessage(playerid,COLOR_RED,"Вы далеко от игрока!");
		if(PlayerInfo[params[0]][pLeader] > 0 ) return SendClientMessage(playerid,COLOR_RED,"Вы не можете добавить лидера в чс");
		if(!IsPlayerConnected(params[0])) return SendClientMessage(playerid,COLOR_GREY,"Игрок не найден");
