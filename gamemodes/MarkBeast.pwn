RenamePlayer(playerid, name[])
{
	if(IsPlayerLogged[playerid])
	{
		new string[150], oldname[24];
		GetPlayerName(playerid,oldname,sizeof(oldname));
		format(string, sizeof(string), "UPDATE `usrboston` SET `NickName` = '%s' WHERE `ID` = '%i'", name, PlayerInfo[playerid][pID]);
		mysql_tquery(mysql, string);
		format(string, sizeof(string), "UPDATE `usrboston` SET `Referal` = '%s' WHERE `Referal` = '%s'", name, oldname);
		mysql_tquery(mysql, string);
		format(string, sizeof(string), "UPDATE `onlineinfo` SET `Name` = '%s' WHERE `Name` = '%s'", name, oldname);
		mysql_tquery(mysql, string);

		new house = PlayerInfo[playerid][pHouseKey];
		new hotel = PlayerInfo[playerid][pFrom];
		if(PlayerInfo[playerid][pLeader]>0) SetString(OrgInfo[playerid][oLeader],Name(playerid));
		if(house != -1 && GetString(HouseInfo[house][hOwner],oldname))
		{
			SetString(HouseInfo[house][hOwner],name);
			UpdateHouse(house);
		}
		if(hotel != -1 && GetString(HotelInfo[hotel][vladelec],oldname))
		{
			SetString(HotelInfo[hotel][vladelec],name);
			UpdateHotel(hotel);
		}
		for(new i = 0;i < OWNABLECARS;i ++)
		{
			new number = OwnableCar[i];
			if(IsPlayerRegCar(playerid,number))
			{
				SetCarOwner(number,name);
				//SetCarKeyer(number,name);
				//	UpdateCarInfo(number);
				SaveOwnableCar(i);
			}
		}
		for(new i = 0;i < OWNABLECASINO;i ++)
		{
			if(GetString(CasinoInfo[i][casOwner],oldname))
			{
				SetString(CasinoInfo[i][casOwner],name);
				UpdateCas(i);
				SaveCasino(i);
			}
		}
		foralbiz(i)
		{
			if(GetString(BizInfo[i][bOwner],oldname))
			{
				SetString(BizInfo[i][bOwner],name);
				SaveBiz(i);
			}
		}
		format(string,sizeof(string),"Теперь %s известен как %s.",oldname,name);
		SendClientMessageToAll(0xA7FC00FF,string);
		SendClientMessage(playerid,COLOR_ACHAT, "По скольку вы сменили имя, вам необходимо сделать новый паспорт и мед-карту.");
	}
	SetPlayerNameEx(playerid, name);
	return true;
}
