CMD:sellfamcar(playerid, params[])
{
	new carid = GetPlayerVehicleID(playerid);
	if(carid >0 )
	{
		new FamVeh_ID = GetFamVehicleID(carid);
		if(FamVehInfo[FamVeh_ID][FamID] != PlayerInfo[playerid][pFam]) return SendClientMessage(playerid,-1,"Этот транспорт не принадлежит вашей семье!");
		if(PlayerInfo[playerid][pFamRank] < FRANK_ZAM) return SendClientMessage(playerid,-1,"Ваш ранг не позволяет вам продать этот транспорт!");

		if(sellcarto[playerid]>CurrentTimer)return SendClientMessage(playerid,-1,"Делать предложение о продаже/обмене можно раз в минуту!");

		new id, cost;
		if(sscanf(params,"ud",id,cost)) return SendClientMessage(playerid,COLOR_PROJECT,"Используйте: {FFFFFF}/sellfamcar [id] [цена]");

		if(PlayerInfo[id][pFam] == PlayerInfo[playerid][pFam]) return SendClientMessage(playerid,-1,"Нельзя продать семейный транспорт члену своей семьи.");


		if(cost<7500)return SendClientMessage(playerid,-1,"Нельзя продать машину дешевле чем за 7500$");
		if(!IsPlayerConnected(id)) return SendClientMessage(playerid,-1,"Неверный id игрока!");
		if(id == playerid) return false;
		if(!ProxDetectorS(7.0, playerid, id)) return SendClientMessage(playerid,COLOR_GREY, "Игрок должен быть рядом с транспортом");
		if(PlayerInfo[id][pFam] == 0) return SendClientMessage(playerid,-1,"У этого игрока нет семьи!");

		sellcarto[playerid]=CurrentTimer+60;

		new string[150];
		format(string,sizeof(string),"{FFFFFF}Вы предлагаете %s купить у вас транспорт\n\nМодель: %s\nНомер: %d\nЦена: %d",
		Name(id),VehicleName[GetVehicleModelEx(carid)-400],carid,cost);
		ShowPlayerDialogEx(playerid,INVALID_DIALOG_ID,0,"Продажа транспорта",string,"Ок","");

		format(string,sizeof(string),"{FFFFFF}%s предлагает вам купить у него транспорт\n\nМодель: %s\nНомер: %d\nЦена: %d",
		Name(playerid),VehicleName[GetVehicleModelEx(carid)-400],carid,cost);
		ShowPlayerDialogEx(id,3001,0,"Покупка транспорта",string,"Купить","Отмена");
		CarOffer[id] = playerid;
		CarOffer[playerid] = id;
		CarPrice[id] = cost;
		return true;
	}
	SendClientMessage(playerid,COLOR_GREY,"Вы должны сидеть в семейном транспорте");
	return true;
}
