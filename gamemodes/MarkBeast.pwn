}
SetVehicleToEliteParking(vehicleid,number)
{
	TryAgainForSoldVehicle[vehicleid] = 0;
	MARK_TRY_TO_PARK:
	new MinimumTime = 2147483643;
	new goodplacetodoit = 0;
	for(new i = 0;i < max_parking_elite;i ++)
	{
		if(LastUsed_Elite[i] < MinimumTime)
		{
			goodplacetodoit = i;
			MinimumTime = LastUsed_Elite[i];
		}
	}
	for(new i = 0;i < MAX_VEHICLES;i ++)
	{
		if(!IsValidVehicle(i)) continue;
		if(GetVehicleVirtualWorld(i) != 0) continue;
		if(GetVehicleDistanceFromPoint(i, ParkingPos_Elite[goodplacetodoit][0],ParkingPos_Elite[goodplacetodoit][1],ParkingPos_Elite[goodplacetodoit][2]) > 2.0)continue;
		LastUsed_Elite[goodplacetodoit] = CurrentTimer;
		TryAgainForSoldVehicle[vehicleid]++;
		if(TryAgainForSoldVehicle[vehicleid] < max_parking_elite)
		{
			goto MARK_TRY_TO_PARK;
		}
		break;
	}

	LastUsed_Elite[goodplacetodoit] = CurrentTimer;

	SetVehicleVirtualWorld(vehicleid, 0);
	LinkVehicleToInterior(vehicleid, 0);

	CarInfo[number][cPos_X] = ParkingPos_Elite[goodplacetodoit][0];
	CarInfo[number][cPos_Y] = ParkingPos_Elite[goodplacetodoit][1];
	CarInfo[number][cPos_Z] = ParkingPos_Elite[goodplacetodoit][2];
	CarInfo[number][cPos_A] = ParkingPos_Elite[goodplacetodoit][3];

	CarInfo[number][cSell_X] = CarInfo[number][cPos_X];
	CarInfo[number][cSell_Y] = CarInfo[number][cPos_Y];
	CarInfo[number][cSell_Z] = CarInfo[number][cPos_Z];
	CarInfo[number][cSell_A] = CarInfo[number][cPos_A];

	DestroyVehicleEx(vehicleid);
	OwnableCar[number] = J_CreateVehicle(CarInfo[number][cModel],CarInfo[number][cPos_X],CarInfo[number][cPos_Y],CarInfo[number][cPos_Z],CarInfo[number][cPos_A],CarInfo[number][cColor_1],CarInfo[number][cColor_2],1000000);
	RealVehicleID[OwnableCar[number]] = number;
	return true;
}
