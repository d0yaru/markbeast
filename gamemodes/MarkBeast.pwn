		if(PlayerInfo[playerid][pJob] != VehicleInfo[vehicleid][vJob])
		{
			SendClientMessage(playerid,COLOR_GREY,"[Мысли] Хм.. Мне не завести двигатель без ключей...");
			ExitCar(playerid);
		}
	}
	else if(VehicleInfo[vehicleid][vTeam] != 0)
	{
		if(VehicleInfo[vehicleid][vTeam] != TEAM_LICENSERS && CarTest[playerid] == -1)
		{
			if(IsAtMafia(playerid) && GetVehicleModel(vehicleid) == 433 )
			{
				return false;
			}
			else
			{
				if(GetTeamID(playerid) == VehicleInfo[vehicleid][vTeam])
				{
					if(!TeamDuty[playerid])
					{
						SendClientMessage(playerid,COLOR_GREY,"[Мысли] По уставу брать служебный транспорт вне рабочее время запрещено... (( Начните работу ))");
						ExitCar(playerid);
						return false;
					}
					if(PlayerInfo[playerid][pRank] < VehicleInfo[vehicleid][vRank])
					{
						SendClientMessage(playerid,COLOR_GREY,"[Мысли] Главные разозлятся, если я возьму этот транспорт...");
						ExitCar(playerid);
						return false;
					}
				}
				else
				{
					if(VehicleInfo[vehicleid][vLock] == 1)
					{
						SendClientMessage(playerid,COLOR_GREY,"[Мысли] Транспорт закрыт...");
						ExitCar(playerid);
						return false;
					}
				}
			}
		}
		if(VehicleInfo[vehicleid][vTeam] == TEAM_LICENSERS && GetTeamID(playerid) != TEAM_LICENSERS && CarTest[playerid] == -1)
		{
			SendClientMessage(playerid,COLOR_GREY,"[Мысли] Хм.. Мне не завести двигатель без ключей...");
			ExitCar(playerid);
			return false;
		}
	}

	if(IsAPlane(vehicleid))
	{
		if(CarTest[playerid] == -1 || VehicleInfo[vehicleid][vTeam] != TEAM_LICENSERS)
		{
			if(PlayerInfo[playerid][pFlyLic] < YES_LICENSE)
			{
				SendClientMessage(playerid,COLOR_GREY,"[Мысли] Я не умею управлять этой штукой! (( Пройдите обучение полетам на самолете, пройдя его вы так же получите лицензию ))");
				ExitCar(playerid);
				return true;
			}
		}
	}
	if(IsABoat(vehicleid))
	{
		if(PlayerInfo[playerid][pBoatLic] < YES_LICENSE)
		{
			SendClientMessage(playerid,COLOR_GREY,"[Мысли] Я не умею управлять этой штукой! (( Пройдите обучение в автошколе /gps - важные места - автошкола ))");
			ExitCar(playerid);
			return true;
		}
	}
	if(IsABike(vehicleid))
	{
		if(PlayerInfo[playerid][pBikeLic] < YES_LICENSE)
		{
			SendClientMessage(playerid,COLOR_GREY,"[Мысли] Я не умею управлять этой штукой! (( Пройдите обучение в автошколе /gps - важные места - автошкола ))");
			ExitCar(playerid);
			return true;
		}
	}
	return true;
}
