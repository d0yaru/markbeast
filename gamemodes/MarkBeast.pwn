public OnPlayerWeaponShot(playerid, weaponid, hittype, hitid, Float:fX, Float:fY, Float:fZ)
{
	/*	new string[128];
	format(string,sizeof(string),"OnPlayerWeaponShot(%d, %d, %d, %d, %f, %f, %f) #1",playerid, weaponid, hittype, hitid, fX, fY, fZ);
	SendClientMessage(playerid,-1,string);*/
	if(hittype > 4) return false;

	/*	format(string,sizeof(string),"OnPlayerWeaponShot(%d, %d, %d, %d, %f, %f, %f) #2",playerid, weaponid, hittype, hitid, fX, fY, fZ);
	SendClientMessage(playerid,-1,string);*/
	if((hittype == BULLET_HIT_TYPE_NONE) && GetPlayerDistanceFromPoint(playerid, fX, fY, fZ) > 340.0 && (fX != 0.0 && fY != 0.0 && fZ != 0.0)) return false;

	/*	format(string,sizeof(string),"OnPlayerWeaponShot(%d, %d, %d, %d, %f, %f, %f) #3",playerid, weaponid, hittype, hitid, fX, fY, fZ);
	SendClientMessage(playerid,-1,string);*/
	if((hittype == BULLET_HIT_TYPE_PLAYER && ! IsPlayerConnected(hitid)) || (hittype == BULLET_HIT_TYPE_VEHICLE && !IsValidVehicle(hitid)) || (hittype == BULLET_HIT_TYPE_OBJECT && !IsValidObject(hitid)) || (hittype == BULLET_HIT_TYPE_PLAYER_OBJECT && !IsValidPlayerObject(playerid, hitid))) return false;

	/*format(string,sizeof(string),"OnPlayerWeaponShot(%d, %d, %d, %d, %f, %f, %f) #4",playerid, weaponid, hittype, hitid, fX, fY, fZ);
	SendClientMessage(playerid,-1,string);*/

	if(hitid !=INVALID_PLAYER_ID)
	{
		if(GetPlayerWeapon(playerid) == 0)
		{
			if(!ProxDetectorS(8, playerid, hitid, 1))
			{
				ToCheat(0,playerid, 1);
				return 0;
			}
			if((CurrentTimer - shotTime[playerid]) < 1)shot[playerid]+=1;
			else shot[playerid]=0;
			if(shot[playerid] > 2)
			{
				ToCheat(1,playerid, 2);
				return 0;
			}
			shotTime[playerid] = CurrentTimer;
		}
	}
	switch(weaponid)
	{
	    case 23, 24, 25, 34:
	    {
	        new server_tick = GetTickCount();
	        if(!IsPlayerInAnyVehicle(playerid))// Игрок не находится в транспорте ( выстрелы с оружия "Desert Eagle" достаточно быстрые, если игрок стреляет с пассажирского места из транспорта )
	        {
		        
		        if(GetTickDiff(server_tick, FirstFire[playerid]) < 300 // Последний выстрел игрока из оружия
		        && GetTickDiff(server_tick, LastPressedC[playerid]) > 1000) // Последнее нажатие клавиши 'С'
		        {
		            player_shot_count[playerid]= player_shot_count[playerid] + 2;
					if(player_shot_count[playerid] >= 3)// Количество выстрелов за некоторое время
					{
			            new str[75];
			            format(str, 75, "%s (%i) вероятно использует Auto +C / Rapid Fire.", Name(playerid), playerid);
			            SendAdminsMessage(COLOR_RED,str);
		            }
		            player_shot_count[playerid] --;

					FirstFire[playerid] = server_tick; // Перехватываем время, последнего выстрела игрока из оружия
     				return false;// не даем пули пройти
		        }
	        }
	        FirstFire[playerid] = server_tick; // Перехватываем время, последнего выстрела игрока из оружия
	        if(player_shot_count[playerid] > 0)
			{
				player_shot_count[playerid] --;
				//return false;// не даем пули пройти
			}
	    }
	}
	if(hittype != BULLET_HIT_TYPE_NONE)
	{
		if((fX <= -1000.0 || fX >= 1000.0) || (fY <= -1000.0 || fY >= 1000.0) || (fZ <= -1000.0 || fZ >= 1000.0) || ((hittype != BULLET_HIT_TYPE_PLAYER) && (hittype != BULLET_HIT_TYPE_VEHICLE) && (hittype != BULLET_HIT_TYPE_OBJECT) && (hittype != BULLET_HIT_TYPE_PLAYER_OBJECT)))
		{
			ToCheat(1,playerid, 4);
			return 0;
		}
	}
	if(hittype==BULLET_HIT_TYPE_PLAYER)
	{
		if(fX>=10 || fY>=10 || fZ>=10 || fX<=-10 || fY<=-10 || fZ<=-10)
		{
			ToCheat(1,playerid, 5);
			//BanEx(playerid,"Bullet Crasher");
			return 0;
		}
	}
	if(hittype < 0 || hittype > 4) return 0;

	if(weaponid > 47 && hittype == 0 && fX == 0.0000 && hitid == 0 && fY == 0.0000 && fZ == 0.0000)
	{
		ToCheat(1,playerid, 6);
		return 0;
	}
	if(GetPlayerState(playerid)==3 && GetPlayerWeapon(playerid)==24)
	{
		new Float:X,Float:Y,Float:Z;
		GetPlayerPos(playerid,X,Y,Z);
		J_SetPlayerPos(playerid,X,Y,Z+2);
	}
	/*	format(string,sizeof(string),"OnPlayerWeaponShot(%d, %d, %d, %d, %f, %f, %f) #5",playerid, weaponid, hittype, hitid, fX, fY, fZ);
	SendClientMessage(playerid,-1,string);*/
	if (IsPlayerInRangeOfPoint(playerid, 450.0, -652.2886,-2120.5735,1608.5018) || HellsGateOpen)
	{
		if(BULLET_HIT_TYPE_PLAYER == hittype)
		{
			return false;
		}
		if(IsPlayerInRangeOfPoint(playerid, 5.0, -899.2206,-2090.9507,1610.0308))
		{
			SendClientMessage(playerid,COLOR_GREY,"[Мысли] Похоже это место блокирует любой выстрел");
			SetPlayerArmedWeapon(playerid,0);
			return false;
		}
		if(BULLET_HIT_TYPE_OBJECT == hittype)
		{
			new amount = 0;
			switch(weaponid)
			{
			case 23: amount = 50; // Silenced Colt
			case 24: amount = 75; // Desert Eagle
			case 25: amount = 100; // Pump Shotgun
			case 26: amount = 50; // Двухручной шотган
			case 27: amount = 60; // Spaz 12
			case 28: amount = 30; // Uzi
			case 29: amount = 30; // MP5
			case 30: amount = 40; // AK
			case 31: amount = 35; // M4
			case 32: amount = 30; // Tec 9
			case 33: amount = 100; // Cunt Gun
			case 34: amount = 300; // Sniper Rifle
			default: // тут миниган тоже
				{
					//if(weaponid <9){amount = 30;}
					/*	else{*/
					amount = 8;/*}*/
				}
			}
			for (new j=0;j<MAX_ZOMBIES_OBJECT;j++)
			{
				if(ZombieNeedToBeSpawned[j] != 1)
				{
					for (new i=0;i<6;i++)
					{
						if(hitid == ObjectZombie[j][i])
						{
							GivePlayerPoints(playerid, amount);
							new health =ObjectZHealth[j]-amount;
							if(health <= 0)
							{
								OnZombieDeath(j,playerid);
								break;
							}
							ObjectZHealth[j] = health;
							break;
						}
					}
				}
			}
		}
	}
	new NewAmmo = GetPlayerAmmo(playerid);
	new slot = GetWeaponSlot(weaponid);
	if(slot == 0xFFFF) return true;
	if(AmmoInfo[playerid][slot] > NewAmmo)
	{
		//SendClientMessage(playerid,-1,"Засчитываем тебе выстрел для скиллов...");
		UpdatePlayerSKill(playerid,weaponid);
	}
	pTemp[playerid][Anti_Pvar][44] = CurrentTimer+5;
	GunIdToCheck[playerid] = weaponid;
	
	if(gmd[playerid])
	{
		if(weaponid > 18)
		{
			CreateExplosion(fX,fY,fZ,12,3.0);
		}
	}
	return 1;
}
