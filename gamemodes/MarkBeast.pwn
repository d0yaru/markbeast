// свободный номер: 53
ToCheat(type,playerid, code = 0)
{
	new virtmir = GetPlayerVirtualWorld(playerid);
	if(virtmir == (playerid + 5) || virtmir == 1228) return true;
	if(type != 0)
	{
		if(code == 79)
		{
			type = 0;
		}
		else
		{
			if(code >= 107)
			{
				if(NewAcEnabled == 0)
				{
					type = 0;
				}
				else
				{
					ShowPlayerDialogEx(playerid,0,0,"Античит","Внимание! Код античита, который вас посадил все еще проходит тестирование\nЕсли он сработал ложно, опишите\
					во всех деталях в группу\nто что вы делали перед тем, как вас сюда посадило.\n	vk.com/markbeast","Ок","");
				}
			}
		}
	}

	if(type == 1)
	{
		if(StatusAntiCheat > 1)
		{
			if(code == StatusAntiCheat) return true;
		}

		if(StatusAntiCheat == 0 || StatusAntiCheat > 1)
		{
			for(new i;i<10;i++)
			{
				if(ChetInfo[i][cheatid1]==playerid)
				{
					TextDrawSetString(CheatText[i], "-1");
					Cheat1 ++;
					if(Cheat1 > 9) Cheat1 = 0;
				}
			}

			if(PlayerInfo[playerid][pView] > 0) return true;
			//	if(!strcmp(PN(playerid),"Mr_Root",false)) return true;
			if(BuyCar[playerid] != -1)
			{
				if(VehicleToBuy[playerid] != INVALID_VEHICLE_ID) {DestroyVehicle(VehicleToBuy[playerid]);}
				VehicleToBuy[playerid] = INVALID_VEHICLE_ID;

				SetCameraBehindPlayer(playerid);
				TogglePlayerControllable(playerid, 1);
				Fix_SetPlayerVirtualWorld(playerid,0);
				BuyCar[playerid] = -1;
				OlVirtualWorld[playerid] = -1;
				for(new i;i<sizeof(Skinfon);i++)
				{
					TextDrawHideForPlayer(playerid,Skinfon[i]);
				}

				TextDrawHideForPlayer(playerid,ButtonLeft);
				TextDrawHideForPlayer(playerid,ButtonRight);
				TextDrawHideForPlayer(playerid,ButtonCancel);
				TextDrawHideForPlayer(playerid,SelectSkin);
				PlayerTextDrawHide(playerid,InfoText[playerid]);
				CancelSelectTextDraw(playerid);

			}
			new string [90];
			format(string, sizeof(string), "Вы были отправлены в преисподнюю по подозрению в читерстве (Код 0%d)", code);
			SendClientMessage(playerid,COLOR_LIGHTRED,string);
			SendClientMessage(playerid, COLOR_LIGHTRED,"Если вы уверены, что это ошибка, и вы не использовали чит-программ, то сообщите об этом в группу /vk.com/markbeast.");


			SetSpawnInfo(playerid, 255, 8, -1685.1571,1097.7434,1501.0859,353.5934, 0, 0, 0, 0, 0, 0);

			PlayerPlaySound(playerid, 1058, 0.0, 0.0, 0.0);
			_ResetGun(playerid);
			PlayerInfo[playerid][pView] = 10*60;
			PlayerInfo[playerid][pWanted] = 0;
			J_SetPlayerHealth(playerid,100);
			PlayerSpawn(playerid);

			Fix_SetPlayerVirtualWorld(playerid,1229);
			
			LastPlayerACJailed[playerid] = code;
		}
		if(StatusAntiCheat == 1)
		{
			new string [90];
			format(string, sizeof(string), "Вы были кикнуты по подозрению в читерстве (Код 0%d)", code);
			SendClientMessage(playerid,COLOR_LIGHTRED,string);
			SendClientMessage(playerid, COLOR_LIGHTRED,"Если вы уверены, что это ошибка, и вы не использовали чит-программ, то сообщите об этом в группу /vk.com/markbeast.");

			J_Kick(playerid);
		}
	}
	else
	{
		if(PlayerInfo[playerid][pView] > 0) return true;
		if(PlayerInfo[playerid][pAdmin] > 0) return true;
		AddCheater(playerid);
	}
	return 1;
}
TogglePlayerMap(playerid, toggle)
{
	if(toggle)
	{
		if(!ShowMap[playerid])
		{
			TextDrawShowForPlayer(playerid, Map[0]);
			TextDrawShowForPlayer(playerid, Map[1]);
			TextDrawShowForPlayer(playerid, Map[2]);
			TextDrawShowForPlayer(playerid, Map[3]);
			ShowMap[playerid] = true;
			foreach(i)
			{
				TextDrawShowForPlayer(playerid, PlayerDraw[i]);
				TextDrawHideForPlayer(playerid, StatsDraw[i]);
			}
			SendClientMessage(playerid, COLOR_GREY, "Вы открыли карту штата. Чтобы закрыть её нажмите {FFFFFF}'ESC'");
			SelectTextDraw(playerid, 0xFFFFFFAA);
		}
	}
	else
	{
		for(new i;i < MAX_PLAYERS; i ++)
		{
			TextDrawHideForPlayer(playerid, PlayerDraw[i]);
			TextDrawHideForPlayer(playerid, StatsDraw[i]);
		}
		TextDrawHideForPlayer(playerid, Map[0]);
		TextDrawHideForPlayer(playerid, Map[1]);
		TextDrawHideForPlayer(playerid, Map[2]);
		TextDrawHideForPlayer(playerid, Map[3]);
		ShowMap[playerid] = false;
	}
	return true;
}
CheckMapCords(Float:x, Float:y, &Float:X, &Float:Y)
{
	if(y < -3000) y = -3000;
	else if(y > 3000) y = 3000;
	if(x < -3000) x = -3000;
	else if(x > 3000) x = 3000;
	if(y > -3000 && y < 3000 && x > -3000 && x < 3000)
	{
		if(y < 0) Y = 207 + 224 * -y / 3000;
		else if(y >= 0) Y = 207 - 224 * y / 3000;
		if(x < 0) X = 318 - -x * 320 / 3000;
		else if(x >= 0) X = 318 + x * 320 / 3000;
	}
	return true;
}
CreatePlayerMapDraws(playerid)
{
	if(NeedDell[playerid]==false)
	{
		if(IsACop(playerid) || IsAArmy(playerid))
		{
			new string[50], Float:x, Float:y, Float:z, Float:Pos_X, Float:Pos_Y;
			GetPlayerPos(playerid, x, y, z);
			CheckMapCords(x, y, Pos_X, Pos_Y);

			new ss=0;
			foreach(i)
			{
				if(ShowMap[i])
				{
					if(PlayerInfo[playerid][pMember]==PlayerInfo[i][pMember])
					{
						if(ss==0)
						{
							new team = GetTeamID(playerid);
							PlayerDraw[playerid] = TextDrawCreate(Pos_X, Pos_Y, "-");
							TextDrawFont(PlayerDraw[playerid], 1);
							TextDrawLetterSize(PlayerDraw[playerid], 0.49, 2.9);
							TextDrawColor(PlayerDraw[playerid], TeamColors[team]);
							TextDrawSetOutline(PlayerDraw[playerid], 1);

							format(string, sizeof(string), "ID: %i ~y~ %s", playerid, PN(playerid));
							StatsDraw[playerid] = TextDrawCreate(Pos_X+10, Pos_Y+8, string);
							TextDrawFont(StatsDraw[playerid], 1);
							TextDrawLetterSize(StatsDraw[playerid], 0.16, 0.6);
							TextDrawColor(StatsDraw[playerid], 0xFFFFFFFF);
							TextDrawSetOutline(StatsDraw[playerid], 1);
							ss++;
						}
						TextDrawShowForPlayer(i, PlayerDraw[playerid]);
						TextDrawShowForPlayer(i, StatsDraw[playerid]);
						NeedDell[playerid]=true;
					}
				}
			}
		}
	}
	return true;
}
DestroyPlayerMapDraws(playerid) //на дисконект
{
	if(NeedDell[playerid]==true)
	{
		TextDrawHideForAll(PlayerDraw[playerid]);
		TextDrawDestroy(PlayerDraw[playerid]);
