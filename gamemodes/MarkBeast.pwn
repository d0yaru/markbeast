CMD:passwd(playerid, params[])
{
	new oldpass[50],newpass[50],newpass2[50], string[256];
	if(sscanf(params, "s[50]s[50]s[50]", oldpass, newpass, newpass2)) return SendClientMessage(playerid,COLOR_LIGHTBLUE,"/passwd [oldpass] [newpass] [newpass2]");
	format(string, sizeof(string), "SELECT `Password` FROM `usrboston` WHERE `NickName` = '%s' LIMIT 1", PN(playerid));
	new Cache:result = mysql_query(mysql, string); cache_get_row(0, 0, string, mysql); cache_delete(result, mysql);
	if(strcmp(MD5_Hash(oldpass), string, true)!=0) return SendClientMessage(playerid,-1,"Текущий пароль с введенным вами не совпадает!");
	mysql_real_escape_string(newpass, newpass);
	if(strcmp(newpass, newpass2, true)!=0) return SendClientMessage(playerid,-1,"Новые пароли между собой не совпадают!");
	new query[256];
	format(query, sizeof(query), "UPDATE `usrboston` SET `Password`=MD5('%s') WHERE `NickName` = '%s'", newpass, PN(playerid));
	mysql_tquery(mysql, query);
	query = "";
	format(query, sizeof(query),"Ваш новый пароль: "COLOR_PROJECT_EX"%s{FFFFFF} | сделайте скриншот клавишей "COLOR_PROJECT_EX"F8",newpass);
	SendClientMessage(playerid,-1,query);
	return 1;
}
