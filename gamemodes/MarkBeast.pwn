public MysqlCheckNickReferal(playerid, inputtextsave[])
{
	new string[256];
	if(!strlen(inputtextsave) || ( cache_get_row_count(mysql)>0 && !GetString(inputtextsave, PlayerName[playerid]) ) )
	{
		RegisterState[playerid] = 0;
		if(!strlen(inputtextsave)) SetString(RegReferal[playerid],"No Referal");
		else
		{
			mysql_real_escape_string(inputtextsave, RegReferal[playerid]);
			new refid = GetNameID(RegReferal[playerid]);
			if(refid != INVALID_PLAYER_ID)
			{
				format(string, sizeof(string), Language[49][lang], PN(playerid));
				SendClientMessage(refid, COLOR_WHITE, string);
				SendClientMessage(refid, COLOR_PROJECT, Language[50][lang]);
			}
		}
		
	 	Retry:
		RegisterState[playerid] = 0;
		OnPlayerRegister(playerid, RegPass[playerid]);
		//OnPlayerLogin(playerid);
		ClearChatForPlayer(playerid);
		format(string,sizeof(string),Language[48][lang],PlayerName[playerid], playerid);
		SendAdminsMessage(COLOR_WHITE,string);


		SetFullness(playerid, 100);
		PlayerInfo[playerid][pBuyWalent]=1;
		Hunger(playerid,0);
		PlayerInfo[playerid][pGold]=1;
		IncReset[playerid]=1; // диалог
		PlayerInfo[playerid][pMoney]=500;
		PlayerInfo[playerid][pZKP]=20;
		PlayerInfo[playerid][pRedio]=3;
		J_SetPlayerHealth(playerid,100);
		
		for(new i = 0; i < sizeof(YoutubeNames); i++)
		{
		    if(strfind(YoutubeNames[i], RegReferal[playerid], true) != -1)
		    {
		        PlayerInfo[playerid][pGetbonus] = 2;
		        PlayerInfo[playerid][pEagle_Skill] = 10000;
		      //  printf("%s и %s еее рок",YoutubeNames[i], RegReferal[playerid]);
		        break;
		    }
		    //printf("%s и %s",YoutubeNames[i], RegReferal[playerid]);
		}
		
	}
	else
	{
	    new FixedName[MAX_PLAYER_NAME+3];
	    mysql_real_escape_string(inputtextsave, FixedName);
		for(new i = 0; i < sizeof(YoutubeNames); i++)
		{
		    if(strfind(YoutubeNames[i], FixedName, true) != -1)
		    {
		        PlayerInfo[playerid][pGetbonus] = 2;
		        
		        mysql_real_escape_string(YoutubeNames[i], RegReferal[playerid]);
		        goto Retry;
		    }
		}
		ShowPlayerDialogEx(playerid,1,DIALOG_STYLE_INPUT,"{ffffff}Пригласивший "COLOR_PROJECT_EX"| {ffffff}Шаг:"COLOR_PROJECT_EX" 4{ffffff}/4" ,"{ffffff}Введите ник пригласившего либо промокод ютубера или нажмите пропустить\nОшибка! Указанный игрок не найден.\n\nВнимание! Размер букв имеет значение!",Language[6][lang],"Пропустить");
	}
	return 1;
}
