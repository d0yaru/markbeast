	new string[350];
	if(PlayerInfo[playerid][pAdmin] < 2) return SendClientMessage(playerid,COLOR_GREY,"У вас недостаточно прав!");
	if(ADuty[playerid]!=228) return cmd_apanel(playerid,"");
	new id, veh;
	if(sscanf(params, "dd", id,veh))
	{
		id=playerid;
		veh = 411;
	}
	if(veh < 400 || veh > 611) return SendClientMessage(playerid, COLOR_PROJECT, "Номер Транспортного средства не может быть ниже 400 или выше 611 !");
	if(veh==538 || veh==449 || veh==570) return 1;
	if(GetPlayerState(id)!=1) return SendClientMessage(playerid,COLOR_GREY,"Не нехочу!");
	new Float: XYZ[4];
	GetPlayerPos(id, XYZ[0], XYZ[1], XYZ[2]); GetPlayerFacingAngle(id, XYZ[3]);
	if(PlayerInfo[id][plveh]!=-1)DestroyVehicle(PlayerInfo[id][plveh]);
	PlayerInfo[id][plveh] = J_CreateVehicle(veh, XYZ[0], XYZ[1], XYZ[2], XYZ[3], -1, -1, -1);
	VehicleInfo[PlayerInfo[id][plveh]][vAntiRepair] = 2;
	VehicleHealth[PlayerInfo[id][plveh]] = 1000;
	SetVehicleVirtualWorld(PlayerInfo[id][plveh], GetPlayerVirtualWorld(playerid));
	LinkVehicleToInterior(PlayerInfo[id][plveh], GetPlayerInterior(playerid));
	AC_PutPlayerInVehicle(id, PlayerInfo[id][plveh], 0);
	format(string,sizeof(string),"[A] Администратор %s[%d] выдал временную машину игроку %s[%d] [%d]",PN(playerid),playerid,PN(id),id,PlayerInfo[id][plveh]);
	SendAdminsMessage(COLOR_GREY,string);
	if(!IsAVelik(PlayerInfo[id][plveh]))SetEngineStatus(PlayerInfo[id][plveh],false, playerid);

	Fuel[PlayerInfo[id][plveh]] = GasMax;
	return 1;
}
