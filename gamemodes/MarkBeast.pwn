		format(string,sizeof(string),"Поздравляем! Вы достигли %d-го уровня!",PlayerInfo[playerid][pLevel]);
		SendClientMessage(playerid,COLOR_PROJECT,string);

		switch(PlayerInfo[playerid][pLevel])
		{
		case 10,25,40:
			{
				DeletePass(playerid);
				SendClientMessage(playerid,-1,"Вам пора менять паспорт! Сделать это можно в мэрии");
			}
		}
		if(PlayerInfo[playerid][pLevel] >= 3)
		{
		    if(PlayerInfo[playerid][pGetbonus] != 5 && PlayerInfo[playerid][pGetbonus] >= 2)
		    {
			    PlayerInfo[playerid][pGetbonus] = 5;
				GiveMoney(playerid,50000);
				PlayerInfo[playerid][pLighter] = 100;
				PlayerInfo[playerid][pSigs] =20-PlayerInfo[playerid][pSigs];
				SendClientMessage(playerid,-1,"Вы получили 50 000$, пачку сигарет и зажигалку по промокоду!");
