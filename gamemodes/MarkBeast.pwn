CMD:hangup(playerid, params[])
{
	if(PlayerInfo[playerid][pCalling] == INVALID_PLAYER_ID)
	{
		if(PlayerInfo[playerid][pCalled] != INVALID_PLAYER_ID)
		{
			new PlayerWhoCall = PlayerInfo[playerid][pCalled];
			if(PlayerInfo[PlayerWhoCall][pCalling] == playerid)
			{
				PlayerInfo[PlayerWhoCall][pCalling] = INVALID_PLAYER_ID;
				PlayerInfo[PlayerWhoCall][pCalled] = INVALID_PLAYER_ID;

				SendClientMessage(PlayerWhoCall,  COLOR_GRAD, "Абонент занят...");
				SetPlayerSpecialAction(PlayerWhoCall,SPECIAL_ACTION_STOPUSECELLPHONE);
				RemovePlayerAttachedObjectEx(playerid, 6);

				SendPlayerMe(playerid, "отклонил(а) звонок");

				PlayerInfo[playerid][pONPhoneTalk] = 0;
				PlayerInfo[PlayerWhoCall][pONPhoneTalk] = 0;
			}
			PlayerInfo[playerid][pCalled] = INVALID_PLAYER_ID;
			return true;
		}
		//	SendClientMessage(playerid, COLOR_GRAD, "Вы ни с кем не разговариваете.");
		return true;
	}
	new otherplayer = PlayerInfo[playerid][pCalling];

	PlayerInfo[playerid][pCalling] = INVALID_PLAYER_ID;
	PlayerInfo[playerid][pONPhoneTalk] = 0;
	PlayerInfo[playerid][pCalled] = INVALID_PLAYER_ID;

	PlayerInfo[otherplayer][pCalling] = INVALID_PLAYER_ID;
	PlayerInfo[otherplayer][pONPhoneTalk] = 0;
	PlayerInfo[otherplayer][pCalled] = INVALID_PLAYER_ID;

	SetPlayerSpecialAction(playerid,SPECIAL_ACTION_STOPUSECELLPHONE);
	SetPlayerSpecialAction(otherplayer,SPECIAL_ACTION_STOPUSECELLPHONE);
	RemovePlayerAttachedObjectEx(playerid, 6);
	RemovePlayerAttachedObjectEx(otherplayer, 6);

	SendClientMessage(playerid,  COLOR_GRAD, "Вы положили трубку");
	SendClientMessage(otherplayer,  COLOR_GRAD, "Абонент положил(a) трубку");
	SendPlayerMe(playerid, "положил(а) трубку");
	return true;
}
