public OnPlayerStateChange(playerid, newstate, oldstate)
{
	if(IsPlayerNPC(playerid)) return true;
	PlayerInfo[ playerid ][ CheatTime ] = CurrentTimer + 3;

	new string[700];
	new vehid = GetPlayerVehicleID(playerid);
	if(newstate == PLAYER_STATE_DRIVER || newstate == PLAYER_STATE_PASSENGER)
	{
	    if(EnterVehT[playerid] != 1)
	    {
		    new Difference = GetTickDiff(GetTickCount(), EnterVehT[playerid]);

		    new MaxDifference = 250;
	 		if(IsABoat(vehid))
	   		{
	     		MaxDifference = 50;
		    }
		    else
		    {
		        if(IsAVelik(vehid) || IsABike(vehid) || IsAMoped(vehid))
		        {
		            MaxDifference = 100;
		        }
		        else
		        {
			   		MaxDifference = 250;
	   			}
		   	}
			if(Difference < MaxDifference || !EnterVehT[playerid])
			{
	  			new ping = GetPlayerPing(playerid);
			   	if(ping > 100)
			   	{
			   	    string = "";
			    	format(string, sizeof(string), "%s[%d] Возможно посажен ложно по коду 107 ибо его пинг %d, сел в машину за %d милисек", Name(playerid),playerid,ping,Difference);
					SendAdminsMessage(COLOR_LIGHTRED,string);
			   	}

			    string ="";
			    format(string,sizeof(string),"%s | Level: %d | VehModel: %d (%d) | EnterVeh > State == %d MS",Name(playerid),PlayerInfo[playerid][pLevel],GetVehicleModel(vehid),vehid,Difference);
			    WriteACLog(playerid,107,string);
				ToCheat(1,playerid, 107);
				if(newstate == PLAYER_STATE_DRIVER)
				{
				    J_SetVehicleHealth(vehid,1000);
					J_SetVehiclePos(vehid,VehicleInfo[vehid][vPos_X],VehicleInfo[vehid][vPos_Y],VehicleInfo[vehid][vPos_Z]);
				}
			}
		}
		if(GetPlayerWeapon(playerid) == 24) SetPlayerArmedWeapon(playerid,0);
		
		EnterVehT[playerid] = 0;
		

		new Float:XY[3];
		GetVehiclePos(vehid,XY[0],XY[1],XY[2]);

		PlayerDistToCar[playerid] = GetDistance(PlayerInfo[ playerid ][ BeforeOldPos ][ 0 ], PlayerInfo[ playerid ][ BeforeOldPos ][ 1 ], XY[0], XY[1]); // перед ем как сел в машину и коорды машины


		if(PlayerDistToCar[playerid] > 20)
		{
			if(newstate == PLAYER_STATE_DRIVER)
			{
				if(OlVirtualWorld[playerid] == -1)
				{
					format(string, sizeof(string), "%s[%d] Возможно раскидывает машины! (ТП в Авто | Дистанция: %.1f) Код 105", Name(playerid),playerid,PlayerDistToCar[playerid]);
					SendAdminsMessage(COLOR_LIGHTRED,string);
				}
			}
			else
			{
			    new model  = GetVehicleModel(vehid);
			    if(model!=570 && model!=449)
			    {
					VehWarnings[playerid] = VehWarnings[playerid] + 2;
					if(VehWarnings[playerid] > 2)
					{
						format(string, sizeof(string), "%s[%d] Возможно раскидывает машины! (ТП в Авто | Дистанция: %.1f) Код 106", Name(playerid),playerid,PlayerDistToCar[playerid]);
						SendAdminsMessage(COLOR_LIGHTRED,string);
					}
				}
			}
		}


		pTemp[playerid][Anti_Pvar][47] = GetTickCount();//+1000;
		OldHealthofVeh[playerid] = VehicleHealth[vehid];

		VehID[playerid] = vehid;
		if(VehicleInfo[VehID[playerid]][NotToRide] ==1) return ToCheat(1,playerid, 78);
	}
	else if(oldstate == PLAYER_STATE_DRIVER || oldstate == PLAYER_STATE_PASSENGER)
	{
		new tempvehid = VehID[playerid];
		VehID[playerid] = INVALID_VEHICLE_ID;
		///////////////////////////////////
		if(oldstate == PLAYER_STATE_DRIVER)
		{
			leavevehicle[playerid] = tempvehid; // отдельный ач
			pTemp[playerid][Anti_Pvar][46] = CurrentTimer+5;
		}
		///////////////////////////////////

		if(VehWarnings[playerid] > 0)
		{
			VehWarnings[playerid] --;
		}


		new CarInOutTimeOut = GetTickDiff(GetTickCount(), pTemp[playerid][Anti_Pvar][47]);


		if(CarInOutTimeOut < 1000)// если за 1000 мс он пошел и вышел.
		{
			if(oldstate == PLAYER_STATE_DRIVER)
			{
				if(PlayerDistToCar[playerid] > 20) // машина была дальше от него аж на 20 метров
				{

					new Float:Velocity[3];
					GetPlayerVelocity(playerid, Velocity[0], Velocity[1], Velocity[2]);

					new Float:VehVelocity[3];
					GetVehicleVelocity(tempvehid, VehVelocity[0], VehVelocity[1], VehVelocity[2]);

					string = "";
					format(string,sizeof(string),"PlayerDistToCar: %.1f | TickCount: %d | PlayerLevel: %d | Name: %s | %.1f %.1f %.1f | veh %.1f %.1f %.1f",PlayerDistToCar[playerid],
					CarInOutTimeOut,PlayerInfo[playerid][pLevel],Name(playerid),
					Velocity[0], Velocity[1], Velocity[2],
					VehVelocity[0], VehVelocity[1], VehVelocity[2]);

					WriteACLog(playerid,103,string);


					J_SetVehicleHealth(tempvehid,1000);
					if(tempvehid != BotVehicles[0] &&  tempvehid != BotVehicles[1])
					{
						J_SetVehiclePos(tempvehid,VehicleInfo[tempvehid][vPos_X],VehicleInfo[tempvehid][vPos_Y],VehicleInfo[tempvehid][vPos_Z]);
					}
					ToCheat(1,playerid, 103);

