CMD:incar(playerid, params[])
{
	new id,mesto,string[200];
	if(sscanf(params,"ud",id,mesto) || mesto < 1 || mesto > 3) return SendClientMessage(playerid, COLOR_PROJECT, "Используйте:{FFFFFF} /incar [id] [id места(1-3)]");
	if(!ProxDetectorS(4,playerid,id)) return SendClientMessage(playerid, COLOR_GREY, "Вы далеко от игрока");
	if(!TeamDuty[playerid]) return SendClientMessage(playerid,COLOR_RED,"Вы не на дежурстве!");
	if(IsPlayerInAnyVehicle(id)) return SendClientMessage(playerid,COLOR_RED,"Этот игрок уже в машине!");
	new imcop = IsACop(playerid);
	new cuffed = (imcop && Cuffed[id]);
	if( !cuffed ) return SendClientMessage(playerid, COLOR_GREY, "Этот игрок не обездвижен");
	for(new i = 1;i < ADD_VEHICLES;i ++)
	{
		if(!IsACar(i)) continue;
		if(GetDoorsStatus(i)) continue;
		if(GetVehicleSeat(i, mesto) != INVALID_PLAYER_ID) continue;
		if( mesto > GetVehicleMaxSeat(i) ) continue;
		if( !( IsACopCar(i) && imcop )) continue;
		new Float:X,Float:Y,Float:Z;
		GetVehiclePos(i,X,Y,Z);
		if(!IsPlayerInRangeOfPoint(playerid,2.5,X,Y,Z)) continue;
		AC_PutPlayerInVehicle(id, i, mesto);
		TogglePlayerControllableEx(id, false);
		format(string,sizeof(string)," %s затащил игрока %s в машину.",cPN(playerid),cPN(id));
		ProxDetector(25,playerid,string,COLOR_PURPLE);
		break;
	}
	return true;
}
