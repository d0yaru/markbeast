CMD:geton(playerid, params[])
{
	if(PlayerInfo[playerid][pAdmin] >=4)
	{
		new Name[24];
		if(sscanf(params,"s[24]",Name)) return SendClientMessage(playerid,COLOR_PROJECT,"Используйте:{FFFFFF} /geton [Ник]");
		new str[100],Day,Month,Year,Minute,Hour,IP[16],Time,TimePlayedTodat;//Second,
		CMDstr = "";
		format(CMDstr, sizeof(CMDstr), "SELECT * FROM `onlineinfo` WHERE `Name` = '%s' LIMIT 1", Name);
		new Cache:result = mysql_query(mysql, CMDstr);
		if(cache_get_row_count(mysql) > 0)
		{
			cache_get_row(0, 0, Name, mysql);
			cache_get_row(0, 1, IP, mysql);
			//Second = cache_get_row_int(0, 2, mysql);
			Minute = cache_get_row_int(0, 3, mysql);
			Hour = cache_get_row_int(0, 4, mysql);
			Day = cache_get_row_int(0, 5, mysql);
			Month = cache_get_row_int(0, 6, mysql);
			Year = cache_get_row_int(0, 7, mysql);
			Time = cache_get_row_int(0, 8, mysql);
			TimePlayedTodat = cache_get_row_int(0, 9, mysql);
			TimePlayedTodat = TimePlayedTodat / 60;

			CMDstr = "";
			format(str,sizeof(str),"{FFFFFF}Ник: {1578af}%s\n",Name);
			strcat(CMDstr,str);
			format(str,sizeof(str),"{FFFFFF}Последнее посещение: {1578af}%d/%02d/%02d в %d:%02d\n",Day,Month,Year,Minute,Hour);
			strcat(CMDstr,str);
			format(str,sizeof(str),"{FFFFFF}Время пребывания: {1578af}%d минут(ы/у)\n",Time/60);
			strcat(CMDstr,str);
			format(str,sizeof(str),"{FFFFFF}IP адрес: {1578af}%s\n",IP);
			strcat(CMDstr,str);
			format(str,sizeof(str),"{FFFFFF}Сегодня играл: {1578af}%d минут\n",TimePlayedTodat);
			strcat(CMDstr,str);

			ShowPlayerDialogEx(playerid,0,0,"Информация об игроке",CMDstr,"Ясно","Понятно");
		}
		else
		{
			SendClientMessage(playerid,COLOR_GREY, "Об этом игроке нет информации");
		}
		cache_delete(result, mysql);
	}
	return true;
