void OnPlayerUpdatePlayer(playerid)
{
	if (IsPlayerNPC(playerid)) return true;
	if(IsPlayerLogged[playerid])// если атворизовался
	{
		new string[158];
		if(PlayerAFKTime[playerid][0] > 10)
		{
			/*if(PlayerAFKTime[playerid][0] <= MAX_AFK_TIME) */
			format(string,sizeof(string),"Вы простояли в AFK "COLOR_PROJECT_EX"%s.",Convert(PlayerAFKTime[playerid][0]));//{FF9900}
			/*else format(string,sizeof(string),"{FFFFFF}Вы простояли в AFK "COLOR_PROJECT_EX_OLD"%s. {FFFFFF}Из них "COLOR_PROJECT_EX_OLD"%s{FFFFFF} не засчитаны!",Convert(PlayerAFKTime[playerid][0]), Convert(PlayerAFKTime[playerid][0]-MAX_AFK_TIME));*/
			SendClientMessage(playerid, -1, string);
			SetPlayerChatBubble(playerid, " ", COLOR_WHITE, 30.0, 1);
		}
		PlayerAFK[playerid] ++;
		PlayerAFKTime[playerid][0] = 0;


		WFN_OnPlayerUpdate(playerid);
		if(SLF_Status[playerid])
		{
			new keys, updown, leftright;
			GetPlayerKeys(playerid, keys, updown, leftright);
			if(leftright == KEY_LEFT)
			{
				SFL_ShowCamera(playerid, 3);
			}
			else if(leftright == KEY_RIGHT)
			{
				SFL_ShowCamera(playerid, -3);
			}

			if(updown == KEY_UP)
			{
				SFL_ShowCamera(playerid, 0.0, 0.1);
			}
			else if(updown == KEY_DOWN)
			{
				SFL_ShowCamera(playerid, 0.0, -0.1);
			}
		}


		if(PlayerInfo[playerid][pView] < 1) // если не в аду
		{
			new VarAnim = GetPlayerAnimationIndex(playerid); // узнаем индекс анимок
			if(VarAnim != 1130)// Если не падение
			{
				GetPlayerVelocity(playerid, VarVelct[0], VarVelct[1], VarVelct[2]);
				VarVelct[3] = floatround(floatsqroot(VarVelct[0] * VarVelct[0] + VarVelct[1] * VarVelct[1] + VarVelct[2] * VarVelct[2]) * 295); // для следующего античита узнаем велосити

				new Float:XY[3];
				GetPlayerPos(playerid,XY[0],XY[1],XY[2]);
				if(GetPlayerSurfingVehicleID(playerid) == INVALID_VEHICLE_ID) // если не НА машине
				{
					if(!AntiTeleport(playerid,71,30.0)) // если анти-телепорт не выявил нарушений
					{
						// другой ач начало
						if(VarVelct[3] > 29)
						{
							if(VarVelct[0] < VarVelct[2] || VarVelct[1] < VarVelct[2])
							{
								if(VarAnim == 1541 || VarAnim == 948)// флай вроде на них нельзя сюрфить машину
								{


									format(string,sizeof(string),"Position: %.3f, %.3f, %.3f | Velocity: %.3f, %.3f, %.3f | PlayerLevel: %d | Name: %s",XY[0],XY[1],XY[2],VarVelct[0], VarVelct[1], VarVelct[2],PlayerInfo[playerid][pLevel],Name(playerid));
									WriteACLog(playerid,74,string);

									VarVelct[3] = 0;
									SetPlayerVelocity(playerid, 0.0, 0.0, 0.0);
									ToCheat(1,playerid, 74);
								}
							}
							// еще один ач
							if(VarVelct[3] > 55)
							{
								if(VarAnim == 1538 || VarAnim == 1539) // флай
								{

									WarningPlayerAntiFLY[playerid] = WarningPlayerAntiFLY[playerid] + 2;
									if(WarningPlayerAntiFLY[playerid] >=3)
									{
										format(string,sizeof(string),"Position: %.3f, %.3f, %.3f | Velocity: %.3f, %.3f, %.3f | PlayerLevel: %d | Name: %s",XY[0],XY[1],XY[2],VarVelct[0], VarVelct[1], VarVelct[2],PlayerInfo[playerid][pLevel],Name(playerid));
										WriteACLog(playerid,72,string);

										VarVelct[3] = 0;
										SetPlayerVelocity(playerid, 0.0, 0.0, 0.0);
										ToCheat(1,playerid, 72);
									}
								}
								else
								{
									if(IsPlayerInBJ[playerid] != 1)
									{
										if(VarVelct[3] > 99)
										{
											if(VarAnim == 1231 || VarAnim == 1266)
											{
												if(CurrentTimer > PlayerInfo[ playerid ][ CheatTime ])
												{


													format(string,sizeof(string),"Position: %.3f, %.3f, %.3f | Velocity: %.3f, %.3f, %.3f | PlayerLevel: %d | Name: %s",XY[0],XY[1],XY[2],VarVelct[0], VarVelct[1], VarVelct[2],PlayerInfo[playerid][pLevel],Name(playerid));
													WriteACLog(playerid,73,string);

													VarVelct[3] = 0;
													SetPlayerVelocity(playerid, 0.0, 0.0, 0.0);
													ToCheat(1,playerid, 73);// возможно проблемный
												}
											}

										}
									}
								}
							}
						}

					}
				}// конец проверки на серф.
				if(VarVelct[3] > 1499)
				{
					if(VarAnim != 959 && VarAnim != 958 && VarAnim != 971)
					{
						format(string,sizeof(string),"Position: %.3f, %.3f, %.3f | Velocity: %.3f, %.3f, %.3f | PlayerLevel: %d | Name: %s",XY[0],XY[1],XY[2],VarVelct[0], VarVelct[1], VarVelct[2],PlayerInfo[playerid][pLevel],Name(playerid));
						WriteACLog(playerid,75,string);
						SetPlayerVelocity(playerid, 0.0, 0.0, 0.0);
						ToCheat(1,playerid, 75);
					}
				}
			}// конец падения
			
			if(WarningPlayerAntiFLY[playerid] >0){WarningPlayerAntiFLY[playerid] --;}
		}// конец проверки на ад
		PlayerInfo[ playerid ][ BeforeOldPos ][ 0 ] = PlayerInfo[ playerid ][ AC_AirBrk ][ 0 ];
		PlayerInfo[ playerid ][ BeforeOldPos ][ 1 ] = PlayerInfo[ playerid ][ AC_AirBrk ][ 1 ];
		GetPlayerPos(playerid, PlayerInfo[ playerid ][ AC_AirBrk ][ 0 ], PlayerInfo[ playerid ][ AC_AirBrk ][ 1 ], PlayerInfo[playerid][ AC_AirBrk ][ 2 ]); // падение
		return true;
	}
	return false;

}
