	format(CMDstr, 110, "%s и %s бросили кости. Результат: {CC9900}%i : %i",Name(inveteid),Name(playerid),dice,dice1);
	ProxDetector(25.0, playerid, CMDstr, COLOR_PURPLE);
	if(dice > dice1)
	{
		new procent = (Invite_To_Dice_Money[inveteid] / 100) * 3;

