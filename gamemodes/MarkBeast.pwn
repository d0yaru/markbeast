	return carid;
}
bool:floatcomp(Float:X1, Float:X2, Float:Delta = 0.05)
{
	return (floatabs(X1-X2) < Delta) ? true : false;
}

IsAtBankomat(playerid,Float:distance)
{
	for(new i = 0;i < sizeof(Bankomats);i ++) if(IsPlayerInRangeOfPoint(playerid,distance,Bankomats[i][0],Bankomats[i][1],Bankomats[i][2])) return true;
	return false;
}
IsAtGang(playerid)
{
	new team = GetTeamID(playerid);
	switch(team)
	{
	case TEAM_GROOVE,TEAM_VAGOS,TEAM_BALLAS,TEAM_CORONOS,TEAM_RIFA: return true;
	default: return false;
	}
	return false;
}

IsAtMafia(playerid)
{
	new team = GetTeamID(playerid);
	switch(team)
	{
	case TEAM_RM,TEAM_YAKUZA,TEAM_LCN: return true;
