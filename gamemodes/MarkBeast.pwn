CMD:sms(playerid, params[])
{
	if(PlayerInfo[playerid][pTelNum] < 1)return	SendClientMessage(playerid, COLOR_GREY, "У вас нет телефона! Купить его можно в 24/7");
	if(!PlayerInfo[playerid][pTogPhone])return ShowPlayerDialogEx(playerid,2050,DIALOG_STYLE_MSGBOX,"Телефон","Телефон выключен\nХотите включить его?","Да","Отмена");
	if(TimeMessage[playerid]>CurrentTimer)return SendClientMessage(playerid,COLOR_GREY,"Отправлять SMS можно раз в 6 секунд!");

	new result[100], number;
	if(sscanf(params,"ds[100]",number,result))  return SendClientMessage(playerid, COLOR_PROJECT, "Используйте {FFFFFF}: /sms [Номер телефона] [Текст]");

	/*if(!IsPlayerConnected(id)) return	SendClientMessage(playerid, COLOR_GREY, "Игрок не в сети.");
	if(id == playerid)  return SendClientMessage(playerid,COLOR_WHITE,"Вы указали свой ID");*/

	if(PlayerInfo[playerid][pMuteTime] > 0) return SendMuteMessage(playerid);

	if(PlayerInfo[playerid][pIn_Skill] <= 0) return SendClientMessage(playerid,  COLOR_GRAD, "Недостаточно средств...");
	if(IsPlayerInHell[playerid] || PlayerInfo[playerid][pView] > 0) return SendClientMessage(playerid,  COLOR_GREY, "Нет сети...");

	if(IsAIP(result) || IsADomen(result))
	{
		SendClientMessage(playerid,COLOR_LIGHTRED,"Возможно, ваше сообщение содержит спам или рекламу.");

		return true;
	}
	if(number == 555555)
	{
		SendAdvert(playerid,result);
		return true;
	}
	foreach (id)
	{
		if(PlayerInfo[id][pTelNum] ==number)
		{
		    if(IsPlayerInHell[id] || PlayerInfo[id][pView] > 0) return SendClientMessage(playerid,  COLOR_GREY, "Аппарат абонента находится вне зоны действия сети.");
			if(!PlayerInfo[id][pTogPhone]) return SendClientMessage(playerid,  COLOR_GREY, "Аппарат абонента выключен или находится вне зоны действия сети.");

			PlayerInfo[playerid][pIn_Skill] -= 1;

			CMDstr = "";
			format(CMDstr, sizeof(CMDstr), "[Входящее]: %s. Отправитель: %s[%d]",result, Name(playerid), PlayerInfo[playerid][pTelNum]);
			SendClientMessage(id, COLOR_ORANGE, CMDstr);
			PlayerPlaySound(id, 1052, 0.0, 0.0, 0.0);
			PlayerPlaySound(playerid, 1052, 0.0, 0.0, 0.0);

			format(CMDstr, sizeof(CMDstr), "[Исходящее]: %s. Получатель: %s[%d]",result, Name(id), number);
			SendClientMessage(playerid,  COLOR_ORANGE, CMDstr);

			SendPlayerMe(playerid,"что-то набирает в телефоне");
			cmd_do(id,"Раздался звук телефона");

			TimeMessage[playerid]=CurrentTimer+6;
			return true;
		}
	}
	SendClientMessage(playerid, COLOR_GREY, "Абонент вне зоны действия сети");
	return true;
}
