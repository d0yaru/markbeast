UpdateRecordTab()
{
	new mostkills = 0,idd = INVALID_PLAYER_ID;
	new mostkills2 = 0,idd2 = INVALID_PLAYER_ID;
	new poinTs[2];

	new Name_1[MAX_PLAYER_NAME];
	format(Name_1,sizeof(Name_1),"/");

	new Name_2[MAX_PLAYER_NAME];
	format(Name_2,sizeof(Name_2),"/");
	foreach(k)
	{
		if(mostkills < MostKills[k])
		{
			mostkills= MostKills[k];
			idd = k;
			poinTs[0] = ZombiePoints[k];
			format(Name_1,sizeof(Name_1),Name(idd));
		}
	}
	foreach(k)
	{
		if(k == idd) continue;
		if(mostkills2 < MostKills[k])
		{
			mostkills2= MostKills[k];
			idd2 = k;
			poinTs[1] = ZombiePoints[k];
			format(Name_2,sizeof(Name_2),Name(idd2));
		}
	}

	new string[126];
	format(string,sizeof(string),"Most kills:~n~1.  %s[%d] - %d~n~2.  %s[%d] - %d",Name_1,poinTs[0],mostkills,Name_2,poinTs[1],mostkills2);
	TextDrawSetString(Textdraw4, string);
	return true;
}
PlayMusicForAllEx(url[], Float:posX = 0.0, Float:posY = 0.0, Float:posZ = 0.0, Float:distance = 50.0, seconds, musicid,j)
{
	foreach(i)
	{
		if(IsPlayerInRangeOfPoint(i, distance, posX, posY, posZ))
		{
			PlayMusicForPlayerEx(i, url, seconds, musicid, posX, posY, posZ, distance,j);
		}
	}
	return true;
}
CanWePlayThat(i,musicid,CurrentTime_Ex)
{
	//	#define MUSIC_ID_RUN 		(1)
	//	#define MUSIC_ID_BITE 		(2)
	// round 3
	// MUSIC_ID_DEATH 4

	if(musicid == MusicID[i]) // самого себя не перебить
	{
		if(AudioStream[i] > CurrentTime_Ex) return false;
	}
	if(MusicID[i] == MUSIC_ID_ROUND) // раунд не перебить
	{
		if(AudioStream[i] > CurrentTime_Ex) return false;
	}
	if(musicid == MUSIC_ID_RUN) // бег не может перебить (см ниже)
	{
		if(MusicID[i] == MUSIC_ID_BITE || MusicID[i] == MUSIC_ID_DEATH) // укус и смерть
		{
			if(AudioStream[i] > CurrentTime_Ex) return false;
		}
	}
	return true;
}
PlayMusicForPlayerEx(i, url[], seconds, musicid, Float:posX = 0.0, Float:posY = 0.0, Float:posZ = 0.0, Float:distance = 50.0,j)
{

	if(!CanWePlayThat(i,musicid,CurrentTimer)) return true;

	ID_Of_Sounder[i] = j;
	MusicID[i] = musicid;
	PlayAudioStreamForPlayer(i, url, posX, posY, posZ,distance+(distance/float(10)) , 1);
	AudioStream[i] = seconds+CurrentTimer;
	return true;
}
PlayMusicForPlayer(i, url[], seconds, musicid)
{

	if(!CanWePlayThat(i,musicid,CurrentTimer)) return true;

	ID_Of_Sounder[i] = -1;
	MusicID[i] = musicid;
	PlayAudioStreamForPlayer(i, url);
	AudioStream[i] = seconds+CurrentTimer;
	return true;
}
