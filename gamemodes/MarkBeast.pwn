CMD:gag(playerid, params[])
{
	if(sscanf(params,"u",params[0])) return SendClientMessage(playerid, COLOR_PROJECT, "Введите: {FFFFFF}/gag [ID/Name]");
	if(!IsPlayerConnected(params[0])) return SendClientMessage(playerid, COLOR_GREY, "Игрок не был найден.");
	if(params[0] == playerid) return SendClientMessage(playerid, COLOR_GREY, "[Мысли] Вставить кляп самому себе в рот было бы странно...");
	if(IsANotControllable(playerid)) return SendClientMessage(playerid,COLOR_GREY,"[Мысли] Я не могу пошевелиться...");
	if(PlayerInfo[params[0]][pMuteTime] > 0) return SendClientMessage(playerid,COLOR_GREY,"[Мысли] У него и так кляп во рту!");
	if(IsAtMafia(playerid))
	{
		if(!IsANotControllable(params[0])) return SendClientMessage(playerid, COLOR_GREY, "[Мысли] Я не могу вставить ему кляп в рот, он не связан.");
		if(!ProxDetectorS(2.0, playerid, params[0])) return SendClientMessage(playerid, COLOR_GREY, "[Мысли] Он слишком далеко.");
		PlayerInfo[params[0]][pMuteTime] = 2281337;
		CMDstr = "";
		format(CMDstr,sizeof(CMDstr),"вставил кляп в рот %s",Name(params[0]));
		SendPlayerMe(playerid,CMDstr);
		return true;
	}
	SendClientMessage(playerid, COLOR_GREY, "Вам не доступна эта команда.");
	return true;
}
