CMD:fwarn(playerid, params[])
{
	if(PlayerInfo[playerid][pLeader] == 0 && PlayerInfo[playerid][pRank] < 9) return SendClientMessage(playerid,COLOR_GREY,"Вы не можете это сделать!");
	new id,reason[150],string[200];
	if(sscanf(params,"us[150]",id,reason)) return SendClientMessage(playerid, COLOR_PROJECT, "Используйте:{FFFFFF} /fwarn [id] [причина]");
	if(!IsPlayerConnected(id)) return SendClientMessage(playerid, COLOR_RED, "Неверный id игрока!");
	if(!IsGiveWarn(playerid, id)) return SendClientMessage(playerid, COLOR_RED, "Вы не можете ему дать выговор!");
	if(PlayerInfo[id][pLeader] > 0 ) return SendClientMessage(playerid,COLOR_RED,"Вы не можете выдать варн лидеру");
	if(PlayerInfo[playerid][pRank]==PlayerInfo[id][pRank])return SendClientMessage(playerid, COLOR_RED, "Вы не можете ему дать выговор!");
	if(IsADomen(reason)  && PlayerInfo[playerid][pAdmin]<6)
	{
		SendClientMessage(playerid,COLOR_RED,"В вашем сообщении обнаружено запрещенное содержание.");

		return false;
	}
	if(IsAIP(reason))return 1;
	PlayerInfo[id][pFWarns] ++;
	format(string,sizeof(string),"Вы дали выговор игроку %s с причиной %s",PN(id),reason);
	SendClientMessage(playerid,COLOR_LIGHTBLUE,string);
	format(string,sizeof(string),"%s дал вам выговор с причиной %s",PN(playerid),reason);
	SendClientMessage(id,COLOR_ADMINRED,string);
	if(PlayerInfo[id][pFWarns] >= 3)
	{
		PlayerInfo[id][pFWarns] = 0;
		SendClientMessage(id,COLOR_GREY,"Вы были автоматически выгнаны из организации т.к у вас больше 2-х выговоров");
		format(string,sizeof(string),"%s покидает нашу организацию т.к у него больше 2-х выговоров",PN(id));
		SendTeamMessage(PlayerInfo[id][pMember],COLOR_GREY,string);
		PlayerInfo[id][pMember] = 0;
		PlayerInfo[id][pDCTime]=0;
		SendClientMessage(id,COLOR_WHITE,">> {1578af} Местом спавна автоматически назначен ЖД Вокзал. Изменить место спавна >> /setspawn!");
		PlayerInfo[id][pRank] = 0;
		PlayerInfo[id][pJob] = 0;
		SetBumSkin(id);
		PlayerSpawn(id);
	}
	return true;
}
