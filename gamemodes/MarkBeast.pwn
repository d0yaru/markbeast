	if(sscanf(params,"ds[150]",is,result)) return  SendBotMessage(playerid, "Вы кажется что-то перепутали");
	if(strlen(result)>10)return  SendBotMessage(playerid, "Номера в нашем штате не могут быть длиннее 10 символов.");
	if(!IsPlayerHaveCar(playerid,is)) return SendBotMessage(playerid, "Вы шутите? Это не ваша машина!");
	if(GetMoney(playerid) < 1000) return SendClientMessage(playerid,-1,"У вас недостаточно денег. Вы можете пополнить свой баланс [/donate]");

	if(strfind(result, "_")!=-1) return SendBotMessage(playerid, "Вы указали в номере символ, который использовать нелья");
	if(strfind(result, "\n")!=-1) return SendBotMessage(playerid, "Вы указали в номере символ, который использовать нелья");
	if(strfind(result, "/")!=-1) return SendBotMessage(playerid, "Вы указали в номере символ, который использовать нелья");
	new number = GetVehicleID(is);

	if(!GetString(CarInfo[number][cOnNumber],"The State"))return SendBotMessage(playerid, "Забавно, но вам уже выдали разрешение на установку номера для этого транспорта");
	if(!GetString(CarInfo[number][cOnRegister],"The State"))return SendBotMessage(playerid, "Забавно, но вам уже выдали разрешение на установку номера для этого транспорта");

	SetString(CarInfo[number][cOnNumber],"The State");
	SetString(CarInfo[number][cOnRegister],"The State");

	SendBotMessage(playerid, "Заявку принял, ожидайте. Обычно их рассматривают очень быстро.");
	RegCarNumber[playerid]=number;
	RegCarID[playerid]= is;
	RegTimeNumber[playerid] = CurrentTimer;
	format(RegNumbers[playerid],128,"%s",result);
	return true;
}
