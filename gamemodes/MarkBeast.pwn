SetPlayerPosAndFaceToPlayer(playerid,toplayerid,Float:Extra = 0.0)
{
	new Float:PosXYZ[3];
	new Float:angle;
	GetPlayerFacingAngle(toplayerid, angle);
	GetPlayerPos(toplayerid,PosXYZ[0], PosXYZ[1],PosXYZ[2]);
