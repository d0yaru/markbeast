	if(PlayerInfo[playerid][pRank]<5)return SendClientMessage(playerid,COLOR_GREY,"Вам не доступна эта команда! (Необходим 5-тый ранг!)");
	if(PlayerInfo[playerid][pMuteTime] > 0) return SendMuteMessage(playerid);
	if(sscanf(params,"s[150]",result)) return SendClientMessage(playerid,COLOR_PROJECT,"Используйте: {FFFFFF}/news [текст]");
	if(IsADomen(result) || IsAMg(result))
	{
		format(string,sizeof(string),"%s %d подозревается в попытке слива! Text: %s",PN(playerid),playerid,result);
		SendAdminsMessage(COLOR_RED,string);
		SendClientMessage(playerid,COLOR_RED,"В вашем сообщении обнаружено запрещенное содержание.");

		return false;
	}
	if(IsAIP(result))return 1;
	new carid = GetPlayerVehicleID(playerid);
	if(IsANewsCar(carid) || IsPlayerInRangeOfPoint(playerid,50,1542.7135,-1360.0950,93.2791))
	{
		format(string,sizeof(string),"[SA News] %s %s %s",GetRankName(TEAM_RADIO,PlayerInfo[playerid][pRank]),Name(playerid),result);
		RsendClientMessageToAll(COLOR_TUPAC,string);
		return true;
	}
	SendClientMessage(playerid, COLOR_GREY,"Чтобы начатть эфир, нужно находится в машине либо здании SA News!");
	return true;
}
