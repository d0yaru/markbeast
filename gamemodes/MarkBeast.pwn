CMD:unfwarn(playerid, params[])
{
	if(PlayerInfo[playerid][pLeader] == 0 && PlayerInfo[playerid][pRank] < 9) return SendClientMessage(playerid,COLOR_GREY,"Вы не можете это сделать!");
	new id,string[200];
	if(sscanf(params,"u",id)) return SendClientMessage(playerid, COLOR_PROJECT, "Используйте:{FFFFFF} /unfwarn [id]");
	if(!IsPlayerConnected(id)) return SendClientMessage(playerid, COLOR_RED, "Неверный id игрока!");
	if(GetTeamID(id) != GetTeamID(playerid)) return SendClientMessage(playerid, COLOR_RED, "Вы не можете снять выговор этому человеку");
	if(PlayerInfo[id][pFWarns] == 0) return SendClientMessage(playerid, COLOR_RED, "У этого человека нет выговоров!");
	if(id==playerid)return SendClientMessage(playerid, COLOR_RED, "Вы не можете снять выговор самому себе!");
	PlayerInfo[id][pFWarns] --;
	format(string,sizeof(string),"Вы сняли выговор игроку %s",PN(id));
