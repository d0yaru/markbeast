	}
	else
	{
		GangZoneHideForPlayer(id, HideMapZone);
		SetCameraBehindPlayer(playerid);
		TogglePlayerControllableEx(id,true);
		format(string, sizeof(string), "выкинул %s из транспорта", cPN(id));
		SendPlayerMe(playerid,string);
		J_RemovePlayerFromVehicle(id);
	}
	return true;
