GetSkinOfPlayer(playerid)
{
	if(PlayerInfo[playerid][pHeal] > 0)
	{
		new SkinsHeal[] =
		{
			252,45
		};
		if(PlayerInfo[playerid][pSex] == 2){ return 251;}
		else return SkinsHeal[random(sizeof(SkinsHeal))];
	}
	if(PlayerInfo[playerid][pSprunks] > 0)
	{
		//	new skininjail = 42;
		if(PlayerInfo[playerid][pSex] == 2){ return 69;}
		else return 42;
		//	return skininjail;
	}
	new skin, org = GetTeamID(playerid), rank = PlayerInfo[playerid][pRank];
	if(!org) skin = PlayerInfo[playerid][pSkin];
	else
	{
		if(TeamDuty[playerid]) skin = (PlayerInfo[playerid][pSex] == 2)?GOrgSkins[org][rank]:MOrgSkins[org][rank];
		else skin = PlayerInfo[playerid][pSkin];
		if(IsAtGang(playerid) || IsAtMafia(playerid))skin = (PlayerInfo[playerid][pSex] == 2)?GOrgSkins[org][rank]:MOrgSkins[org][rank];
	}
	return skin;
}
