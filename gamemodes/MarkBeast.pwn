				mycasino[playerid]=i;
				new lock[45];
				switch(CasinoInfo[i][casLock])
				{
				case 0: format(lock,sizeof(lock),"{9C381A}Закрыть{FFFFFF}");
				case 1: format(lock,sizeof(lock),"{1A9C38}Открыть{FFFFFF}");
				}
				format(string,sizeof(string),"%s казино\nПополнить баланс\nСнять с баланса\nПродать казино\nПродать казино игроку\n{E4633C}Статистика казино{FFFFFF}\n{897C78}Информация о казино\n{FFFFFF}Изменить стоимость входа\n{E4633C}Оплата налогов: %d",lock,CasinoInfo[i][casNalog]);
				ShowPlayerDialogEx(playerid,3413,2,"",string,"Выбрать","Закрыть");
			}
			else
			{
				SendClientMessage(playerid,COLOR_GREY,"Вы должны находится в здании своего казино или возле него!");
				mycasino[playerid]=-1;
			}
			return 1;
		}
	}
	SendClientMessage(playerid,COLOR_RED,"Вы не владеете ни одним из казино!");
	return true;
}
