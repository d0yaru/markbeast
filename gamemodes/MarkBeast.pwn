			{
				ApplyAnimation(playerid, "UZI", "UZI_FIRE_POOR", 4.0, 0, 0, 0, 1, 1);
			}
		case 2:
			{
				ApplyAnimation(playerid, "WEAPONS", "SHP_1H_LIFT_END", 4.0, 0, 0, 0, 1, 1);
			}
		case 3:
			{
				ApplyAnimation(playerid, "UZI", "UZI_RELOAD", 4.0, 0, 0, 0, 1, 1);
			}
		}
		fase = fase +1;
