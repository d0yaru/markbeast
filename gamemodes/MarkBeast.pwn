CMD:giveweapon(playerid,params[])
{
	new string[300];
	if(PlayerInfo[playerid][pJailTime] > 0) return SendClientMessage(playerid, COLOR_GREY, "На время пребывания в тюрьме, твоё оружие было изъято");
	if(sscanf(params,"ud",params[0],params[1])) return SendClientMessage(playerid, COLOR_PROJECT, "Используйте: {FFFFFF}/giveweapon [id] [Кол-во патронов]");
	new weapon = GetPlayerWeapon(playerid);
	new slot = GetWeaponSlot(weapon);
	
	if(PlayerInfo[playerid][pAmmos][slot] < params[1] || GetPlayerAmmo(playerid) < params[1]) return SendClientMessage(playerid, COLOR_GREY, "У вас нет столько патронов");
	if(weapon < 1) return SendClientMessage(playerid, COLOR_GREY, "У вас в руках нет оружия");
	if(PlayerInfo[playerid][pGuns][GetWeaponSlot(weapon)] != weapon)return false;
	if(!IsAFireGun(weapon)) return SendClientMessage(playerid, COLOR_GREY, "Передавать можно только огнестрельное оружие!");
	if(IsPlayerConnected(params[0]))
	{
		if(playerid == params[0]) return SendClientMessage(playerid, COLOR_GREEN, "Вы не можете передать оружие сами себе!");
		if(!ProxDetectorS(3.0,playerid,params[0])) return SendClientMessage(playerid,COLOR_GREY,"Вы далеко от игрока");
		SetPlayerChatBubble(playerid, "Передал огнестрельное оружие", COLOR_PURPLE, 30.0, 5000);
		_GiveGun(params[0],weapon,params[1]);
		_GiveGun(playerid,weapon,-params[1]);
		format(string,sizeof(string),"{FFFFFF}Вы успешно передали оружие игроку %s.",PN(params[0]));
		SendClientMessage(playerid,COLOR_PROJECT,string);
		format(string,sizeof(string),"{FFFFFF}Игрок %s[%d] Передал вам '%s' в котором %d патронов",PN(playerid),playerid,GunNames[weapon],params[1]);
		SendClientMessage(params[0],COLOR_PROJECT,string);
	}
	return true;
}
