	new result[150],is;
	if(!IsPlayerInRangeOfPoint(playerid, 2.0, 1214.2332,1943.7872,3006.7874) ) return SendClientMessage(playerid,COLOR_GREY,"Вы должны находиться у регистратуры LSPD!");

