CMD:tradecar(playerid, params[])
{
	new id,dop,string[400];
	new carid = GetPlayerVehicleID(playerid);
	if(!IsABoat(carid))
	{
		if(sellcarto[playerid]>CurrentTimer)return SendClientMessage(playerid,-1,"Делать предложение о покупке/обмене можно раз в минуту!");
		if(sscanf(params,"ud",id,dop)) return SendClientMessage(playerid,COLOR_PROJECT,"Используйте: {FFFFFF}/tradecar [id] [Доплата (если без доплаты то ставьте 0)]");

		if(dop>12000000)return SendClientMessage(playerid,-1,"Доплата не может привышать 12 000 000$");
		if(!IsPlayerRegCar(playerid, GetPlayerVehicleID(playerid))) return SendClientMessage(playerid,COLOR_GREY, "[Мысли] У меня нет ключей от этого транспорта");
		if(!IsPlayerRegCar(id, GetPlayerVehicleID(id))) return SendClientMessage(playerid,COLOR_RED,"Игрок с которым вы хотите обменятся авто, не в своем транспорте.");
		if(!IsPlayerConnected(id)) return SendClientMessage(playerid,COLOR_RED,"Неверный id игрока!");
		if(id == playerid) return false;
		if(dop<0 || dop > 100000000)return 0;
		new pcaridd = GetPlayerVehicleID(id);
		if(!ProxDetectorS(10.0, playerid, id)) return SendClientMessage(playerid,COLOR_GREY,"Игрок должен находится рядом с вами.");

		format(string,sizeof(string),"{FFFFFF}Вы предлагаете %s обменятся транспортом\n\n_____________________\nВаша модель: %s\nНомер: %d\n\n{C16060}Модель %s: %s\nНомер: %d\nДоплата: %d\n\n{FFFFFF}_____________________",Name(id),VehicleName[GetVehicleModelEx(carid)-400],carid,Name(id),VehicleName[GetVehicleModelEx(pcaridd)-400],pcaridd,dop);
		ShowPlayerDialogEx(playerid,INVALID_DIALOG_ID,0,"Обмен транспортом",string,"Ок","");
		format(string,sizeof(string),"{FFFFFF}%s предлагает вам обменятся транспортом\n\n_____________________\nВаша модель: %s\nНомер: %d\n\n{C16060}Модель %s: %s\nНомер: %d\nДоплата: %d\n\n{FFFFFF}_____________________",
		Name(playerid),VehicleName[GetVehicleModelEx(GetPlayerVehicleID(id))-400],GetPlayerVehicleID(id),Name(playerid),VehicleName[GetVehicleModelEx(GetPlayerVehicleID(playerid))-400],GetPlayerVehicleID(playerid),dop);
		ShowPlayerDialogEx(id,2999,0,"Обмен транспортом",string,"Принять","Отменить");
		caoffer[id]=playerid;
		caoffer[playerid]=id;
		cardop[playerid] = dop;
		cartrade[playerid]= GetPlayerVehicleID(playerid);
		cartrade[id] = GetPlayerVehicleID(id);
		sellcarto[playerid]=CurrentTimer+60;
	}
	return true;
}
