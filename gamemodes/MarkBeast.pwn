		pTemp[params[0]][Anti_Pvar][0] = 1;
	}
	AsendClientMessageToAll(COLOR_ADMINRED, string);
	PlayerPlaySound(params[0], 1058, 0.0, 0.0, 0.0);
	_ResetGun(params[0]);
	PlayerInfo[params[0]][pView] = time*60;
	PlayerInfo[params[0]][pWanted] = 0;
	J_SetPlayerHealth(params[0],100);
	PlayerSpawn(params[0]);
	Fix_SetPlayerVirtualWorld(params[0],1229);


