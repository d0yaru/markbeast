Float:fmax(Float:v1, Float:v2)
{
	return v1 > v2?v1:v2;
}
Float:fmin(Float:v1, Float:v2)
{
	return v1 < v2?v1:v2;
}
Float:jfloatsub(Float:value1, Float:value2)
{
	new Float:num1 = fmax(value1, value2);
	new Float:num2 = fmin(value1, value2);
	new Float:diff = (num1 - num2);
	if(diff > 300.0) diff = ( 360.0 - num1 ) + num2;
	return diff;
}
