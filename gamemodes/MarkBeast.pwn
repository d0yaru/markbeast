CMD:eject(playerid, params[])
{
	new id, string[100];
	if(sscanf(params,"u",id)) return SendClientMessage(playerid,COLOR_PROJECT,"Используйте: {FFFFFF}/eject [ID/Name]");
	if(playerid == id) return SendClientMessage(playerid, COLOR_GREY, "Нельзя выкинуть самого себя.");
	if(GetPlayerState(playerid) != 2) return SendClientMessage(playerid, COLOR_GREY, "Только водитель может сделать это");
