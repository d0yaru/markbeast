void checkGM(playerid)
{
	if(!IsPlayerConnected(WhoTesting[playerid])) return  SendClientMessage(playerid, COLOR_GREY, "Он не в сети");
	new Float: Health,string[46+MAX_PLAYER_NAME];
	if(PlayerAFKTime[WhoTesting[playerid]][0]!=0)return SendClientMessage(playerid, COLOR_GREY, "Он в AFK");
	GetPlayerHealth(WhoTesting[playerid],Health);
	if(PlayerInfo[WhoTesting[playerid]][pHealth]!= Health)
	{
		format(string,sizeof(string),"[A] %s[%d] Использует GM!!!",PN(WhoTesting[playerid]),WhoTesting[playerid]);
		SendAdminsMessage(COLOR_RED,string);
	}
	else
	{
		SendClientMessage(playerid, COLOR_RED,"Первый тест на гм пройден, стартуем второй");
		if(IsPlayerInAnyVehicle(WhoTesting[playerid]))
		{
			SendClientMessage(playerid, COLOR_GREY, "Он в машине,выкинул его от туда...");
			J_RemovePlayerFromVehicle(WhoTesting[playerid]);
		}
		J_SetPlayerHealth(WhoTesting[playerid],Health+5);
		OldHealth[WhoTesting[playerid]] = Health+5;
		new Float:boomx, Float:boomy, Float:boomz;
		GetPlayerPos(WhoTesting[playerid],boomx, boomy, boomz);
		CreateExplosion(boomx, boomy+3 , boomz+8, 5, 0);
		SetPlayerTimer(playerid,"checkGMTwo",1000,false);
	}
	return true;
}
void checkGMTwo(i)
{
	new string[300];
	new Float:nowhealth;
	new Float:armour;
	GetPlayerArmour(WhoTesting[i], armour);
	if(IsPlayerInAnyVehicle(WhoTesting[i]))GetVehicleHealth(GetPlayerVehicleID(WhoTesting[i]),nowhealth);
	else GetPlayerHealth(WhoTesting[i],nowhealth);
	if(armour > 0)
	{
		SendClientMessage(i, COLOR_GREY, "У него бронька");
		return true;
	}
	if((OldHealth[WhoTesting[i]]-nowhealth) > 0)
	{
		format(string, sizeof(string), "До: \t\t\t\t%.0f.0 HP\nПосле: \t\t\t%.0f.0 HP\nHP уменьшилось на: \t\t%.0f.0\n\n{7FB151}ГМ не обнаружено. HP игрока востановлено.",OldHealth[WhoTesting[i]],nowhealth,(OldHealth[WhoTesting[i]]-nowhealth));
		ShowPlayerDialogEx(i,0,DIALOG_STYLE_MSGBOX, "Проверка на God Mode",string, "Закрыть", "");
		J_SetPlayerHealth(WhoTesting[i], OldHealth[WhoTesting[i]]);
		WhoTesting[i] = INVALID_PLAYER_ID;
	}
	else if((OldHealth[WhoTesting[i]]-nowhealth) == 0)
	{
		format(string, sizeof(string), "До: \t\t\t%.0f.0 HP\nПосле: \t\t%.0f.0 HP\nHP не уменьшилось\n\n{CD5A5A}Возможно этот игрок использует ГМ.\n Но если он AFK или у него броня, то такой результат нормален",OldHealth[WhoTesting[i]],nowhealth);
		ShowPlayerDialogEx(i,0,DIALOG_STYLE_MSGBOX, "Проверка на God Mode",string, "Закрыть", "");
		WhoTesting[i] = INVALID_PLAYER_ID;
	}
	return true;
}
