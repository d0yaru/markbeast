CheckPassword(pass[])
{
	for(new i; i < strlen(pass); i ++)
	{
		if( (pass[i] >= 'a' && pass[i] <= 'z') ||
				(pass[i] >= 'A' && pass[i] <= 'Z') ||
				(pass[i] >= 'а' && pass[i] <= 'я') ||
				(pass[i] >= 'А' && pass[i] <= 'Я') ||
				(pass[i] >= '0' && pass[i] <= '9')  ) continue;
		else return false;
	}
	return true;
}
