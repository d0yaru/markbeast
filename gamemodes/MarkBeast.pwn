	if(PlayerInfo[playerid][pAdmin] < 3) return false;
	if(ADuty[playerid]!=228) return cmd_apanel(playerid,"");
	new
	result[150],
	string[256],
	Admin[30];

	//if(PlayerInfo[playerid][pAdmin]<6) if(GLS[35]>=5)return SendClientMessage(playerid,COLOR_RED,"В общий чат можно писать не более 5 строк в 10 минут.!");
	if(PlayerInfo[playerid][pAdmin] == ADM_ADMIN) Admin = "Старший Модератор";
	else if(PlayerInfo[playerid][pAdmin] == ADM_SUPER_ADMIN) Admin = "Администратор";
	else if(PlayerInfo[playerid][pAdmin] == ADM_GRAND_ADMIN) Admin = "Главный Администратор";
	else if(PlayerInfo[playerid][pAdmin] == ADM_GRAND) Admin = "Основатель";
	if(sscanf(params,"s[150]",result)) return SendClientMessage(playerid,COLOR_PROJECT,"Используйте: {FFFFFF}/ao [текст]");
	if(IsADomen(result)  && PlayerInfo[playerid][pAdmin]<6)
	{
		format(string,sizeof(string),"%s %d подозревается в попытке слива! - - - %s",PN(playerid),playerid, result);
		SendAdminsMessage(COLOR_RED,string);
		SendClientMessage(playerid,COLOR_RED,"В вашем сообщении обнаружено запрещенное содержание.");

		return false;
	}
	if(IsAIP(result))return 1;
	format(string,sizeof(string),"%s %s:{FFFFFF} %s",Admin,Name(playerid),result);
	SendClientMessageToAll(COLOR_PROJECT,string);
	GLS[35]++;
	return true;
}
