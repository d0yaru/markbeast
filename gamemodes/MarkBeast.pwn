	if(IsADomen(reason)  && PlayerInfo[playerid][pAdmin]<6)
	{
		format(string,sizeof(string),"%s %d подозревается в попытке слива! - - - %s",PN(playerid),playerid, reason);
		SendAdminsMessage(COLOR_RED,string);
		SendClientMessage(playerid,COLOR_RED,"В вашем сообщении обнаружено запрещенное содержание.");

		return false;
	}
	if(days>30 || 1>days) return SendClientMessage(playerid, COLOR_PROJECT, "Используйте:{FFFFFF} /sban [id] [days 1-30] [Причина]");
