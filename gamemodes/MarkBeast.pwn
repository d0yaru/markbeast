	if(IsADomen(reason) || IsAMg(reason))
	{
		SendClientMessage(playerid,COLOR_RED,"В вашем сообщении обнаружено запрещенное содержание.");

		return false;
	}
	if(IsAIP(reason))return 1;
	if(PlayerInfo[playerid][pFamRank]<=PlayerInfo[id][pFamRank])return SendClientMessage(playerid,COLOR_RED,"Только основатель может изгнать заместителя из семьи!");
