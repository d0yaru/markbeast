CMD:ohbcreate(playerid, params[])
{
	if(PlayerInfo[playerid][pAdmin]<6)return false;
	if(ADuty[playerid]!=228) return cmd_apanel(playerid,"");
	new Float: x, Float: y, Float: z, Float: a, rate, string[64], casinoid;
	if(sscanf(params, "ii", casinoid, rate)) return SendClientMessage(playerid, 0xFFFFFFFF, "Использование: /ohbcreate [casinoid] [rate]");

	if(EnterCasino[playerid] != casinoid)  return SendClientMessage(playerid, 0xFFFFFFFF, "Ты не находишься в этом казино!");
	GetPlayerPos(playerid, x, y, z);
	GetPlayerFacingAngle(playerid, a);

	for (new i = 0; i < MAX_BANDITS; i ++)
	{
		if (OHB_Info[i][bnCreated] == 0)
		{
			format(string, sizeof(string), "bandits/%d.ini", i);
			new File = ini_createFile (string);
			if(File >= 0)
			{
				ini_setInteger(File, "casinoid", casinoid);
				ini_setInteger(File, "rate", rate);
				ini_setFloat(File, "x", x);
				ini_setFloat(File, "y", y);
				ini_setFloat(File, "z", z);
				ini_setFloat(File, "a", a);
				ini_closeFile(File);

				OHB_Create(i, rate, casinoid, x, y, z-0.20, a);
			}
			return 1;
		}
	}
	return 1;
}
