CMD:wfncreate(playerid, params[])
{
	if(PlayerInfo[playerid][pAdmin]<6) return false;
	new Float: x, Float: y, Float: z, Float: r, casinoid, maxrate, minrate, string[64];
	if(sscanf(params, "iii", casinoid, minrate, maxrate)) return SendClientMessage(playerid, -1, "Использование: /wfncreate [casinoid] [min-rate] [max-rate]");

	GetPlayerPos(playerid, x, y, z);
	GetPlayerFacingAngle(playerid, r);

	for (new i = 0; i < MAX_WHEELS; i ++)
	{
		if (WFN_Info[i][whCreated] == 0)
		{
			format(string, sizeof(string), "fwheel/%d.ini", i);
			new File = ini_createFile (string);
			if(File >= 0)
			{
				ini_setFloat(File, "x", x);
				ini_setFloat(File, "y", y);
				ini_setFloat(File, "z", z);
				ini_setFloat(File, "a", r);
				ini_setInteger(File, "casinoid", casinoid);
				ini_setInteger(File, "maxrate", maxrate);
				ini_setInteger(File, "minrate", minrate);
				ini_closeFile(File);

				WFN_Create(casinoid, x, y, z, r, maxrate, minrate);
			}
			return 1;
		}
	}
	return 1;
}
