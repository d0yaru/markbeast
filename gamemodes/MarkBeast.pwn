void GrandTimer()
{
	if(GrandEndOK>0)
	{
		new grandstr[256]; format(grandstr, 256, "Потерялся конец GrandTimer. Этап: %d", GrandEndOK);
		WriteLog("GrandLost.txt", grandstr);
	}
	GrandEndOK=1;

	GlobalTimerCount[0]++;
	if(GlobalTimerCount[0] >= 60*5)
	{
		SetWeatherForPlayers(); // смена погода
		GlobalTimerCount[0] = 0;
	}

	CurrentTimer = gettime();

	Reklam ++;
	new year,month,day,minuite,second,hour;
	getdate(year,month,day);
	gettime(hour,minuite,second);
	if(minuite == 0 && second >= 11 && second <= 16) PAYDAY = 0;
	if(minuite == 0 && second >= 0 && second <= 10 && PAYDAY == 0)
	{
		PAYDAY = 1;
		SetWorldTimeEx(hour);
		PayDay(hour);
	}
	FortuneRing();
	/*	if(oldmin != minuite)//
	{
		CheckOnline(hour);
		oldmin = minuite;
	}*/
	if(hour == 5)
	{
		if(minuite == 0)
		{
			if(!Warned) // 1 раз всего, не важно поэтому когда
			{
				Warned = true;
				foreach (i)
				{
					ShowPlayerDialogEx(i,0,DIALOG_STYLE_MSGBOX,"{FF0000}Рестарт","{FFFFFF}Внимание через 5 минут будет {FF0000}рестарт {FFFFFF}сервера\n\nВы будете кикнуты сервером\nЭто {FF0000}обезопасит {FFFFFF}ваш аккаунт\nСейчас происходит сохранение всего","Закрыть","");
				}
				//	SendRconCommand("password 656616");
				SendRconCommand("hostname Mark Beast | Перезагрузка сервера");
				printf("[Перезагрузка] Закрыли и переиминовали сервер");
				printf("[Перезагрузка] Отключили команды");
			}
		}
		else if(minuite == 5 && second >= 0 && second <= 10) // 5 минута
		{
			if(!Restarted)
			{
				Restarted = true;
				SendClientMessageToAll(COLOR_RED,"Рестарт!");
				foreach (i)
				{
					Kick(i);
				}
				FinishMaintenance();
				printf("Уничтожили все динамические штуки и вызвали GMX");
			}
		}
	}
	GrandEndOK=2;
	if(Reklam >= 1200)
	{
		Reklam = 0;
		foreach(i)
		{
			if(IsPlayerLogged[i])
			{
				pTemp[i][Anti_Pvar][12] = 0;
			}
		}
		SendClientMessageToAll(COLOR_TUPAC, "Всю информацию об игре, вы можете узнать на нашем сайте » "Mode_Site#);
		GLS[36]=0;
		GLS[35]=0;
	}

	for(new i = 0;i < MAX_VEHICLES; i ++)
	{
		if(i < MAX_BOMBS) BombTimer(i);

		if(i > 0)
		{
			if(IsVehicleConnected(i))
			{
				CheckPassengers(i);
				CheckGas(i);
				CheckCarTime(i);


				if(BotVehicles[0] != i && BotVehicles[1] != i)
				{
					if(VehicleInfo[i][VehicleRespawn]<VehicleInfo[i][VehicleRespawnDelay])
					{
						VehicleInfo[i][VehicleRespawn]--;
						if(!VehicleInfo[i][VehicleRespawn])
						{
							SetVehicleToRespawn(i);
							VehicleInfo[i][VehicleRespawn]=VehicleInfo[i][VehicleRespawnDelay];
						}
					}
				}
			}
		}
		if(i < MAX_ORGS)
