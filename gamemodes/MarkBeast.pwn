	new inveteid = Invited_To_Dice[playerid];
	if(inveteid == INVALID_PLAYER_ID) return SendClientMessage(playerid, COLOR_GREY, "Вам никто не предлагал сыграть в кости.");

	new dice = random(5)+2;
	new dice1 = random(5)+2;

	if(!ProxDetectorS(6.0, playerid, inveteid)) return SendClientMessage(playerid, COLOR_GREY, "Вы слишком далеко от игрового стола!");

	new fiski;
	fiski=PlayerInfo[playerid][pMoney];
	new fiski1;
	fiski1=PlayerInfo[inveteid][pMoney];
	if(fiski < Invite_To_Dice_Money[inveteid])
