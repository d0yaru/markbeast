		GameTextForPlayer(playerid, CMDstr, 3000, 1);

		Invite_To_Dice_Money[inveteid] = Invite_To_Dice_Money[inveteid] - procent;

		GiveMoney(inveteid,Invite_To_Dice_Money[inveteid],"casino");

		SendClientMessage(inveteid, -1,"Поздравляем! Вы выиграли! 3 процента от выигрыша уходит в казино.");
		SendClientMessage(playerid, -1,"К сожалению, Вы проиграли!");
		format(CMDstr, 20, "~g~+%d$", Invite_To_Dice_Money[inveteid]);
		GameTextForPlayer(inveteid, CMDstr, 3000, 1);

	}
	else if(dice < dice1)
	{
		new procent = (Invite_To_Dice_Money[inveteid] / 100) * 3;

		CasinoInfo[EnterCasino[playerid]][casBalance] = CasinoInfo[EnterCasino[playerid]][casBalance] + procent;
		CasinoInfo[EnterCasino[playerid]][casWin] += procent;

		GiveMoney(inveteid,-Invite_To_Dice_Money[inveteid],"casino");

		format(CMDstr, 20, "~r~-%d$", Invite_To_Dice_Money[inveteid]);
		GameTextForPlayer(inveteid, CMDstr, 3000, 1);

		Invite_To_Dice_Money[inveteid] = Invite_To_Dice_Money[inveteid] - procent;


		GiveMoney(playerid,Invite_To_Dice_Money[inveteid],"casino");


		SendClientMessage(playerid, -1,"Поздравляем! Вы выиграли! 3 процента от выигрыша уходит в казино.");
