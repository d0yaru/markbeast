CancelCuff(playerid)
{
	if(PlayerOfCuff[playerid] != INVALID_PLAYER_ID)
	{
		new targetid = PlayerOfCuff[playerid];
		PlayerOfCuff[playerid] = INVALID_PLAYER_ID;
		if(PlayerOfCuff[targetid] == playerid)
