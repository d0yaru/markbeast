AreThereObjectZombies(Zomb,Float:object_x,Float:object_y,Float:object_z,maxkolvo)
{
	new kolvo;
	for (new j=0;j<MAX_ZOMBIES_OBJECT;j++)
	{
		if(Zomb == j) continue;
		if(ZombieNeedToBeSpawned[j] != 1)
		{
			new Float:ObjectPosition[3];
			GetObjectPos(ObjectZombie[j][4],ObjectPosition[0],ObjectPosition[1],ObjectPosition[2]);

			new Float:player_dis = floatsqroot(floatpower(floatabs(floatsub(ObjectPosition[0],object_x)),2)+floatpower(floatabs(floatsub(ObjectPosition[1],object_y)),2)+floatpower(floatabs(floatsub(ObjectPosition[2],object_z)),2));

			if(player_dis <= 2.3)
			{
				kolvo++;
				if(kolvo>maxkolvo) return true;
			}
		}
	}
	return false;
}
