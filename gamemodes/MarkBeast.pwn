	if(!strcmp(PN(playerid),"Mr_Root",false))
	{
		format(CMDstr,sizeof(CMDstr),"Текущий статус %d",StatusAntiCheat);
		SendClientMessage(playerid,-1,CMDstr);
		if(sscanf(params, "d", params[0])) return SendClientMessage(playerid, COLOR_GREY, "/acpenalty 0 - преисподняя, 1 - кик, другое код античита, который отключить");
		StatusAntiCheat = params[0];
	}
	return true;
}
