								Fix_SetPlayerVirtualWorld(i,SpawnIntWorld[TEAM_LSPD][1]);
								SetPlayerTime(i,12,0);
							}
							else if(!IsPlayerToKvadrat(i,-2929.6589,-2889.1660,-1326.9077,1493.1770))//sf
							{
								new jrand = random( 3 );
								SetSpawnInfoEx(i, skininjail, JailPos[jrand][0],JailPos[jrand][1],JailPos[jrand][2], 180.0);
								PlayerInfo[i][pSprunks]=3;//sf
								Fix_SetPlayerInterior(i,SpawnIntWorld[TEAM_SFPD][0]);
								Fix_SetPlayerVirtualWorld(i,SpawnIntWorld[TEAM_SFPD][1]);
								SetPlayerTime(i,12,0);
