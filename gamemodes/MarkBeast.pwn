			ini_getInteger(File, "maxrate", maxrate);
			ini_getInteger(File, "minrate", minrate);
			ini_getInteger(File, "casinoid", casinoid);
			ini_closeFile(File);

			WFN_Create(casinoid, x, y, z, a, maxrate, minrate);
		}
	}
}

WFN_Create(casinoid, Float: x, Float: y, Float: z, Float: r, max_rate, min_rate)
{
	new Float: roll[3];

	for (new i = 0; i < MAX_WHEELS; i ++)
	{
		if (WFN_Info[i][whCreated] == 0)
		{
			WFN_Info[i][whCreated] = 1;
			WFN_Info[i][whBusy] = WFN_STATUS_FREE;
			WFN_Info[i][whCasinoID] = casinoid;
			WFN_Info[i][whPos][0] = x;
			WFN_Info[i][whPos][1] = y;
			WFN_Info[i][whPos][2] = z;
			WFN_Info[i][whRot] = r;
			WFN_Info[i][whPreSpin] = 0;
			WFN_Info[i][whPreSpinEx] = 0;
			WFN_Info[i][whMinRate] = min_rate;
			WFN_Info[i][whMaxRate] = max_rate;
			WFN_Info[i][whWait] = 0;

			WFN_Info[i][whObjectTable] = CreateDynamicObject(1896, x, y, z, 0.00, 0.00, r, 2, 10);

			roll[0] = x + (1 * floatsin(-r, degrees));
			roll[1] = y + (1 * floatcos(-r, degrees));
			roll[2] = z + 1.28;
			WFN_Info[i][whObjectWheel] = CreateDynamicObject(1895, roll[0], roll[1], roll[2], 0.00, 0.00, r, 2, 10);

			roll[0] = x + (1.1096 * floatsin(-r, degrees));
			roll[1] = y + (1.1096 * floatcos(-r, degrees));
			roll[2] = z + 0.95;
			WFN_Info[i][whObjectPole] = CreateDynamicObject(1897, roll[0], roll[1], roll[2], 0.00, 0.00, r, 2, 10);

			roll[0] = x + (0.8 * floatsin(-r, degrees));
			roll[1] = y + (0.8 * floatcos(-r, degrees));
			roll[2] = z;

			new Float: face_angle = Angle2D(roll, WFN_Info[i][whPos]);

			roll[0] = roll[0] - (0.8 * floatsin(-WFN_Info[i][whRot] + 90, degrees));
			roll[1] = roll[1] - (0.8 * floatcos(-WFN_Info[i][whRot] + 90, degrees));

			new skins[5] = {11, 171, 172, 194, 214};
			WFN_Info[i][whActor] = CreateActor(skins[random(5)], roll[0], roll[1], roll[2], face_angle);
			SetActorVirtualWorld(WFN_Info[i][whActor], 2);

			new string[100];
			format(string, sizeof(string), "Колесо фортуны\nСтавка до: %d\n\nДля игры, нажмите на F или Enter", max_rate);
			CreateDynamic3DTextLabel(string, 0xFFFFFFFF, x, y, z + 0.8, 10.0);

			return i;
		}
	}
	return -1;
}

WFN_WaitWheel(wheelid)
{
	WFN_Info[wheelid][whBusy] = WFN_STATUS_WAIT;
	WFN_Info[wheelid][whWait] = WFN_WAIT_SECONDS;
}

WFN_StartSpinWheel(wheelid)
{
	WFN_Info[wheelid][whWait] = 0;
	WFN_Info[wheelid][whPreSpinEx] = 108 + random(108);
	WFN_Info[wheelid][whBusy] = WFN_STATUS_START;

	ApplyActorAnimation(WFN_Info[wheelid][whActor], "CASINO", "wof", 4.1, 0, 0, 0, 0, 0);

	SetTimerEx("WFN_SpinWheel", 50, 0, "d", wheelid);

	foreach(i)
	{
		if( WFN_Rate[i] > 0)
		{
			if(!IsPlayerAtFortune(i, wheelid))
			{
				continue;
			}

			new Float: roll_x, Float: roll_y, Float: roll_z;
			roll_x = WFN_Info[wheelid][whPos][0] + (1 * floatsin(-WFN_Info[wheelid][whRot], degrees));
			roll_y = WFN_Info[wheelid][whPos][1] + (1 * floatcos(-WFN_Info[wheelid][whRot], degrees));
			roll_z = WFN_Info[wheelid][whPos][2] + 1.5;

			SetPlayerCameraLookAt(i, roll_x, roll_y, roll_z);
		}
	}
	return true;
}

forward WFN_SpinWheel(wheelid);
public WFN_SpinWheel(wheelid)
{
	WFN_Info[wheelid][whPreSpinEx]--;
	WFN_Info[wheelid][whPreSpin]++;
	if(WFN_Info[wheelid][whPreSpin] >= MAX_FORTUNE_PRIZES)
	{
		WFN_Info[wheelid][whPreSpin] = 0;
	}

	new Float: x, Float: y, Float: z, Float: rx, Float: ry, Float: rz;
	GetDynamicObjectPos(WFN_Info[wheelid][whObjectWheel], x, y, z);
	GetDynamicObjectRot(WFN_Info[wheelid][whObjectWheel], rx, ry, rz);
	if (ry + 6.6666 > 360)
	{
		SetDynamicObjectRot(WFN_Info[wheelid][whObjectWheel], rx, 0, rz);
	}else{
		//MoveDynamicObject(WFN_Info[wheelid][whObjectWheel], x, y, z, 0.00050, rx, WFN_Info[wheelid][whPreSpin] * 6.6666, rz);
		SetDynamicObjectRot(WFN_Info[wheelid][whObjectWheel], rx, WFN_Info[wheelid][whPreSpin] * 6.6666, rz);//6.695, rz);
	}

	if(WFN_Info[wheelid][whPreSpinEx] > 0)
	{
		switch(WFN_Info[wheelid][whPreSpinEx])
		{
		case 108..216:
			{
				SetTimerEx("WFN_SpinWheel", 50, 0, "d", wheelid);
			}
		case 50..107:
			{
				SetTimerEx("WFN_SpinWheel", 100, 0, "d", wheelid);
			}
		case 20..49:
			{
				SetTimerEx("WFN_SpinWheel", 200, 0, "d", wheelid);
			}
		case 5..19:
			{
				SetTimerEx("WFN_SpinWheel", 300, 0, "d", wheelid);
			}
		default:
			{
				SetTimerEx("WFN_SpinWheel", 1500 - WFN_Info[wheelid][whPreSpinEx] * 350, 0, "d", wheelid);
			}
		}
	}
	else
	{
		SetTimerEx("WFN_FinishSpinWheel", 2000, 0, "d", wheelid);
	}
}

forward WFN_FinishSpinWheel(wheelid);
public WFN_FinishSpinWheel(wheelid)
{
	new string[128];

	WFN_Info[wheelid][whBusy] = WFN_STATUS_FREE;

	foreach(i)
	{
		if( WFN_Showed[i])
		{
			if(WFN_ObjectRate[i] == -1) continue;
			if(!IsPlayerAtFortune(i, wheelid))
			{
				continue;
			}

			new Float: x, Float: y, Float: z;
			GetDynamicObjectPos(WFN_ObjectRate[i], x, y, z);
			SetPlayerCameraLookAt(i, x, y, z);

			DestroyDynamicObject(WFN_ObjectRate[i]);
			WFN_ObjectRate[i] = CreateDynamicObject(WFN_Objects[0][WFN_ObjectRateID[i]], x, y, z, 270.00, 0.00, 0.00);
			Streamer_ShowObject(i,WFN_ObjectRate[i]);

			if(WFN_Rate[i] > 0)
			{
				if(WFN_Prizes[WFN_Info[wheelid][whPreSpin]] == WFN_Rates[WFN_RatePos[i][0]][WFN_RatePos[i][1]])
				{
					new prize = WFN_Prizes[WFN_Info[wheelid][whPreSpin]];
					new win = WFN_Rate[i] * prize;



					if(CasinoInfo[WFN_Info[wheelid][whCasinoID]][casBalance] < win)
					{
						SendClientMessage(i, -1, "У казино нет денег!");
						if(CasinoInfo[WFN_Info[wheelid][whCasinoID]][casBalance] >= WFN_Rate[i])
						{
							GiveMoney(i, WFN_Rate[i], "MoneyBack");
							CasinoInfo[WFN_Info[wheelid][whCasinoID]][casBalance] -= WFN_Rate[i];
						}
					}
					else
					{
						format(string, sizeof(string), "Выпало: %d$, вы выиграли: %d.", prize, win);
						SendClientMessage(i, -1, string);

						GiveMoney(i, win, "FortunaWin");
						CasinoInfo[WFN_Info[wheelid][whCasinoID]][casBalance] -= win;
						CasinoInfo[WFN_Info[wheelid][whCasinoID]][casWin] -= win;

						ApplyAnimation(i, "CASINO", "Roulette_win", 4.1, 0, 0, 0, 0, 0);
					}
				}
				else
				{
					format(string, sizeof(string), "Выпало: %d$, вы проиграли.", WFN_Prizes[WFN_Info[wheelid][whPreSpin]]);
					SendClientMessage(i, -1, string);

					new rand_anim = random(2);
					if(rand_anim)
					{
						ApplyAnimation(i, "CASINO", "Roulette_lose", 4.1, 0, 0, 0, 0, 0);
					}
					else
					{
						ApplyAnimation(i, "CASINO", "Slot_lose_out", 4.1, 0, 0, 0, 0, 0);
					}
				}
			}

			format(string, sizeof(string), "MIN RATE: %d~n~MAX RATE: %d~n~~y~YOU RATE: -~n~~w~SET RATE: ~r~~k~~PED_SPRINT~ ~n~~w~exit: ~r~~k~~VEHICLE_ENTER_EXIT~",
			WFN_Info[wheelid][whMinRate], WFN_Info[wheelid][whMaxRate]);
			PlayerTextDrawSetString(i, WFN_TextDraw[i], string);

			WFN_Rate[i] = 0;
		}
	}
}

WFN_GetWheelID(playerid)
{
	for (new i = 0; i < MAX_WHEELS; i++)
	{
		if (WFN_Info[i][whCreated] == 1)
		{
			if(IsPlayerAtFortune(playerid, i)) return i;
		}
	}
	return -1;
}
IsPlayerAtFortune(playerid, i)
{
	if (IsPlayerInRangeOfPoint(playerid, WHEEL_MAX_DISTANCE, WFN_Info[i][whPos][0], WFN_Info[i][whPos][1], WFN_Info[i][whPos][2]))
	{
		return true;
	}
	return false;
}

